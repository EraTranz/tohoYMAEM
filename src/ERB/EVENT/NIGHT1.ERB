;========================================================
;夜イベント各本体イベント部
;=========================================================
;-----------------------------------------------
;眠っている主人に色々とする
;-----------------------------------------------
@NIGHT_EVENT_1, ARG, ARG:1, ARG:2, ARG:3
;地の文カット
SIF CFLAG:99 == 0
	PRINTFORMW 丑三つ時……%CALLNAME:TARGET%が既に眠ってしまった%CALLNAME:MASTER%の元に訪れた
PRINTL 
;確率1/3で夜這い失敗
IF RAND:3 == 0
	;幼稚キャラの添い寝失敗の場合
	IF ARG:2 == 2
		;地の文カット
		IF CFLAG:99 == 0
			PRINTFORML %CALLNAME:MASTER%と一緒の布団の中で目一杯甘えようと思っていた%CALLNAME:TARGET%の目に、涙が滲む
			PRINTFORML 部屋に帰ると枕を掻き抱き、%CALLNAME:MASTER%がいないベッドの中で寂しさに小さく震え続けた
		ENDIF
		TRYCALL SELF_KOJO, TARGET, 403
		;貞操帯装着中は排泄行為不可
		IF CFLAG:42 & 2
			PRINTW 　
			;地の文カット
			SIF CFLAG:99 == 0
				PRINTFORML その夜、%CALLNAME:TARGET%は怖い夢を見てしまったようだ……
			;17恐怖の珠
			;ARG:3の使用がおかしい気がするが、なに、気にすることはない
			CALL COMMON_UP_JUEL, TARGET, 17, 300+ARG:3*RAND:701, 1
		ELSE
			IF EXP:放尿経験 >= 1
				;地の文カット
				SIF CFLAG:99 == 0
					PRINTFORML その夜、怖い夢を見てしまった%CALLNAME:TARGET%はベッドの中で粗相をしてしまったようだ……
				LOCAL = 300+ARG:3*RAND:701
				;06放尿経験
				CALL COMMON_UP_EXP, TARGET, 6, 1, 0
				;15恥情の珠
				CALL COMMON_UP_JUEL, TARGET, 15, LOCAL, 0
				;17恐怖の珠
				CALL COMMON_UP_JUEL, TARGET, 17, LOCAL, 1
			ENDIF
		ENDIF
	;夜襲撃失敗
	ELSE
		;地の文カット
		SIF CFLAG:99 == 0
			PRINTFORML 扉の向こうからはとぼとぼと立ち去る%CALLNAME:TARGET%の足跡が暫らく聞こえていた……
		;貞操帯装着中は自慰にも失敗
		IF CFLAG:42 & 2
			;地の文カット
			IF CFLAG:99 == 0
				PRINTFORML 欲情押さえられぬ%CALLNAME:TARGET%は、自慰行為をしようと股間に手を伸ばすが
				PRINTFORML 黒く光る貞操帯によりその行為は阻まれてしまった…
			ENDIF
			TRYCALL SELF_KOJO, TARGET, 160
			;13欲情の珠
			CALL COMMON_UP_JUEL, TARGET, 13, (ARG*150), 0
			;14屈服の珠
			CALL COMMON_UP_JUEL, TARGET, 14, (ARG*200), 1
		ELSE
			;地の文カット
			SIF CFLAG:99 == 0
				PRINTFORML しかし欲情押えられぬ%CALLNAME:TARGET%は、ついつい自慰行為を始めてしまったようだ
			TRYCALL SELF_KOJO, TARGET, 151
			PRINTL 
			;02絶頂経験
			CALL COMMON_UP_EXP, TARGET, 2, (ARG/2), 0
			;10自慰経験
			CALL COMMON_UP_EXP, TARGET, 10, (ARG/2), 0
			;00快Ｃの珠
			CALL COMMON_UP_JUEL, TARGET, 0, (ARG*(100*ABL:Ｃ感覚)), 0
			;12恭順の珠
			CALL COMMON_UP_JUEL, TARGET, 12, (ARG*100), 0
			;13欲情の珠
			CALL COMMON_UP_JUEL, TARGET, 13, (ARG*200), 1
			PRINTL 
			;地の文カット
			SIF CFLAG:99 == 0
				PRINTFORML {ARG/2}回の自慰の果てに満足した%CALLNAME:TARGET%は疲れきった様子でベッドへと倒れ込んだ……
		ENDIF
	ENDIF
ELSE
	;地の文カット
	SIF CFLAG:99 == 0
		PRINTFORML %CALLNAME:TARGET%は\@(90 >= GET_AGG(TARGET)) ? 恐る恐る部屋へと侵入すると # 堂々と部屋に侵入すると\@%CALLNAME:MASTER%のベッドへと潜り込んだ
	;愛情あり、三分の一で添い寝
	IF TALENT:恋慕 && RAND:3 == 0
		;地の文カット
		SIF CFLAG:99 == 0
			PRINTFORMW 愛しい%CALLNAME:MASTER%の寝顔にそっとキスをした%CALLNAME:TARGET%は\@(RAND:2 == 0) ? 胸に頬を寄せ嬉しそうに添い寝した # 腕を枕にして嬉しそうに添い寝した \@
		;添い寝イベント口上呼び出し
		TRYCALL SELF_KOJO, TARGET, 400
		PRINTL 
		;21愛情経験
		CALL COMMON_UP_EXP, TARGET, 21, ((ARG:1/2)+2), 0
		;12恭順の珠
		CALL COMMON_UP_JUEL, TARGET, 12, (ARG:1*200), 1
		PRINTL 
		;地の文カット
		SIF CFLAG:99 == 0
			PRINTFORML その夜、%CALLNAME:TARGET%は幸せな夢を見た……
	;勝手に欲望の発散をしてしまう
	ELSE
		LOCAL:1 = RAND:4
		;貞操帯装着中は勝手に欲望の発散が出来ない
		IF CFLAG:42 & 2
			;地の文カット
			SIF CFLAG:99 == 0
				PRINTFORML %CALLNAME:MASTER%の躰に発情してしまった%CALLNAME:TARGET%だが、貞操帯がその欲望を阻む。
			CALLF SET_NIGHT_KOJO(0, 5)
			;夜這い騎乗位(貞操帯防御)イベント口上呼び出し
			TRYCALL SELF_KOJO, TARGET, 401
			PRINTL 
			;13欲情の珠
			CALL COMMON_UP_JUEL, TARGET, 13, (ARG*150), 0
			;14屈服の珠
			CALL COMMON_UP_JUEL, TARGET, 14, (ARG*200), 1
			PRINTL 
			SIF CFLAG:99 == 0
				PRINTFORML %CALLNAME:MASTER%の隣で悶々とした気持ちを抱えた%CALLNAME:TARGET%は、中々眠ることができなかった
		;奴隷に棒があり、主人に穴がある場合。夜這いされる。
		;反発LV3と禁断と調合知識があったら、穴を作ってでも犯しに来る
		;反発関係は移転。よって反発3でない奴隷棒有り、主人穴のみここになる
		ELSEIF (TALENT:オトコ || TALENT:ふたなり) && TALENT:MASTER:オトコ == 0
			;地の文カット
			IF CFLAG:99 == 0
				;主人が処女の場合素股に変更。処女素股は浪漫らしいので。
				IF TALENT:MASTER:処女 >= 1
					;ふたなりなら尻に突っ込む。例え経験が無くとも。
					IF TALENT:MASTER:ふたなり
						PRINTFORML %CALLNAME:TARGET%は、寝ている%CALLNAME:MASTER%の下着を脱がせ
						PRINTFORML 秘処に挿入しようとしたが、%CALLNAME:MASTER%が処女である事に気付いて躊躇った
						PRINTL しかし既に怒張は痛いほど張っており、このまま帰るなど考えられない
						PRINTFORML %CALLNAME:TARGET%は%CALLNAME:MASTER%の秘裂を舐めあげ、愛液を滴らせると
						PRINTFORML それを%CALLNAME:MASTER%の尻穴と己の怒張に塗りたくり
						PRINTL 意を決して怒張を腸内に捻じ込んだ
						PRINTL 　
						IF EXP:MASTER:Ａ経験 == 0
							PRINTFORML 突然の衝撃に目を覚ました%CALLNAME:MASTER%は、腸内に捻じ込まれる異物に恐怖するも
							PRINTFORML 慌てた%CALLNAME:TARGET%の弁解を受け、
							PRINTFORML その最中でも腸内を貪るのを止めようとはしない%CALLNAME:TARGET%の様子に
							PRINTL ここで無理に抵抗すると、処女を散らされかねないと思い
							PRINTL 出来るだけ楽な姿勢をとって、後ろの穴での初めての経験をやり過ごす事にした。
						ELSE
							PRINTFORML 突然の衝撃に目を覚ました%CALLNAME:MASTER%はしばし呆然とするも
							PRINTFORML %CALLNAME:TARGET%との行為を受け入れ、自ら腰を振りだした･･･。
						ENDIF
					;処女素股
					ELSE
						PRINTFORML %CALLNAME:TARGET%は、寝ている%CALLNAME:MASTER%の下着を脱がせ
						PRINTFORML 秘処に挿入しようとしたが、%CALLNAME:MASTER%が処女である事に気付いて躊躇い
						PRINTL その股に怒張を擦り付けて欲望を晴らすことにした。
						PRINTL 　
						PRINTFORML 体への重みで目を覚ました%CALLNAME:MASTER%は、股に怒張を擦り付ける%CALLNAME:TARGET%に呆然とするも
						PRINTFORML クスリと笑い、自らが上になって奉仕してやった。
					ENDIF
				ELSE
					PRINTFORML %CALLNAME:TARGET%は、寝ている%CALLNAME:MASTER%の下着を脱がせ
					PRINTL 顔を埋め秘処を嘗め回した。
					PRINTL そして十分に濡れて来たのを確認すると、怒張を宛がい一息に貫いた。
					PRINTL 　
					PRINTFORML 下腹部への衝撃に目を覚ました%CALLNAME:MASTER%はしばし呆然とするも
					PRINTFORML %CALLNAME:TARGET%との行為を受け入れ、自ら腰を振りだした……。
				ENDIF
			ENDIF
			CALLF SET_NIGHT_KOJO(0, 6)
			PRINTL 
			;夜這い騎乗位(主人が犯される系)イベント口上呼び出し
			TRYCALL SELF_KOJO, TARGET, 401
			PRINTL 
			;主人が処女
			IF TALENT:MASTER:処女
				IF TALENT:MASTER:ふたなり
					;Ａ経験(主人)
					CALL COMMON_UP_EXP, MASTER, 1, (ARG*2), 0
				ENDIF
				;03射精経験
				CALL COMMON_UP_EXP, TARGET, 3, ARG, 0
				;LUNATIC以上は射精するたびに体力が減少
				CALL EXHAUST_EJAC, TARGET, ARG
				;00快Ｃの珠
				CALL COMMON_UP_JUEL, TARGET, 0, (ARG*(70*ABL:Ｃ感覚*ABL:Ｃ感覚)), 0
				;12恭順の珠
				CALL COMMON_UP_JUEL, TARGET, 12, (ARG:1*60*ABL:従順), 0
				;13欲情の珠
				CALL COMMON_UP_JUEL, TARGET, 13, (ARG*100*ABL:欲望), 1
			ELSE
				;00Ｖ経験(主人)
				CALL COMMON_UP_EXP, MASTER, 0, (ARG*2), 0
				;03射精経験
				CALL COMMON_UP_EXP, TARGET, 3, ARG, 0
				;LUNATIC以上は射精するたびに体力が減少
				CALL EXHAUST_EJAC, TARGET, ARG
				;00快Ｃの珠
				CALL COMMON_UP_JUEL, TARGET, 0, (ARG*(70*ABL:Ｃ感覚*ABL:Ｃ感覚)), 0
				;12恭順の珠
				CALL COMMON_UP_JUEL, TARGET, 12, (ARG:1*60*ABL:従順), 0
				;13欲情の珠
				CALL COMMON_UP_JUEL, TARGET, 13, (ARG*100*ABL:欲望), 1
			ENDIF
			PRINTL 
			;地の文カット
			IF CFLAG:99 == 0
				IF TALENT:MASTER:処女 > 0
					PRINTFORML そして%CALLNAME:MASTER%に{ARG}回精液を吐き出すと、%CALLNAME:TARGET%は気恥ずかしげに帰って行った。
				ELSE
					PRINTFORML そして%CALLNAME:TARGET%は膣内に{ARG}回精液を吐き出すと、二人交わったまま眠りについた……。
				ENDIF
			ENDIF
		;主人にモノがついていて、奴隷は非オトコで恋慕もしくは淫乱有り。4分の3でエッチ
		ELSEIF (TALENT:MASTER:オトコ || TALENT:MASTER:ふたなり) && TALENT:オトコ == 0 && (TALENT:恋慕 || TALENT:淫乱) && LOCAL:1 >= 1
			;地の文カット
			SIF CFLAG:99 == 0
				PRINTFORML %CALLNAME:TARGET%は寝ている%CALLNAME:MASTER%のモノをフェラで準備を整えると
			IF TALENT:処女 == 1 && TALENT:恋慕
				;地の文カット
				IF CFLAG:99 == 0
					PRINTFORMW 自らの純潔を愛しい%CALLNAME:MASTER%へ捧げようと陰唇に亀頭を宛がう
					PRINTFORML だが期待に濡れすぎた陰唇が竿を滑ると%CALLNAME:TARGET%は嘆息と共に%CALLNAME:MASTER%へと寄り添った
				ENDIF
				CALLF SET_NIGHT_KOJO(0, 1)
				;夜這い騎乗位(失敗)イベント口上呼び出し
				TRYCALL SELF_KOJO, TARGET, 401
				PRINTL 
				;21愛情経験
				CALL COMMON_UP_EXP, TARGET, 21, ((ARG:1/2)+2), 0
				;12恭順の珠
				CALL COMMON_UP_JUEL, TARGET, 12, (ARG:1*200), 1
				PRINTL 
				;地の文カット
				SIF CFLAG:99 == 0
					PRINTFORML %CALLNAME:TARGET%はいつか%CALLNAME:MASTER%から奪って貰える時を夢見ながら眠りに落ちた……
			ELSE
				IF GET_AGG(TARGET) >= 90 && TALENT:処女 == 0 && TALENT:淫乱
					;地の文カット
					SIF CFLAG:99 == 0
						PRINTFORML 陰唇へと擦り付けて膣内へと導き、淫猥な腰使いとあえぎ声で%CALLNAME:MASTER%を起こした
					CALLF SET_NIGHT_KOJO(0, 2)
					;夜這い騎乗位(成功)イベント口上呼び出し
					TRYCALL SELF_KOJO, TARGET, 401
					PRINTL 
					;00Ｖ経験
					CALL COMMON_UP_EXP, TARGET, 0, ARG, 0
					;02絶頂経験
					CALL COMMON_UP_EXP, TARGET, 2, (ARG/2), 0
					;04精液経験
					CALL COMMON_UP_EXP, TARGET, 4, (ARG/2), 0
					IF COM_ABLE_CHECK_FEMALE(TARGET) && ARG/2 > 0
						C_NUMBER = MAX(RAND:(ARG/2 + 1), 0)
						CFLAG:3666 += C_NUMBER
						CALL CALCULATE_LIMIT, TARGET
					ENDIF
					;42Ｖ調律経験
					CALL COMMON_UP_EXP, TARGET, 42, ARG, 0
					;01快Ｖの珠
					CALL COMMON_UP_JUEL, TARGET, 1, (ARG*100), 0
					;12恭順の珠
					CALL COMMON_UP_JUEL, TARGET, 12, (ARG:1*150), 0
					;13欲情の珠
					CALL COMMON_UP_JUEL, TARGET, 13, (ARG*50), 1
					PRINTL 
					;地の文カット
					SIF CFLAG:99 == 0
						PRINTFORML そして{ARG}回行為を繰り返し絶頂すると、そのまま%CALLNAME:MASTER%の胸の上で眠ってしまった
				ELSE
					;地の文カット
					SIF CFLAG:99 == 0
						PRINTFORML 自らの秘部に挿入しようとしたが気恥ずかしさにそれを止めた
					CALLF SET_NIGHT_KOJO(0, 3)
					;夜這い騎乗位(思いとどまる)イベント口上呼び出し
					TRYCALL SELF_KOJO, TARGET, 401
					PRINTL 
					;04精液経験
					CALL COMMON_UP_EXP, TARGET, 4, 1, 0
					;22フェラ経験
					CALL COMMON_UP_EXP, TARGET, 22, ARG, 0
					;11習得の珠
					CALL COMMON_UP_JUEL, TARGET, 11, (ARG*150), 0
					;12恭順の珠
					CALL COMMON_UP_JUEL, TARGET, 12, (ARG:1*150), 1
					PRINTL 
					;地の文カット
					SIF CFLAG:99 == 0
						PRINTFORML そのままフェラチオを続け、%CALLNAME:MASTER%を射精させるとその隣で一緒に眠った……
				ENDIF
			ENDIF
		;ついてない×ついてない場合、レズッ気3以上で貝あわせ。
		ELSEIF (TALENT:オトコ == 0 && TALENT:ふたなり == 0) && (TALENT:MASTER:オトコ == 0 && TALENT:MASTER:ふたなり == 0) && ABL:レズっ気 > 2
			;地の文カット
			IF CFLAG:99 == 0
				PRINTFORML %CALLNAME:TARGET%は、寝ている%CALLNAME:MASTER%の下着をそっと脱がせると
				PRINTFORML すっかり濡れそぼった己の秘処を%CALLNAME:MASTER%の秘処と擦り合せ、快楽を貪りだした。
				PRINTL 　
				PRINTFORML 下腹部の違和感に目を覚ました%CALLNAME:MASTER%は
				PRINTFORML 途端に襲ってきた快楽に戸惑いながらも、負けじと腰を振り%CALLNAME:TARGET%を攻め立てた。
			ENDIF
			CALLF SET_NIGHT_KOJO(0, 7)
			;夜這い騎乗位(貝あわせ)イベント口上呼び出し
			TRYCALL SELF_KOJO, TARGET, 401
			PRINTL 
			;02絶頂経験
			CALL COMMON_UP_EXP, TARGET, 2, (ARG/2), 0
			;12レズ経験
			CALL COMMON_UP_EXP, TARGET, 12, ARG, 0
			;00快Ｃの珠
			CALL COMMON_UP_JUEL, TARGET, 0, (ARG*400), 0
			;12恭順の珠
			CALL COMMON_UP_JUEL, TARGET, 12, (ARG:1*300), 0
			;13欲情の珠
			CALL COMMON_UP_JUEL, TARGET, 13, (ARG*600), 1
			PRINTL 
			;地の文カット
			SIF CFLAG:99 == 0
				PRINTFORML そして{ARG}回行為を繰り返し%CALLNAME:TARGET%が絶頂すると、二人絡み合ったまま眠りに着いた。
		;ついていないと、強制的にオナニー
		ELSE
			;地の文カット
			IF CFLAG:99 == 0
				PRINTFORM %CALLNAME:MASTER%の手を取った%CALLNAME:TARGET%は、
				IF 90 >= GET_AGG(TARGET)
					PRINTFORML 下着の上から控え目にも秘所を擦らせ始める
				ELSE
					PRINTFORML 大胆にも下着の中にまで手を差し入れさせた
				ENDIF
			ENDIF
			CALLF SET_NIGHT_KOJO(0, 4)
			;夜這い騎乗位(オナニー)イベント口上呼び出し
			TRYCALL SELF_KOJO, TARGET, 401
			PRINTL 
			;02絶頂経験
			CALL COMMON_UP_EXP, TARGET, 2, 1, 0
			;10自慰経験
			CALL COMMON_UP_EXP, TARGET, 10, 1, 0
			;00快Ｃの珠
			CALL COMMON_UP_JUEL, TARGET, 0, (ARG*(100*ABL:Ｃ感覚)), 0
			;12恭順の珠
			CALL COMMON_UP_JUEL, TARGET, 12, (ARG:1*100), 0
			;13欲情の珠
			CALL COMMON_UP_JUEL, TARGET, 13, (ARG*200), 1
			PRINTL 
			;地の文カット
			IF CFLAG:99 == 0
				IF 90 >= GET_AGG(TARGET)
					PRINTFORML 声を押し殺して小さく絶頂すると、その余韻を残したまま眠りについた……
				ELSE
					PRINTFORML 遠慮のない甘い声と共にオナニーを続け%CALLNAME:MASTER%を見つめながら絶頂した……
				ENDIF
			ENDIF
		ENDIF
	ENDIF
ENDIF

;-----------------------------------------------
;添い寝成功
;-----------------------------------------------
;幼稚で反抗的、素直な場合
@NIGHT_EVENT_2, ARG
;地の文カット
IF CFLAG:99 == 0
	PRINTFORML 夜、%CALLNAME:TARGET%が寝間着を身につけ枕を抱えて%CALLNAME:MASTER%の部屋へとやってきた
	IF TALENT:反抗的 || TALENT:気丈 || TALENT:プライド高い
		PRINTFORMW 今日は%CALLNAME:MASTER%へのご褒美と、%CALLNAME:TARGET%が一緒に眠ってくれるとのことらしい
	ELSE
		PRINTFORMW 一人で眠るのが怖かったらしく、寂しそうな表情で%CALLNAME:MASTER%を見つめている
	ENDIF
ENDIF
;添い寝成功イベント口上呼び出し
TRYCALL SELF_KOJO, TARGET, 402
PRINTL 
LOCAL = (200*ABL:Ｃ感覚)+(ABL:Ｃ感覚*RAND:101)
;21愛情経験
CALL COMMON_UP_EXP, TARGET, 21, ((ARG/2)+2), 0
;12恭順の珠
CALL COMMON_UP_JUEL, TARGET, 12, LOCAL, 1
PRINTL 
;地の文カット
IF CFLAG:99 == 0
	IF TALENT:反抗的 || TALENT:気丈 || TALENT:プライド高い
		PRINTFORM 気丈に振る舞う%CALLNAME:TARGET%も、ベッドの中では
		SIF TALENT:小人体型 || TALENT:小柄体型
			PRINT 小さな身体で
		PRINTFORMW ギュッと%CALLNAME:MASTER%に抱きついてきた
	ELSE
		PRINTFORMW 一緒に寝てあげると、ベッドの中で%CALLNAME:TARGET%は嬉しそうに%CALLNAME:MASTER%に抱きついてきた
	ENDIF
ENDIF

;-----------------------------------------------
;嫁との一夜
;-----------------------------------------------
@NIGHT_EVENT_3, ARG, ARG:1
LOCAL = 1 + RAND:3
CALLF SET_NIGHT_KOJO(0, LOCAL)
SELECTCASE LOCAL
	CASE 1
		;地の文カット
		IF CFLAG:99 == 0
			PRINTFORMW %CALLNAME:MASTER%の嫁である%CALLNAME:TARGET%が夜食の招待に来てくれた
			PRINTFORML 真心のこもった愛妻料理を堪能すると、劣情を抱いた%CALLNAME:MASTER%は%CALLNAME:TARGET%を押し倒した
		ENDIF
		;嫁との一夜イベント口上呼び出し
		TRYCALL SELF_KOJO, TARGET, 404
		PRINTL 
