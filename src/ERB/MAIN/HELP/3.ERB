IF LOCAL >= 0 && LOCAL < LOCAL:1
	FOR LOCAL:3, 0, 60
		LOCAL:2 = LOCAL:3 + LOCAL * 60
		SIF STRLENS(TRAINNAME:(LOCAL:2)) > 0
			PRINTFORMLC [{LOCAL:2, 3}]%TRAINNAME:(LOCAL:2)%
