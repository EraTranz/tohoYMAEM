FOR PC_VAR1, 0, CHARANUM
	MARK:PC_VAR1:93 = 0
	;自分自身はスキップ
	SIF PC_VAR1 == ARG
		CONTINUE
	;手元にARG以外の子供がいて、かつ、ARGと同世代である
	IF (NO:PC_VAR1 == 2048 || NO:PC_VAR1 == 2049) && CFLAG:PC_VAR1:3108 == CFLAG:ARG:3108
