			CONTINUE
		CALLF SET_LIST, PC_VAR2, PC_VAR1
		PC_VAR2++
		MARK:PC_VAR1:93 = 1
	ENDIF
NEXT
PL_VAR_X:6 = PC_VAR2
PL_VAR_L:1 = PC_VAR2 / 5
