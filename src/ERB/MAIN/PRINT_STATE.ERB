;=============================================================================
;種族遺伝関連処理
;=============================================================================
;キャラ登録番号をARGに受け取り、そのキャラの種族を判定(=もっとも強く遺伝している種族遺伝子を探す)、STR:2に格納する
;なお、遺伝強度35%以上の遺伝子が二つあった場合、「○○と××のハーフ」という表現になる
;すべての遺伝子の遺伝強度が35未満なら「混血」と表現される
;-------------------------------------------------
;例外種族処理
;-------------------------------------------------
@GET_RACE , ARG
IF NO:ARG == 1000 && TALENT:ARG:998 && TALENT:ARG:動物耳
	STR:2 = チーター
	RETURN 1
ELSEIF NO:ARG == 5000
	STR:2 = 半人半菜
	RETURN 1
ELSEIF TALENT:ARG:呪精
	STR:2 = %STR:145%
	RETURN 1
ELSEIF TALENT:ARG:妖精 && CFLAG:ARG:5 == 1
	STR:2 = %STR:103%
	RETURN 1
ELSEIF TALENT:ARG:吸血鬼 && CFLAG:ARG:5 == 2
	STR:2 = %STR:106%
	RETURN 1
ELSEIF TALENT:ARG:機械 && CFLAG:ARG:5 == 3
	STR:2 = %STR:146%
	RETURN 1
ELSEIF TALENT:ARG:霊体 && CFLAG:ARG:5 == 4
	STR:2 = %STR:143%
	RETURN 1
ELSEIF TALENT:ARG:人形 && CFLAG:ARG:5 == 5
	STR:2 = %STR:121%
	RETURN 1
ELSEIF TALENT:ARG:悪魔 && CFLAG:ARG:5 == 6
	STR:2 = %STR:104%
	RETURN 1
ELSEIF TALENT:ARG:河童 && CFLAG:ARG:5 == 7
	STR:2 = %STR:127%
	RETURN 1
ELSEIF TALENT:ARG:妖狐 && CFLAG:ARG:5 == 8
	STR:2 = %STR:112%
	RETURN 1
ELSEIF TALENT:ARG:半白澤 && CFLAG:ARG:5 == 9
	STR:2 = %STR:116%
	RETURN 1
ELSEIF TALENT:ARG:スペルカード
	STR:2 = スペルカード
	RETURN 1
ENDIF

CALL FIND_RACE , ARG

;結果取り出し
LOCAL:1 = RESULT:1
LOCAL:2 = RESULT:2

;特定の混血は特殊表現にする(例：ワーハクタク、機械人形)
IF (LOCAL:1 == 3201 && LOCAL:2 == 3253) || (LOCAL:1 == 3253 && LOCAL:2 == 3201)
	STR:2 = ワーハクタク
ELSEIF (LOCAL:1 == 3221 && LOCAL:2 == 3246) || (LOCAL:1 == 3246 && LOCAL:2 == 3221)
	STR:2 = 機械人形
;以下標準処理
ELSEIF LOCAL:1 && LOCAL:2
	;片方の遺伝子が人形なら「○○人形」という表現
	IF LOCAL:1 == 3221 || LOCAL:2 == 3221
		STR:2 = \@(LOCAL:1 == 3221) ? %RACENAME(LOCAL:2)% # %RACENAME(LOCAL:1)%\@人形
	;そうでないなら「○○と××のハーフ」という形式
	ELSE
		STR:2 = %RACENAME(LOCAL:1)%と%RACENAME(LOCAL:2)%のハーフ
	ENDIF
;発現した遺伝子(=遺伝強度35%以上)がなければ混血
ELSEIF LOCAL:1 == 0 && LOCAL:2 == 0
	STR:2 = 混血
ELSE
	STR:2 = %RACENAME(LOCAL:1)%
ENDIF
RETURN 1

;-------------------------------------------------
;種族決定関数
;-------------------------------------------------
;キャラ登録番号を受け取り、種族を判定。変数重複が怖いので結果の取り出しはRESULT:X(Xは1か2)を直接参照
@FIND_RACE , ARG
;遺伝子を検索し、一番強く遺伝しているものと二番目に強く遺伝しているものをそれぞれLOCAL:1とLOCAL:2に格納する
;遺伝強度35%未満の遺伝子は無視される。該当するものがなければ0が格納される
;戻り値と種族の対応については@RACENAMEを参照
LOCAL:1 = 0
LOCAL:2 = 0
VARSET RESULT
FOR LOCAL , 3200 , 3400
	IF CFLAG:ARG:LOCAL >= 35
		IF CFLAG:ARG:LOCAL > CFLAG:ARG:(LOCAL:1) || LOCAL:1 == 0
			LOCAL:2 = LOCAL:1
			LOCAL:1 = LOCAL
		ELSEIF CFLAG:ARG:LOCAL > CFLAG:ARG:(LOCAL:2) || LOCAL:2 == 0
			LOCAL:2 = LOCAL
		ENDIF
	ENDIF
NEXT
RESULT:1 = LOCAL:1
RESULT:2 = LOCAL:2
RETURN 1

