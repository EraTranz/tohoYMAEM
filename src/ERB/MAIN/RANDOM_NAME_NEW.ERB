;=============================================================================
;名無しキャラ呼び名生成処理
;=============================================================================
;ここでは名無しキャラの呼び名・名前をランダムに決定。
;-----------------------------------------------
;奴隷用ランダムCALLNAME生成処理
;ARG:0……命名用分岐
;-----------------------------------------------
;その名の通り奴隷用の処理。SHOP2またはSYSTEMより呼び出される。
@CHARA_RANDOM_NAME , ARG:0

;変数の掃除
VARSET LOCAL
VARSET LOCALS
PL_VAR_I:0 = 0
$RETRY

TSTR:0 = %CALLNAME%

RESULT = 0
SIF TALENT:オトコ == 0 || ((NO == 2048 || NO == 3020) && TALENT:オトコ)
	CALL RANDOM_N , ARG:0

LOCAL:3 = RESULT

;重複チェック
LOCAL = FINDCHARA(CALLNAME, TSTR:0, 1)
LOCAL = (LOCAL != -1 && LOCAL != TARGET) ? 1 # 0
SIF LOCAL == 0
	LOCAL = RANDOM_N_CHECK_BABYES(TSTR:0)

;被ったらやりなおし300回まで
IF LOCAL && PL_VAR_I:0 < 300 && LOCAL:3 == 0
	PL_VAR_I:0 += 1
	NICKNAME = 
	GOTO RETRY
;300回やってもダメなら外国名か和名を200回回す
ELSEIF LOCAL && PL_VAR_I:0 < 500 && LOCAL:3 == 0
	$RETRY2
	;確率は半々
	LOCAL = RAND:2
	IF TALENT:オトコ
		IF LOCAL
			CALL RANDOM_N_BOYPOPNAME
		ELSE
			CALL RANDOM_N_BOY , RAND:2
		ENDIF
	ELSE
		IF LOCAL
			CALL RANDOM_N_POPNAME
		ELSE
			CALL RANDOM_N_GIRL , RAND:2
		ENDIF
	ENDIF
	;重複チェック
	LOCAL = FINDCHARA(CALLNAME, TSTR:0, 1)
	LOCAL = (LOCAL != -1 && LOCAL != TARGET) ? 1 # 0
	SIF LOCAL == 0
		LOCAL = RANDOM_N_CHECK_BABYES(TSTR:0)
	PL_VAR_I:0 += 1
	IF LOCAL && PL_VAR_I:0 < 500
		NICKNAME = 
		GOTO RETRY2
	ELSEIF LOCAL
		NICKNAME = 
		GOTO RETRY
	ENDIF
ENDIF

CALLNAME = %TSTR:0%
SIF TFLAG:651
	RETURN 1

IF TALENT:人形 == 1
	PRINTL その人形は自分に付けられたタグを指し示した。
ELSE
	PRINTFORM 名を尋ねると、%NAME%は
ENDIF

IF PL_VAR_I:0 < 500
	;人形はしゃべらないと思った
	IF TALENT:人形 == 1
		PRINTFORMW そこには「%CALLNAME%」と記してあった。
	ELSE
		;素質分岐
		IF TALENT:感情乏しい && NO:TARGET != 3016
			PRINT 鷹揚に
		ELSEIF TALENT:プライド高い
			PRINT 不機嫌そうに
		ELSEIF TALENT:臆病
			PRINT 怯えた様子で
		ELSEIF TALENT:貞操観念 || TALENT:抵抗 || TALENT:反抗的
			PRINT こちらを睨んで
		ELSEIF TALENT:幼稚
			PRINT はきはきと元気に
		ELSE
			PRINT 不審そうにしながらも
		ENDIF

		PRINTFORMW 「%CALLNAME%」と名乗った。
	ENDIF

	CALL DEFINE_JINKAKU_SELECT, MASTER
	;人形の場合
	IF TALENT:MASTER:人形 == 1
		PRINTL [0] 受け入れる
		PRINTL [1] 自分で名付ける
	;あなたの場合
	ELSEIF NO:MASTER == 1000 && TALENT:MASTER:オトコ
		PRINTL [0] いい名前だ
		PRINTL [1] 関係ないな
	ELSEIF NO:MASTER == 1000
		PRINTL [0] いい名前ね
		PRINTL [1] 関係ないわね
	;以下人格ごとに
	ELSEIF TFLAG:197 == 3 || TFLAG:197 == 1
		PRINTL [0] いい名前ね
		PRINTL [1] その名前はだめね
	ELSEIF TFLAG:197 == 2 || TFLAG:197 == 12
		PRINTL [0] いい名前だ
		PRINTL [1] その名はだめだ
	ELSEIF TFLAG:197 == 5 || TFLAG:197 == 6 || TFLAG:197 == 13
		PRINTL [0] いい名前ですね
		PRINTL [1] その名はちょっと…
	ELSEIF TFLAG:197 == 4 || TFLAG:197 == 14
		PRINTL [0] いい名前だね
		PRINTL [1] その名前はだめだよ
	ELSEIF TFLAG:197 == 11
		PRINTL [0] いい名じゃの
		PRINTL [1] その名はだめじゃ
	ELSE
		PRINTL [0] いい名前だ
		PRINTL [1] 関係ないな
	ENDIF

	$INPUT_LOOP_1
	INPUT
	IF RESULT == 1
		PRINTFORMW %CALLNAME:MASTER%は%NAME%に名前をつけてやることにした…
		PRINTL 
		$INPUT_LOOP_2
		PRINTL どんな名前にしますか？(0で元の名前)
		$INPUT_LOOP_3
		INPUTS
		STRLENS RESULTS
		IF RESULT > 14
			CLEARLINE 1
			REUSELASTLINE 名前が長すぎます！
			GOTO INPUT_LOOP_2
		ELSEIF RESULTS == "0" || RESULTS == CALLNAME
			CALL DEFINE_JINKAKU_SELECT, MASTER
			;人形の場合
			IF TALENT:MASTER:人形 == 1
				PRINTFORMW 「%CALLNAME%」を受け入れることにした…
			;あなたの場合
			ELSEIF NO:MASTER == 1000 && TALENT:MASTER:オトコ
				PRINTFORMW やはり、「%CALLNAME%」のままでいいだろう…
			ELSEIF NO:MASTER == 1000
				PRINTFORMW やはり、「%CALLNAME%」のままでいいわね…
			;以下人格ごとに
			ELSEIF TFLAG:197 == 3 || TFLAG:197 == 1
				PRINTFORMW やはり、「%CALLNAME%」のままでいいわよね…
			ELSEIF TFLAG:197 == 2 || TFLAG:197 == 12
				PRINTFORMW やはり、「%CALLNAME%」のままでいいだろう…
			ELSEIF TFLAG:197 == 5 || TFLAG:197 == 6|| TFLAG:197 == 13
				PRINTFORMW やはり、「%CALLNAME%」のままでいいですね…
			ELSEIF TFLAG:197 == 4 || TFLAG:197 == 14
				PRINTFORMW やはり、「%CALLNAME%」のままでいいよね…
			ELSEIF TFLAG:197 == 11
				PRINTFORMW やはり、「%CALLNAME%」のままでいいのう…
			ELSE
				PRINTFORMW やはり、「%CALLNAME%」のままでいいだろう…
			ENDIF

			PRINTFORM %CALLNAME%は
			;素質分岐
			IF TALENT:感情乏しい
				PRINT 興味がなさそうに
			ELSEIF TALENT:プライド高い
				PRINT 意味深に笑って
			ELSEIF TALENT:臆病
				PRINT ほっとしたように
			ELSEIF TALENT:貞操観念 || TALENT:抵抗 || TALENT:反抗的
				PRINT 怪しむ視線を向けたまま
			ELSEIF TALENT:幼稚
				PRINT 良くわかっていない様子で
			ELSE
				PRINT 少し安心したのか
			ENDIF
			PRINTFORMW 、小さく頷いた。
		ELSEIF RESULTS == ""
			GOTO INPUT_LOOP_3
		ELSE
			PRINTFORML 「%RESULTS%」でよろしいですか？
			PRINTL [0] はい
			PRINTL [1] いいえ
			$INPUT_LOOP_4
			INPUT
			IF RESULT == 0
				PRINTFORM %CALLNAME%改め%RESULTS%は、
				;素質分岐
				IF TALENT:感情乏しい && NO:TARGET != 3016
					PRINTW そう、と一言言っただけだった…
				ELSEIF TALENT:プライド高い
					PRINTW 歯軋りをして屈辱を堪えている…
				ELSEIF TALENT:臆病
					PRINTW 諦めたように力なくうなだれた…
				ELSEIF TALENT:貞操観念 || TALENT:抵抗 || TALENT:反抗的
					PRINTW 激昂して繰り返し抗議する…
				ELSEIF TALENT:幼稚
					PRINTW きょとんと首を傾げた…
				ELSE
					PRINTW 言いたい事を飲み込み、顔を背けた…
				ENDIF
				;任意名フラグ
				CFLAG:5100 = 1
				CALLNAME = %RESULTS%
			ELSEIF RESULT == 1
				GOTO INPUT_LOOP_2
			ELSE
				CLEARLINE 1
				REUSELASTLINE
				GOTO INPUT_LOOP_4
			ENDIF
		ENDIF
	ELSEIF RESULT == 0
		PRINTFORM いい名前だと言うと、%CALLNAME%は
		;素質分岐
		IF TALENT:感情乏しい && NO:TARGET != 3016
			PRINTW そう、と一言呟いた…
		ELSEIF TALENT:プライド高い
			PRINTW 当然と言うように鼻を鳴らした…
		ELSEIF TALENT:臆病
			PRINTW 不思議なものを見るような目をした…
		ELSEIF TALENT:貞操観念 || TALENT:抵抗 || TALENT:反抗的
			PRINTW 訝しげに眉を顰めた…
		ELSEIF TALENT:幼稚
			PRINTW にっこり笑って見せた…
		ELSE
			PRINTW 意図が解らずに首を傾げた…
		ENDIF
	ELSE
		CLEARLINE 1
		REUSELASTLINE
		GOTO INPUT_LOOP_1
	ENDIF
;全部被った場合
ELSE
	IF TALENT:人形 == 1
		PRINTW 名前を示すものはどこにも付いてなかった。
	ELSE
		PRINTW 名前はないと答えた。
	ENDIF
	PRINTFORMW %CALLNAME:MASTER%は%NAME%に名前をつけてやることにした…
	PRINTL 
	$INPUT_LOOP_5
	PRINTL どんな名前にしますか？
	$INPUT_LOOP_6
	INPUTS
	STRLENS RESULTS
	IF RESULT > 14
		CLEARLINE 1
		REUSELASTLINE 名前が長すぎます！
		GOTO INPUT_LOOP_5
	ELSEIF RESULTS == ""
		GOTO INPUT_LOOP_6
	ELSE
		PRINTFORML 「%RESULTS%」でよろしいですか？
		PRINTL [0] はい
		PRINTL [1] いいえ
		$INPUT_LOOP_7
		INPUT
		IF RESULT == 0
			PRINTFORMW %CALLNAME:MASTER%は%NAME%を、%RESULTS%と名付けた。
			PRINTFORM %RESULTS%は
			;素質分岐
			IF TALENT:人形 == 1
				PRINTW 不思議そうに首をかしげた…
			ELSEIF TALENT:感情乏しい
				PRINTW 、そう、と一言言っただけだった…
			ELSEIF TALENT:プライド高い
				PRINTW 案外、満更でもない様子だ…
			ELSEIF TALENT:臆病
				PRINTW 怯えながらも確かに頷いた…
			ELSEIF TALENT:貞操観念 || TALENT:抵抗 || TALENT:反抗的
				PRINTW 意外そうだったが、やがて満足げに微笑んだ…
			ELSEIF TALENT:幼稚
				PRINTW 嬉しそうに飛び跳ねている…
			ELSE
				PRINTW 不思議そうにくすくす笑った…
			ENDIF
			CALLNAME = %RESULTS%
		ELSEIF RESULT == 1
			GOTO INPUT_LOOP_5
		ELSE
			CLEARLINE 1
			REUSELASTLINE
			GOTO INPUT_LOOP_7
		ENDIF
	ENDIF
ENDIF

