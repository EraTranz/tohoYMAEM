TALENT:濡れやすい = 0
TALENT:Ｃ敏感 = 0
TALENT:Ｖ敏感 = 0
TALENT:Ａ敏感 = 0
TALENT:Ｂ敏感 = 0
REPEAT 4
	LOCAL = 1
	POWER LOCAL, 2, COUNT
	IF TALENT:(150+COUNT)
		CFLAG:14 -= LOCAL
		TALENT:(150+COUNT) = 0
	ENDIF
REND
SIF TALENT:服従
	CFLAG:15 -= 1
