@KOJO_1013_TRAIN_MESSAGE_S_COM_220
;初回のみ
IF TFLAG:95 == 1
	PRINTFORMW 「痛い！痛いよぉ！やめてぇぇぇ！」
;二回目以降
ELSE
	;恋慕＋マゾっ気Lv5以上
	IF TALENT:恋慕 == 1 && ABL:マゾっ気 >= 5
		PRINTFORMW 
	;恋慕＋マゾっ気Lv3以上
	ELSEIF TALENT:恋慕 == 1 && ABL:マゾっ気 >= 3
		PRINTFORMW 
	;恋慕
	ELSEIF TALENT:恋慕 == 1
		PRINTFORMW 「痛い……けど、なんか暖かい…」
	;服従＋マゾっ気Lv5以上
