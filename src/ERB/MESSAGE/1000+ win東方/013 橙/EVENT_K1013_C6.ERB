	IF TALENT:恋慕 == 1
		PRINTFORMW 
	;服従
	ELSEIF TALENT:服従 == 1
		PRINTFORMW 
	;V感覚Lv3以上
	ELSEIF ABL:Ｖ感覚 >= 3
		PRINTFORMW 
	;それ以外
	ELSE
		PRINTFORMW 
	ENDIF
ENDIF


