;-------------------------------------------------
;乳首ローター
;-------------------------------------------------
@KOJO_1026_TRAIN_MESSAGE_S_COM_190
IF KCOM:190 == 0
	PRINTW 「乳首に……ローター？」
	;陥落済み
	IF KFALL
		PRINTW 「ふぅん……構わんが、優しくしてくれ」
	;その他の場合
	ELSE
		PRINTW 「うぅ……悔しいけど気持ちいい……」
	ENDIF
	KCOM:190 = 1
ELSE
	;陥落済み
	IF KFALL
		SELECTCASE RAND:2
			CASE 0
				PRINTW 「気持ちいいよ……お前がやってくれると、なんでも気持ちいい」
				PRINTW 「もっと、強くできる……？」
			CASE 1
				PRINTW 「振動すごい、乳首じんじんする……ふふ……」
		ENDSELECT
	;その他の場合
	ELSE
		PRINTW 「なんだかなぁ……」
		PRINTW 「変な気分になる……」
	ENDIF
ENDIF
