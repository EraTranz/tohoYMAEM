	RETURN 0

;開始時
IF TEQUIP:コスチュームプレイ
	;初めて
	IF CFLAG:6090 == 0
		;恋慕
		IF TALENT:恋慕
			PRINTFORMW 「えっ？　素肌の上に……直接着けるの？」
			PRINTFORMW 「でも、それだと後ろとか丸見えだし……あんまり意味無さそうじゃないかしら？」
			PRINTFORMW %CALLNAME:MASTER%はメディスンに、裸エプロンとは新妻が着るべき礼装だと教えた
			PRINTFORMW 「……新妻？」
			PRINTFORMW 「それってつまり……、私は%TSTR:300%のお嫁さんって事よね？」
			PRINTFORMW 「～～♪♪」
			PRINTFORMW メディスンは嬉々としてエプロンを着けている…
		;それ以外
		ELSE
			PRINTFORMW 「エプロン？　これを着けるの？」
			PRINTFORMW 「人形遊びの次は……、今度はおままごと？」
		ENDIF
		CFLAG:6090 = 1
	;二回目以降
	ELSE
		;恋慕
		IF TALENT:恋慕
			IF RAND:2 == 0
				PRINTFORMW 「%TSTR:300%、今度は何するの？」
				PRINTFORMW 「あ、\@(CFLAG:5400 == 9) ? ご主人様 # 旦那様\@の方が良い？　それとも、\@(CFLAG:5400 == 8) ? ご主人様 # あなた\@？」
			ELSE
				PRINTFORMW 「%TSTR:300%、ご飯にする？　お風呂が先？」
				PRINTFORMW 「それとも……わ・た・し？」
				PRINTFORMW メディスンは両手を後ろで組んで、ニコニコと無邪気に微笑んでいる
			ENDIF
			CFLAG:6090 = 3
		;それ以外
		ELSE
			IF RAND:2 == 0
				PRINTFORMW 「そんなに“ごっこ遊び”がしたいの？」
				PRINTFORMW 「私の力を戻してくれたらいっぱいしてあげるよ？　……弾幕ごっこをね」
			ELSE
				PRINTFORMW 「はぁ……、こんなもの着せて、いったい何の意味があるの？」
				PRINTFORMW 「……裸に比べれば多少はマシかもしれないけど」
			ENDIF
			CFLAG:6090 = 2
		ENDIF
	ENDIF
;終了時
ELSEIF TEQUIP:コスチュームプレイ == 0
	;初めて
	IF CFLAG:8823 == 0
		;恋慕
		IF TALENT:恋慕
			PRINTFORMW 「えぇ～、脱ぐの～？」
			PRINTFORMW 「ちょっと気に入ってたのに～～」
			PRINTFORMW 「まぁ、%TSTR:300%の言いつけだものね」
		;それ以外
		ELSE
			PRINTFORML 「終わり？」
			PRINTFORMW 「人形相手におままごとして楽しかった？」
		ENDIF
		CFLAG:8823 = 1
	;二回目以降
	ELSE
		;恋慕
		IF TALENT:恋慕
			PRINTFORMW 「%TSTR:300%、脱がせて♪」
			PRINTFORMW %CALLNAME:MASTER%はメディスンの後ろに回り、結び目をシュルリと解いた
			CFLAG:8823 = 3
		;それ以外
		ELSE
			PRINTFORMW 「着せたり脱がしたり、どっちなの？」
			CFLAG:8823 = 2
		ENDIF
	ENDIF
ENDIF
