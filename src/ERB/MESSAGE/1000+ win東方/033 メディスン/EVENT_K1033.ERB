	;フラグリセットして戻す
	CALLF K1033_CFLAG75()
	RETURN LOCAL
ENDIF
;選んだ回数がキャラ数を越えている
IF LOCAL:1 >= CHARANUM
	;フラグリセットして戻す
	CALLF K1033_CFLAG75()
	RETURN -1
ELSE
	;選ぶ余地があるのでもう少し頑張る
	GOTO SET_LOOP
ENDIF

