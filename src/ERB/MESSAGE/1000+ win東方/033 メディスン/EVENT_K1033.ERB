;RETURN 33で戻ってきたならここで終わり
SIF RESULT == 33
	RETURN 0

;開始時
IF TEQUIP:Ｖ系装着器具 == 1
	;初めて
	IF CFLAG:6501 == 0
		;処女
		IF TALENT:処女 == 1
			;恋慕
			IF TALENT:恋慕
				PRINTFORMW 「やだっ！　痛い痛い！　%TSTR:300%、お願い、やめさせて！」
				PRINTFORMW 「痛いの……っ！　やぁぁっ！　助けてーーっ！！」
				PRINTFORML 悲鳴を上げ助けを求めるメディスンをよそに、触手は容赦無く純潔の膜を破り
				PRINTFORMW 子宮の手前までを一気に埋め尽くしていった
				PRINTFORMW 「いやぁぁあああっ！！　痛い痛ぁぁああいっ！！」
			;非恋慕
			ELSE
				PRINTFORMW 「ひっ！？　いやぁぁっ、そんなとこ入らな……っ！？」
				PRINTFORMW 「きゃふぁあっ！？　い、痛い……痛い痛い！」
				PRINTFORML 悲鳴を上げ身をよじるメディスンをよそに、触手は容赦無く純潔の膜を破り
				PRINTFORMW 子宮の手前までを一気に埋め尽くしていった
				PRINTFORMW 「ぅ、くぅっ！　やぁぁ……抜いて、抜いてぇっ！」
			ENDIF
		;非処女
		ELSE
			;恋慕＋触手経験20以上＋V感覚3以上
			IF TALENT:恋慕 && EXP:触手経験 >= 20 && ABL:Ｖ感覚 >= 3
				PRINTFORMW 「んっ、私は%TSTR:300%とだけがいいのに……あんっ！」
				PRINTFORMW 「こんなのとさせるなんて……%TSTR:300%のいじわる……んぁっ！」
				PRINTFORML メディスンはちょっと寂しそうに%CALLNAME:MASTER%の方を見ていたが、
				PRINTFORMW すぐに甘い声を漏らし、身をよじり始めた
				PRINTFORMW どうやら、胎内で“こんなの”呼ばわりされた触手が頑張っているようだ……
				PRINTFORMW 「いやぁ、はん、ふぁっ！　%TSTR:302%、やめさせて……ひぁぁんっ♪」
			;恋慕
			ELSEIF TALENT:恋慕
				PRINTFORMW 「いやぁ、私の中に入れていいのは%TSTR:300%だけなのぉ……！」
				PRINTFORMW 「だからダメぇ……！　入ってこないでぇ……！」
				PRINTFORMW 「んぅっ！　んぁ……はんっ……%TSTR:302%……」
				PRINTFORMW メディスンは苦しそうに表情を歪ませ、助けを求めるように%CALLNAME:MASTER%の方を見ている
			;非恋慕
			ELSE
				PRINTFORMW 「いやぁぁっ！　そんな得体の知れない妖怪なんか入れたくないわよっ！」
				PRINTFORML メディスンは身をよじって必死に抵抗しようとしているが、
				PRINTFORMW 触手は難なくその胎内へと潜り込んでいく
				PRINTFORMW 「はん、くぅ……！　やぁっ、私の中から出てって……」
			ENDIF
		ENDIF
		CFLAG:6501 = 1
	;二回目以降
	ELSE
		;恋慕＋触手中毒Lv3以上
		IF TALENT:恋慕 && ABL:触手中毒 >= 3
			IF RAND:3 == 0
				PRINTFORML 「あはっ、また触手に犯されちゃうよぉ」
				PRINTFORMW メディスンは恍惚とした表情で、膣内に押し入ってくる触手を見つめていた
				PRINTFORML 「ん、あなたも気持ちよくなりたいんでしょ？　一緒に気持ちよくなりましょ♪」
				PRINTFORMW そう言って笑うと、自らも腰を振るようにして積極的に触手の抽送を受け入れている
			ELSEIF RAND:2 == 0
				PRINTFORMW メディスンの秘唇は涎を垂らし触手を受け入れていく
				PRINTFORML 「あっ、あんっ！　触手、入ってくるぅ……！」
				PRINTFORMW 「ひぁぁんっ、もう奥に当たってるの……それ以上入らないよぉ……！」
			ELSE
				PRINTFORML 触手はメディスンの膣奥まで到達すると、その身をうねうねとくねらせ始めた
				PRINTFORMW 「あぁん、ダメぇ、そんなに中で動いちゃぁ……ふぁぁん♪」
				PRINTFORMW 「はぁぁあんっ、あんっ♪　やぁ、んぁっ、壊れちゃうよぉ……！」
			ENDIF
			CFLAG:6501 = 6
		;恋慕＋触手経験20以上
		ELSEIF TALENT:恋慕 && EXP:触手経験 >= 20
			IF RAND:2 == 0
				PRINTFORMW 「はんぅぅっ、……また、なの？」
				PRINTFORMW 「もしかして%TSTR:300%……んっ、私と触手がしてるの見て、興奮しちゃってるの？」
			ELSE
				PRINTFORMW 「ん……、ほんとは%TSTR:300%に入れて貰いたいのに……」
				IF RAND:2 == 0
					PRINTFORML 「仕方ないから……%TSTR:300%とした時、%TSTR:300%に気持ち良くなって貰える様に、
					PRINTFORMW 　あなたと練習よ。お手柔らかにね」
				ELSE
					PRINTFORMW 「せめて、%TSTR:300%の代わりに気持ちよくしてね」
				ENDIF
			ENDIF
			CFLAG:6501 = 5
		;恋慕
