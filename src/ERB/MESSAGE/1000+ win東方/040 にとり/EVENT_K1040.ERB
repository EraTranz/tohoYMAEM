				CFLAG:7300 = 7
			ELSE
				CFLAG:7300 = 5
			ENDIF
		;人間
		CASE 5
			;経験がなければその他
			IF CFLAG:7205 == 0
				CFLAG:7300 = 7
			ELSE
				CFLAG:7300 = 6
			ENDIF
		;その他
