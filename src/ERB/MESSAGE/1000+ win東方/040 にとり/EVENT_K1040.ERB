;ARG:1 プレイ強度　回数や入手珠などに補正
;ARG:2 プレイ相手 (bit 犬=1,豚=2,馬=4,触手=8,機械=16,人間=32,モブ=64)
;ARG:3　プレイ内容　（セックス=1,奉仕=2,浣腸=3,自慰=4,拡張=5）
;========================================================
@KOJO_1040_EVENT_NIGHT
;LOCAL 夜イベ相手
;LOCAL:1 NTRレベル
;屈服３イベント以前は弾く
SIF CFLAG:7000 < 1
	RETURN 0
PRINTL 
VARSET LOCAL
CFLAG:7300 = 0
CFLAG:7301 = 0
CFLAG:7225 = 0
;NTRレベルを合計
FOR COUNT, 7216, 7222
	CFLAG:7225 += CFLAG:COUNT
NEXT

;NTRイベントチェックへ
FOR LOCAL:1, 0, 6
	FOR COUNT, 7200, 7206
		RESULT = 0
		SIF CFLAG:(COUNT + 16) == LOCAL:1
			CALL K40_NTR_EVENT_CHECK, COUNT, LOCAL:1
		IF RESULT == 1
			CALL K40_AFFAIR_COUNT, RESULT:1, RESULT:2, RESULT:3, RESULT:4
			RETURN
		ENDIF
	NEXT
NEXT
;NTR有りや無しや？
IF CFLAG:7215 >= 2
	;立ってるビットのチェック
	;複数立ってる＋NTRレベル高で乱交
	;それ以外はNTR相手ランダム
	FOR COUNT, 1, 7
		GETBIT CFLAG:7215, COUNT
		SIF RESULT == 1
			CFLAG:7301 ++
	NEXT
	;乱交？
	IF (CFLAG:7301 > 1) && (CFLAG:7225 > 13)
		LOCAL = 2
	ENDIF
	;相手選択
	SELECTCASE RAND:7
		;犬
		CASE 0
			;経験が無ければその他
			IF CFLAG:7200 == 0
				CFLAG:7300 = 7
			ELSE
				CFLAG:7300 = 1
			ENDIF
		;豚
		CASE 1
			;経験が無ければその他
			IF CFLAG:7201 == 0
				CFLAG:7300 = 7
			ELSE
				CFLAG:7300 = 2
			ENDIF
		;馬
		CASE 2
			;経験が無ければその他
			IF CFLAG:7202 == 0
				CFLAG:7300 = 7
			ELSE
				CFLAG:7300 = 3
			ENDIF
		;触手
		CASE 3
			;経験が無ければその他
			IF CFLAG:7203 == 0
				CFLAG:7300 = 7
			ELSE
				CFLAG:7300 = 4
			ENDIF
		;機械
		CASE 4
			;経験が無ければその他
			IF CFLAG:7204 == 0
				CFLAG:7300 = 7
			ELSE
				CFLAG:7300 = 5
			ENDIF
		;人間
		CASE 5
			;経験がなければその他
			IF CFLAG:7205 == 0
				CFLAG:7300 = 7
			ELSE
				CFLAG:7300 = 6
			ENDIF
		;その他
		CASEELSE
			CFLAG:7300 = 7
	ENDSELECT
	;NTRレベルで分岐
	IF CFLAG:(7215 + CFLAG:7300) == 0
		LOCAL = 0
	;確率で非NTRの文章を
	ELSEIF RAND:(CFLAG:(7215 + CFLAG:7300) + 3)
		LOCAL = 0 
	ELSE
		LOCAL = 1
	ENDIF
	;合計NTRレベルが高かったら専用の”その他”
	SIF CFLAG:7300 == 7 && CFLAG:7225 > 4
		LOCAL = 1
;NTR無し
ELSE
	SELECTCASE RAND:7
		;犬
		CASE 0
			;経験が無ければその他
			IF CFLAG:7200 == 0
				CFLAG:7300 = 7
			ELSE
				CFLAG:7300 = 1
			ENDIF
		;豚
		CASE 1
			;経験が無ければその他
