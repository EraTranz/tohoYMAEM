@K40_ANAL_TEQUIP_CHECK
;直腸や括約筋などの開発回数を加算する為の関数
;触手アナル
IF TEQUIP:触手調教 && (TEQUIP:Ａ系装着器具 == 1)
	SELECTCASE CFLAG:7010
		;ブラシ　直腸開発
		CASE 3
			CFLAG:7051 += 1
		;吸盤　括約筋開発
		CASE 4
			CFLAG:7052 += 1
		;ブドウ　Ｓ字結腸開発
		CASE 5
			CFLAG:7050 += 1
		CASEELSE
			RETURN 0
	ENDSELECT
;アナルバイブ
ELSEIF TEQUIP:Ａ系装着器具 == 1
	SELECTCASE CFLAG:7001
		;歪バイブ　直腸開発
		CASE 3
			CFLAG:7051 += 1
		;先端バイブ　Ｓ字結腸開発
		CASE 4
			CFLAG:7050 += 1
		;根元バイブ　括約筋開発
		CASE 5
			CFLAG:7052 += 1
		CASEELSE
			RETURN 0
	ENDSELECT
;アナルローター
ELSEIF TEQUIP:Ａ系装着器具 == 3
	SELECTCASE CFLAG:7005
		;ピストンローター　Ｓ字結腸開発
		CASE 3
			CFLAG:7050 += 1
		;スクリューローター　直腸開発
		CASE 4
			CFLAG:7051 += 1
		;膨張ローター　括約筋開発
		CASE 5
			CFLAG:7052 += 1
		CASEELSE
			RETURN 0
	ENDSELECT
;アナル電極
ELSEIF TEQUIP:Ａ系装着器具 == 4
	SELECTCASE CFLAG:7004
		;排泄操作電極　直腸開発
		CASE 3
			CFLAG:7051 += 1
		;Ｓ字電極　Ｓ字結腸開発
		CASE 4
			CFLAG:7050 += 1
		;低周波　括約筋開発
		CASE 5
			CFLAG:7052 += 1
		CASEELSE
			RETURN 0
	ENDSELECT
;アナルビーズ
ELSEIF TEQUIP:Ａ系装着器具 == 5
	SELECTCASE CFLAG:7002
		;歪　括約筋開発
		CASE 3
			CFLAG:7052 += 1
		;振動　直腸開発
		CASE 4
			CFLAG:7051 += 1
		;長い　Ｓ字結腸開発
		CASE 5
			CFLAG:7050 += 1
		CASEELSE
			RETURN 0
	ENDSELECT
;浣腸
ELSEIF TEQUIP:Ａ系装着器具 == 7
	SELECTCASE CFLAG:7003
		;シャワ浣　括約筋開発
		CASE 3
			CFLAG:7052 += 1
		;エネマシリンジ　Ｓ字結腸開発
		CASE 4
			CFLAG:7050 += 1
		;イルリガートル　直腸開発
		CASE 5
			CFLAG:7051 += 1
		CASEELSE
			RETURN 0
	ENDSELECT
ELSE
	RETURN 0
ENDIF

@K40_NTR_COUNT
;触手、機械に対する好感度をカウント
;獣については個別コマンドで対応
;触手
IF TEQUIP:触手調教
	;挿入は２加算
	IF TEQUIP:Ｖ系装着器具 || TEQUIP:Ａ系装着器具 || TEQUIP:触手催眠 || TEQUIP:触手膣内産卵 || TEQUIP:触手腸内産卵
		CFLAG:7203 += 2
	ELSE
		CFLAG:7203 += 1
	ENDIF
	RETURN 0
;機械挿入
ELSEIF TEQUIP:Ｖ系装着器具 || TEQUIP:Ａ系装着器具
	CFLAG:7204 += 2
ELSEIF TEQUIP:Ｃ系装着器具 || TEQUIP:Ｕ系装着器具 || TEQUIP:Ｂ系装着器具
	CFLAG:7204 += 1
ELSE
	RETURN 0
ENDIF

@K40_NTR_COUNT_DECREASE, ARG
;主人による射精時にNTR好感度を減少させる
;ARG 射精箇所　ＡＶとそれ以外 (1 or 0)
;LOCAL 好感度減少数
SELECTCASE ARG
	;ＡＶ射精
	CASE 1
		LOCAL = 3
	;それ以外
	CASEELSE
		LOCAL = 1
ENDSELECT

FOR COUNT, 7200, 7206
	CFLAG:COUNT -= LOCAL
	SIF CFLAG:COUNT < 0
		CFLAG:COUNT = 0
NEXT

@K40_AFFAIR_COUNT, ARG, ARG:1, ARG:2, ARG:3
;夜イベントで使う
;行為の回数、内容、好感度など管理
;ビット演算なのは将来的にNTRで乱交とかやりたいから
;ARG プレイ場所　（bit Ｃ=1,Ｖ=2,Ａ=4,Ｂ=8）
;ARG:1 プレイ強度　回数などに補正
;ARG:2 プレイ相手 (bit 犬=1,豚=2,馬=4,触手=8,機械=16,人間=32,モブ=64)
;ARG:3　プレイ内容　（セックス=1,奉仕=2,浣腸=3,自慰=4,拡張=5,薬物=6）
;LOCAL 基本プレイ回数
;LOCAL:1　乱交フラグ（参加人数）
;LOCAL:2　プレイ場所カウント　経験への補正用
;LOCAL:3　計算用一時変数
;LOCAL:4　計算用変数
;
;CFLAG:7230 犬NTR用　最大輪姦犬数
;CFLAG:7231 犬NTR用　延べ経験犬数
;CFLAG:7232 犬NTR用　最大射精量
;CFLAG:7233 犬NTR用　延べ射精量
;CFLAG:7234 豚NTR用　最大精液浣腸量
;CFLAG:7235 豚NTR用　延べ精液浣腸量
;CFLAG:7236 豚NTR用　最大浣腸我慢時間
;CFLAG:7237 豚NTR用　延べ浣腸我慢時間
;CFLAG:7238 馬NTR用　最大飲精量
;CFLAG:7239 馬NTR用　延べ飲精量
;CFLAG:7240 馬NTR用　Ｖサイズ
;CFLAG:7241 馬NTR用　Ａサイズ
;CFLAG:7242 触手NTR用　最大産卵数
;CFLAG:7243 触手NTR用　延べ産卵数
;CFLAG:7244 触手NTR用　触手戦闘力
;CFLAG:7245 触手NTR用　触手サイズ
;CFLAG:7246 機械NTR用　最大生動画閲覧者数
;CFLAG:7247 機械NTR用　累計動画閲覧者数
;CFLAG:7248 機械NTR用　最大連続絶頂回数
;CFLAG:7249 機械NTR用　延べ連続絶頂回数
;CFLAG:7250 人間NTR用　最大売春単価
;CFLAG:7251 人間NTR用　売春総額
;CFLAG:7252 人間NTR用　経験人数
;CFLAG:7253 人間NTR用　薬物耐性
;

;初期化
VARSET LOCAL

;基本プレイ回数を算出
LOCAL = RAND:(ABL:欲望 + 1) + ARG:1 
;プレイ相手が増える毎に回数加算
FOR COUNT, 1, 7
	GETBIT ARG:2, COUNT
		IF RESULT == 1
			LOCAL += ARG:1
			LOCAL:1 ++
		ENDIF
NEXT
LOCAL /= 2 
;プレイ内容を見て、それに見合った経験を追加
;乱交は別に処理
IF LOCAL:1 >= 2

ELSE
	;感覚部位別に経験追加
	FOR COUNT, 0, 4
		GETBIT ARG, COUNT
		IF RESULT == 1
			LOCAL += RAND:5
			TRYCALLFORM K40_AFFAIR_PRINT_{COUNT}, LOCAL
			LOCAL:2 ++
		ENDIF
	NEXT
	;プレイ内容に応じて経験追加
	;獣姦、触手、排泄、自慰、奉仕快楽、フェラ、キス、拡張、苦痛快楽、被写、売春、失神辺りか
	SELECTCASE ARG:3
		;奉仕　奉仕快楽、フェラ、キス
		CASE 2
			LOCAL:3 = LOCAL / 2 + RAND:5
			EXP:キス経験 += LOCAL:3
			PRINTFORML キス経験 + {LOCAL:3}
			LOCAL:3 = LOCAL / 2 + RAND:10
			EXP:フェラ経験 += LOCAL:3
			PRINTFORML フェラ経験 + {LOCAL:3}
			EXP:奉仕快楽経験 += LOCAL
			PRINTFORML 奉仕快楽経験 + {LOCAL}
		;浣腸　排泄
		CASE 3
			EXP:調教排泄経験 += LOCAL
			PRINTFORML 調教排泄経験 + {LOCAL}
		;自慰　自慰
		CASE 4
			EXP:自慰経験 += LOCAL
			PRINTFORML 自慰経験 + {LOCAL}
		;拡張　苦痛快楽、拡張
		CASE 5
			GETBIT ARG, 1
			IF RESULT == 1
				EXP:Ｖ拡張経験 += LOCAL
				PRINTFORML Ｖ拡張経験 + {LOCAL}
			ENDIF
			GETBIT ARG, 2
			IF RESULT == 1
				EXP:Ａ拡張経験 += LOCAL
				PRINTFORML Ａ拡張経験 + {LOCAL}
			ENDIF
			EXP:苦痛快楽経験 += LOCAL
			PRINTFORML 苦痛快楽経験 + {LOCAL}
		;薬物
		CASE 6
			EXP:薬物経験 += LOCAL / 2
			PRINTFORML 薬物経験 + {LOCAL / 2}
	ENDSELECT
	;獣姦、触手、被写、売春はここで 好感度も同時に加算
	;獣姦？
	LOCAL:3 = 0
	IF ARG:2 & 1
		LOCAL:3 += LOCAL
		CFLAG:7200 += LOCAL
	ENDIF
	IF ARG:2 & 2
		LOCAL:3 += LOCAL
		CFLAG:7201 += LOCAL
	ENDIF
	IF ARG:2 & 4
		LOCAL:3 += LOCAL
		CFLAG:7202 += LOCAL
	ENDIF
	IF LOCAL:3 >= 1
		EXP:動物性愛経験 += LOCAL:3
		PRINTFORML 動物性愛経験 + {LOCAL:3}
	ENDIF
	;触手？
	IF ARG:2 & 8
		EXP:触手経験 += LOCAL
		CFLAG:7203 += LOCAL
		PRINTFORML 触手経験 + {LOCAL}
	ENDIF
	;被写？（機械と人間で）
	LOCAL:3 = 0
	IF ARG:2 & 16
		LOCAL:3 += (LOCAL / 5)
		CFLAG:7204 += LOCAL
	ENDIF
	IF ARG:2 & 32
		LOCAL:3 += (LOCAL / 2)
		CFLAG:7205 += LOCAL
	ENDIF
	IF LOCAL:3 >= 1
		EXP:被写経験 += LOCAL:3
		PRINTFORML 被写経験 + {LOCAL:3}
	ENDIF
	;薬物？（人間で）
	LOCAL:3 = 0
	IF ARG:2 & 32
		LOCAL:3 += (LOCAL / 3)
	ENDIF
	IF LOCAL:3 >= 1
		EXP:薬物経験 += LOCAL:3
		PRINTFORML 薬物経験 + {LOCAL:3}
	ENDIF
	;売春？（モブで）
	LOCAL:3 = 0
	CFLAG:7231 = 0
	SIF ARG:2 & 64
		LOCAL:3 += (RAND:LOCAL)
	IF LOCAL:3 >= 1
		EXP:売春経験 += LOCAL:3
		PRINTFORML 売春経験 + {LOCAL:3}
		;売春単価
		CFLAG:7250 = (50 * CFLAG:7221) + (RAND:100) - (EXP:薬物経験 / 10)
		;今回売春額
		LOCAL:4 = CFLAG:7250 * LOCAL:3
		;売春総額に加算
		CFLAG:7251 += LOCAL:4
	ENDIF
	;精液経験、絶頂回数及び失神回数 もしあれば射精回数
	LOCAL:3 = LOCAL + RAND:10
	;機械は精液出さない
	SIF ARG:2 == 16
		LOCAL:3 = 0
	;自慰かつふたなりでない場合精液経験なし
	SIF ARG:2 == 0 && TALENT:ふたなり == 0
		LOCAL:3 = 0
	EXP:精液経験 += LOCAL:3
	SIF LOCAL:3 > 0
		PRINTFORML 精液経験 + {LOCAL:3}
	;絶頂経験は常にLOCAL以上
	MAX LOCAL, RAND:((LOCAL + LOCAL:2 + ARG:1) * 3)
	;Ａが責められたらもっかい計算
	SIF ARG & 4
		MAX RESULT, RAND:((LOCAL + LOCAL:2 + ARG:1) * 3)
	;責められた箇所一つのみ且つＡの場合ボーナス付きでもっかい計算
	SIF LOCAL:2 == 1 && (ARG & 4)
		MAX RESULT, RAND:((LOCAL + ARG:1 + 5) * 3)
	EXP:絶頂経験 += RESULT
	SIF RESULT > 0
		PRINTFORML 絶頂経験 + {RESULT}
	IF TALENT:ふたなり
		LOCAL:3 = LOCAL + RAND:3
		EXP:射精経験 += LOCAL:3
		PRINTFORML 射精経験 + {LOCAL:3}
	ENDIF
	LOCAL:3 = RESULT / 10
	EXP:調教失神経験 += LOCAL:3
	SIF LOCAL:3 > 0
		PRINTFORML 調教失神経験 + {LOCAL:3}
	IF CFLAG:7221 >= 1 && (ARG:2 & 64)
		PRINTFORML 
		;売春単価が正か負か？
		IF CFLAG:7250 > 0
			PRINTFORML にとりの身体は一回{CFLAG:7250}円で買われ、{LOCAL:4}円稼ぎ出しました。
			;借金があるか？
			IF CFLAG:7251 > 0
				PRINTFORML 売春によってにとりが貢いだ総額は{CFLAG:7251}円です。
			ELSE
				PRINTFORML 返済に充てて残った借金は{CFLAG:7251 * (-1)}円です。
			ENDIF
		ELSE
			PRINTFORML 薬物代を含め、一回{CFLAG:7250 * (-1)}の経費が掛かりました。
			;貢いだ金が正か負か
			IF CFLAG:7251 > 0
				PRINTFORML {LOCAL:4 * (-1)}円の借りが生まれ、貢いだ総額は{CFLAG:7251}円です。
			ELSE
				PRINTFORML 新たに{LOCAL:4 * (-1)}円の借りが生まれ、借金は{CFLAG:7251 * (-1)}円です。
			ENDIF
		ENDIF
	ENDIF
	PRINTFORML 
	PRINTFORML ………………
	PRINTFORML …………
	PRINTFORML ……
	;人間は外の世界から帰ってくる風に
	IF ARG:2 & 32
		PRINTFORMW 意識を失ったにとりは、いつの間にか元の部屋へと戻っていた……
	ELSE
		PRINTFORMW にとりは後始末を済ませ、倒れ込むように寝床へ伏せた……
	ENDIF
ENDIF

