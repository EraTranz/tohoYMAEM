@KOJO_1042_TRAIN_MESSAGE_F_COM_500
	CALL K42_KOJO_MESSAGE_A
@KOJO_1042_TRAIN_MESSAGE_S_COM_500
	CALL K42_KOJO_MESSAGE_B
	SIF CFLAG:6951 == 1
		RETURN 0

	;開始時
	IF TEQUIP:触手調教 == 1
		;初めて
		IF CFLAG:7750 == 0
			;深愛＋反発刻印Lv3
			IF CFLAG:6953 == 3
				PRINTFORMW 「ひっ！　い、イソギンチャクのお化け！」
				PRINTFORMW 早苗の体は震え、無意識の内に後退りをしていた
				PRINTFORMW 「や、やだ……こっちに来ないでっ！」
			;反発刻印Lv3
			ELSEIF CFLAG:6953 == 2
				PRINTFORMW 「な、何ですそれ……？」
				PRINTFORMW 「まさか……それを私と交わらせる訳じゃないですよね？」
				PRINTFORMW 早苗は恐怖で引きつった顔をこちらに向けながら尋ねた
			;陥落
			ELSEIF CFLAG:6953 == 1
				PRINTFORMW 「こ、こんなものの相手をしないといけないんですか？」
			;それ以外
			ELSE
				PRINTFORMW 「えっ……何……これ……」
				PRINTFORMW 触手を呼び出すと、早苗は言葉を失い茫然と立ち尽くした
				PRINTFORMW “逃げろ”と心の底から発しているものの、恐怖で体が動かなくなってしまったようだ
			ENDIF
				CFLAG:7750 = 1
		;二回目以降
		ELSE
			;深愛＋反発刻印Lv3
			IF CFLAG:6953 == 3
				PRINTFORMW 「また……ですか？」
				PRINTFORMW 「これ嫌いです……ぬるぬるして乱暴ですから……」
			;反発刻印Lv3
			ELSEIF CFLAG:6953 == 2
				PRINTFORMW 「いやっ、こっちにこないで！」
				PRINTFORMW 触手から逃げ回った早苗だが、そう長くは逃げ回る事は出来なかった
			;陥落＋触手中毒Lv3以上
			ELSEIF CFLAG:6953 == 1 && ABL:触手中毒 >= 3
				IF RAND:4 == 0
					PRINTFORMW 「激しく、して下さい……」
					PRINTFORMW 早苗は抵抗する事も忘れ、触手に身を委ねた
				ELSEIF RAND:3 == 0
					PRINTFORMW 「今日もたくさん、いじめて下さいね」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「また、ぬるねるにしてくれるんですね」
				ELSE
					PRINTFORMW 「んっ、触手さんと、するんですね」
				ENDIF
			;陥落
			ELSEIF CFLAG:6953 == 1
				PRINTFORMW 「やっぱり、%CSTR:90%に愛して欲しいなぁ……」
			;それ以外
			ELSE
				PRINTFORMW 「来ないでっ！　いっ、いやぁぁぁ！」
			ENDIF
		ENDIF
	;終了時
	ELSEIF TEQUIP:触手調教 == 0
		;初めて
		IF CFLAG:7751 == 0
			;深愛＋反発刻印Lv3
			IF CFLAG:6953 == 3
				PRINTFORMW 「終わった……？　終わったんですね？」
				PRINTFORMW 辺りを見回しながら、早苗は%CALLNAME:MASTER%に尋ねた
				PRINTFORMW 恐怖が抜け切らない体は、まだ安息を取り戻してはいないようだ
			;反発刻印Lv3
			ELSEIF CFLAG:6953 == 2
				PRINTFORMW 「こんな酷い事をするあなたは、人ではなく悪魔です……」
				PRINTFORMW 体に付いた粘液に不快感を示しながら、早苗はベッドに横になった
				PRINTFORMW 気力と体力を根刮ぎ奪われたようだ
			;陥落＋触手中毒Lv3以上
			ELSEIF CFLAG:6953 == 1 && ABL:触手中毒 >= 3
				PRINTFORMW 「んっ、くっ……もう終わりですか……？」
				PRINTFORMW 「もっと暴れても良かったのに……」
			;陥落
			ELSEIF CFLAG:6953 == 1
				PRINTFORMW 「はぁ、はーっ……やっぱり%CSTR:90%がいいです」
			;それ以外
			ELSE
				PRINTFORMW 「何でもしますから、触手は、触手だけはもうやめて……」
			ENDIF
				CFLAG:7751 = 1
		;二回目以降
		ELSE
			;深愛＋反発刻印Lv3
			IF CFLAG:6953 == 3
				PRINTFORMW 「触手は、はっ、んっ、もう嫌です」
				PRINTFORMW 息を切らした早苗は、弱々しい声で言い放った
			;反発刻印Lv3
			ELSEIF CFLAG:6953 == 2
				PRINTFORMW 「はーっ、はっ……人でなし……っ」
				PRINTFORMW 息を切らしながら、早苗は言葉を紡いだ
			;陥落＋触手中毒Lv3以上
			ELSEIF CFLAG:6953 == 1 && ABL:触手中毒 >= 3
				PRINTFORMW 「次も、ぬるぬるにして下さい」
			;陥落
			ELSEIF CFLAG:6953 == 1
				PRINTFORMW 「%CSTR:90%なら、ちゃんと愛してくれるのに……」
			;それ以外
			ELSE
				PRINTFORMW 「くっ、あなたに触られる方がまだましです」
			ENDIF
		ENDIF
	ENDIF

