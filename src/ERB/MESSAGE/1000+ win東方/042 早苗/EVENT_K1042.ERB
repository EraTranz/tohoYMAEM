;-------------------------------------------------
;愛撫 CFLAG:7000
;-------------------------------------------------
@KOJO_1042_TRAIN_MESSAGE_F_COM_0
	CALL K42_KOJO_MESSAGE_A
@KOJO_1042_TRAIN_MESSAGE_S_COM_0
	CALL K42_KOJO_MESSAGE_B
	SIF CFLAG:6951 == 1
		RETURN 0



@KOJO_1042_TRAIN_TEXT_COM_0
	CALL K42_KOJO_MESSAGE_C

;-------------------------------------------------
;クンニ（絶頂寸止め中） CFLAG:7001
;クンニ（焦らしプレイ） CFLAG:7002
;クンニ CFLAG:7003
;-------------------------------------------------
@KOJO_1042_TRAIN_MESSAGE_F_COM_1
	CALL K42_KOJO_MESSAGE_A
@KOJO_1042_TRAIN_MESSAGE_S_COM_1
	CALL K42_KOJO_MESSAGE_B
	SIF CFLAG:6951 == 1
		RETURN 0

	;絶頂寸止め中
	IF EX:20 >= 1
		;初めて
		IF CFLAG:7001 == 0
			;深愛＋反発刻印Lv3
			IF CFLAG:6953 == 3
				PRINTFORMW 「はくっ！　うっ、お、お願いっ！」
				PRINTFORMW 「もう、イカせてっ……イカせて下さい……っ」
			;反発刻印Lv3
			ELSEIF CFLAG:6953 == 2
				PRINTFORMW 「はぁ、ひうっ！　んんっ！」
				PRINTFORMW 「はっ、あっ、ああーっ！」
				PRINTFORMW 爆発寸前の快楽は、早苗の頭に直接響いているようだ
				PRINTFORMW 早苗は許しを乞おうとするものの、自制心がその動きにブレーキをかけていた
			;陥落
			ELSEIF CFLAG:6953 == 1
				PRINTFORMW 「はっ、うっ、あぁ……」
				PRINTFORMW 絶頂寸前の状態で止められ、早苗は体をぷるぷると震わせた
			;屈服刻印Lv2以上
			ELSEIF MARK:屈服刻印 >= 2
				PRINTFORMW 「お願いっ……お願いだからイカせて……っ！」
				PRINTFORMW 快楽に支配され、早苗は自ら快楽を望んだ
			;それ以外
			ELSE
				PRINTFORMW 「うっ、あぁ……あっ……」
				PRINTFORMW 「イカせて……もうイカせて……っ」
			ENDIF
				CFLAG:7001 = 1
		;二回目以降
		ELSE
			;深愛＋反発刻印Lv3
			IF CFLAG:6953 == 3
				PRINTFORMW 「はーっ、はーっ、これ以上、やったら、あっ、もうっ！」
			;反発刻印Lv3
			ELSEIF CFLAG:6953 == 2
				PRINTFORMW 「はっ、いっ、イカ……うっ、くっ、」
			;陥落＋C感覚Lv3以上
			ELSEIF CFLAG:6953 == 1 && ABL:Ｃ感覚 >= 3
				;深愛
				IF CFLAG:6805 >= 7
					IF RAND:4 == 0
						PRINTFORMW 「ひぃん！　や、やめたら、あっ、ああっ！」
					ELSEIF RAND:3 == 0
						PRINTFORMW 「はっ、あっ、やめないでっ！　もうイカせてっ！」
						PRINTFORMW 早苗の体は痙攣したように震え、絶頂を望む声が絶えなかった
					ELSEIF RAND:2 == 0
						PRINTFORMW 「いっ、いっ、ああっ……」
						PRINTFORMW 早苗は泣きながら絶頂を乞い願う
						PRINTFORMW だが、その願いが叶う事はなかった
					ELSE
						PRINTFORMW 「ひっ、あっ、ああっ、やぁっ！」
					ENDIF
				;それ以外
				ELSE
					IF RAND:4 == 0
						PRINTFORMW 「いっ、イクッ！　いぐぅ！」
						PRINTFORMW 早苗を絶頂寸前まで追い込むものの、%CALLNAME:MASTER%が昇天を許す事はなかった
					ELSEIF RAND:3 == 0
						PRINTFORMW 「ふっ、あっ、くっ……んっ……」
						PRINTFORMW 早苗の体は小刻みに震え、言葉を紡ぐ事さえままらなかった
					ELSEIF RAND:2 == 0
						PRINTFORMW 「やっ、はっ、あっ、んんっ！」
						PRINTFORMW 「頭が、壊れちゃい、ますから……っ」
					ELSE
						PRINTFORMW 「ひぐっ！　いっ、イカせて、下さいっ！」
					ENDIF
				ENDIF
			;陥落
			ELSEIF CFLAG:6953 == 1
				;深愛
				IF CFLAG:6805 >= 7
					PRINTFORMW 「イクぅ、イッちゃ、あっ……」
					PRINTFORMW 「あっ……ああっ……お願い……っ、イカせて……っ！」
				;それ以外
				ELSE
					PRINTFORMW 「ひっ、ああっ……ちゃんと、言う事聞きますから……っ」
					PRINTFORMW 「だから、イカせて……っ」
				ENDIF
			;屈服刻印Lv3
			ELSEIF MARK:屈服刻印 == 3
				PRINTFORMW 「はっ、ひっ、こんな、無理です……からっ！」
			;それ以外
			ELSE
				PRINTFORMW 「ひゃ、あっ、ああっ！」
				PRINTFORMW 早苗の体はぷるぷると震え、泣きながら昇天する事を乞い願った
			ENDIF
		ENDIF
	;焦らしプレイ中
	ELSEIF TEQUIP:焦らしプレイ == 1
		;初めて
		IF CFLAG:7002 == 0
			;深愛＋反発刻印Lv3
			IF CFLAG:6953 == 3
				PRINTFORMW 「うっ、んっ……そんな、優しすぎて……っ」
				PRINTFORMW 早苗は激しさを求めようとしたが、快感に耽ろうとする自分を恥じた
			;反発刻印Lv3
			ELSEIF CFLAG:6953 == 2
				PRINTFORMW 「あくっ……っ、何でも、ありません……っ！」
				PRINTFORMW 必死に快楽を振り払い、早苗は理性を保った
			;陥落
			ELSEIF CFLAG:6953 == 1
				PRINTFORMW 「はっ、くっ……お願い、もっと、早苗をいじめて……っ！」
			;屈服刻印Lv2以上
			ELSEIF MARK:屈服刻印 >= 2
				PRINTFORMW 「ああっ、くぅ、んんっ！」
				PRINTFORMW 早苗は激しい快楽を求めようとするものの、%CALLNAME:MASTER%の視線が突き刺さる
				PRINTFORMW 微弱な快感に悶える事しか、早苗に出来る事はなかった
			;それ以外
			ELSE
				PRINTFORMW 「ひくっ！　んっ……そんな、事するくらいならっ！」
			ENDIF
				CFLAG:7002 = 1
		;二回目以降
		ELSE
			;深愛＋反発刻印Lv3
			IF CFLAG:6953 == 3
				PRINTFORMW 「はくっ！　やっ、優しくされたらぁ……」
			;反発刻印Lv3
			ELSEIF CFLAG:6953 == 2
				PRINTFORMW 「くっ、はっ、はっ……んんっ！」
			;陥落＋C感覚Lv3以上
			ELSEIF CFLAG:6953 == 1 && ABL:Ｃ感覚 >= 3
				;深愛
				IF CFLAG:6805 >= 7
					IF RAND:4 == 0
						PRINTFORMW 「ひゃ！　やっ、優しくじゃなくていいですっ！」
						PRINTFORMW 「もっと、激しく、して欲しいんですっ！」
					ELSEIF RAND:3 == 0
						PRINTFORMW 「ふあぁ、駄目ぇ！　そんな、優しくなんてぇ！」
						PRINTFORMW 「あっ、こんな、変になるぅ！」
					ELSEIF RAND:2 == 0
						PRINTFORMW 「ひゃうっ！　駄目っ、弄ってもらわないと駄目なんですっ！」
					ELSE
						PRINTFORMW 「はぁ、っはぁ……お願いっ、もっと、激しく……っ！」
					ENDIF
				;それ以外
				ELSE
					IF RAND:4 == 0
						PRINTFORMW 「はくっ、もっと、激しく……っ」
						PRINTFORMW 「ご主人様、お願いします……っ」
					ELSEIF RAND:3 == 0
						PRINTFORMW 「はんっ、あっ、んんっ！」
						PRINTFORMW 「もっと、強くっ！　お願いですからっ！」
					ELSEIF RAND:2 == 0
						PRINTFORMW 「くぅ……どうして激しくしてくれないんです……っ」
					ELSE
						PRINTFORMW 「ふあっ、もっと、もっと強くして下さいっ！」
					ENDIF
				ENDIF
			;陥落
			ELSEIF CFLAG:6953 == 1
				;深愛
				IF CFLAG:6805 >= 7
					PRINTFORMW 「ひゃ、はくっ！　らめれすぅ……」
					PRINTFORMW 微妙な快感に悶える早苗は、更なる快感を求めておねだりした
				;それ以外
				ELSE
					PRINTFORMW 「はっ、くっ……」
					PRINTFORMW 「ご主人様、焦らしなんて……っ」
				ENDIF
			;屈服刻印Lv3
			ELSEIF MARK:屈服刻印 == 3
				PRINTFORMW 「はくっ、んっ、んんーっ！」
				PRINTFORMW 「駄目ぇ……こんなのじゃ駄目なのぉ……」
			;それ以外
			ELSEIF MARK:屈服刻印 <= 1
				PRINTFORMW 「ひっ、あんっ！　そんな、所、触っちゃ！」
			ENDIF
		ENDIF
	;それ以外
	ELSE
		;初めて
		IF CFLAG:7003 == 0
			;深愛＋反発刻印Lv3
			IF CFLAG:6953 == 3
				PRINTFORMW 「ひゃっ！　もっと優しく……」
			;反発刻印Lv3
			ELSEIF CFLAG:6953 == 2
				PRINTFORMW 「ひゃ！？　やめて、そんな事しないで！」
				PRINTFORMW これ以上近付かないように、早苗は%CALLNAME:MASTER%の頭を押さえ付けた
				PRINTFORMW だが、力を封じられた早苗に、%CALLNAME:MASTER%を防ぐ力は残っていなかった
			;陥落
			ELSEIF CFLAG:6953 == 1
				PRINTFORMW 「……早苗のクリトリス、綺麗にして下さい」
				PRINTFORMW 早苗はＭ字に脚を開き、%CALLNAME:MASTER%をその間へと導いた
			;屈服刻印Lv2以上
			ELSEIF MARK:屈服刻印 >= 2
				PRINTFORMW 「んっ、そこ、汚いですから……」
				PRINTFORMW 「舐めちゃ、嫌です」
				PRINTFORMW 早苗は顔を真っ赤にしながら、成り行きを見守った
			;それ以外
			ELSE
				PRINTFORMW 「えっ、ちょ、ちょっと何するんです！」
				PRINTFORMW 「やっ、あっ！？　そんなとこ……っ！」
			ENDIF
				CFLAG:7003 = 1
		;二回目以降
		ELSE
			;深愛＋反発刻印Lv3
			IF CFLAG:6953 == 3
				PRINTFORMW 「拒むつもりはありませんけど……優しくして下さいよ？」
			;反発刻印Lv3
			ELSEIF CFLAG:6953 == 2
				PRINTFORMW 「くっ、うっ！　来ないで！」
				SIF TEQUIP:緊縛関連 == 0
					PRINTFORMW 秘唇に舌を這わせる%CALLNAME:MASTER%を、早苗は全力で引き離した
			;陥落＋C感覚Lv3以上
			ELSEIF CFLAG:6953 == 1 && ABL:Ｃ感覚 >= 3
				;深愛
				IF CFLAG:6805 >= 7
					IF RAND:4 == 0
						PRINTFORMW 「そこっ、剥いちゃ！　ひゃあっ！」
						PRINTFORMW 舌でクリトリスの皮を剥いてみせると、早苗は甲高い声を響かせた
					ELSEIF RAND:3 == 0
						PRINTFORMW 「ふあっ！　あっ、いいっ、ひゃあんっ！」
						PRINTFORMW 早苗は涎を垂らしながら、%CALLNAME:MASTER%を膨らんだ核に近付けた
					ELSEIF RAND:2 == 0
						PRINTFORMW 「んっ、はぁっ！　気持ち、いいっ！」
					ELSE
						PRINTFORMW 「はぁ……もっと気持ち良くして下さい……」
						PRINTFORMW %CALLNAME:MASTER%の頭を撫でながら、早苗は更なる快楽を求めてきた
					ENDIF
				;それ以外
				ELSE
					IF RAND:4 == 0
						PRINTFORMW 「そこっ、敏感ですからっ！」
						PRINTFORMW 「もっと弄ってイカせてぇ！」
					ELSEIF RAND:3 == 0
						PRINTFORMW 「はぁ、っはぁ……ふふっ、敏感な所ばっかりですね」
					ELSEIF RAND:2 == 0
						PRINTFORMW 「っ！？　はっ、うぅ！！」
						PRINTFORMW 早苗は嬌声を押し殺そうと口を閉じている
						PRINTFORMW しかし、敏感になったクリトリスをなぞると、早苗の防壁は簡単に崩れ去った
					ELSE
						PRINTFORMW 「そこ、敏感ですからっ」
						PRINTFORMW 「もっと弄ってぇ！」
					ENDIF
				ENDIF
			;陥落
			ELSEIF CFLAG:6953 == 1
				;深愛
				IF CFLAG:6805 >= 7
					PRINTFORMW 「%CSTR:90%の舌っ、すごくてっ！　ひっ、ひゃう！」
					PRINTFORMW 舌を小刻みに動かすと、早苗のそれは応えるように膨らんだ
				;それ以外
				ELSE
					PRINTFORMW 「あんっ、早苗のそこ敏感なんです」
					PRINTFORMW 「あっ、はうっ！　あっ！　んんっ！」
				ENDIF
			;屈服刻印Lv3
			ELSEIF MARK:屈服刻印 == 3
				PRINTFORMW 「ふあぁ……駄目っ、そんな所舐めないでぇ……」
			;それ以外
			ELSE
				PRINTFORMW 「やめて……やめて下さいっ！　ああっ！」
				SIF TEQUIP:緊縛関連 == 0
					PRINTFORMW %CALLNAME:MASTER%を引き離そうとするも、か弱い力では何の意味を成さなかった
			ENDIF
		ENDIF
	ENDIF

@KOJO_1042_TRAIN_TEXT_COM_1
	CALL K42_KOJO_MESSAGE_C

;-------------------------------------------------
;アナル愛撫（焦らしプレイ中） CFLAG:7004
;アナル愛撫（アナルバイブ装着中） CFLAG:7005
;アナル愛撫（アナルビーズ装着中） CFLAG:7006
;アナル愛撫（浣腸プレイ中） CFLAG:7007
;アナル愛撫 CFLAG:7008
;-------------------------------------------------
@KOJO_1042_TRAIN_MESSAGE_F_COM_2
	CALL K42_KOJO_MESSAGE_A
@KOJO_1042_TRAIN_MESSAGE_S_COM_2
	CALL K42_KOJO_MESSAGE_B
	SIF CFLAG:6951 == 1
		RETURN 0

	;焦らしプレイ中
	IF TEQUIP:焦らしプレイ == 1
		;初めて
		IF CFLAG:7004 == 0
			;アナル処女
			IF EXP:Ａ経験 == 0
				;深愛＋反発刻印Lv3
				IF CFLAG:6953 == 3
					PRINTFORMW 「ひあっ！？　なっ、やめてっ！」
					PRINTFORMW 「お尻なんか弄っちゃ駄目ぇ……」
				;反発刻印Lv3
				ELSEIF CFLAG:6953 == 2
					PRINTFORMW 「はくっ！　何を考えてるんですっ！」
					PRINTFORMW 「お尻なんて触らないでっ！」
					IF TALENT:処女 == 1
						PRINTFORMW 早苗は反抗的な態度を取るものの、前を責める事を告げると唇を噛み締めた
						PRINTFORMW 「っ……くっ、前は、やめて……っ、後ろならいいですから……っ」
					ENDIF
				;陥落
				ELSEIF CFLAG:6953 == 1
					PRINTFORMW 「はうぅ！　んっ、何だか変な感じです……」
					PRINTFORMW 「あくっ、んっ……もう少し、動いてもいいですよ？」
				;それ以外
				ELSE
					PRINTFORMW 「あくっ！　いやぁ！　やあっ！」
					PRINTFORMW 「そんな所、触らないでっ！」
				ENDIF
			;それ以外
			ELSE
				;深愛＋反発刻印Lv3
				IF CFLAG:6953 == 3
					PRINTFORMW 「あっ、ひゃあっ！　そ、そこ、お尻ですよ？」
					PRINTFORMW 「えっ、挿れるつもり……？　そんな……そんな所やめて下さい」
				;反発刻印Lv3
				ELSEIF CFLAG:6953 == 2
					PRINTFORMW 「やっ、どこ触ってるんですっ！」
					PRINTFORMW 「お尻なんて触らないでっ！」
				;淫尻＋陥落
				ELSEIF TALENT:淫尻 == 1 && CFLAG:6953 == 1
					PRINTFORMW 「ひあっ！　あっ……ああっ……」
					PRINTFORMW 早苗の体はぷるぷると震え、アナルに伝わる微弱な快感に悶えていた
					PRINTFORMW しかし、激しさのない快感では、今の早苗は満足出来なくなっていた
					PRINTFORMW 「おっ……お願い……ですっ……お願いだから、激しく……っ！」
				;陥落
				ELSEIF CFLAG:6953 == 1
					PRINTFORMW 「あっ、くっ、んっ……そんな、所、汚いです……からっ」
					PRINTFORMW 「ひゃん！　だ、駄目っ、駄目ですぅ……」
				;それ以外
				ELSE
					PRINTFORMW 「ふあぁっ、いやぁ！　お尻はいやぁ！」
					PRINTFORMW 菊門の皺をなぞる度に、早苗は体を飛び上がらせた
					PRINTFORMW まだアナルを責められる事を受け入れられないようだ
				ENDIF
			ENDIF
				CFLAG:7004 = 1
		;二回目以降
		ELSE
			LOCAL = PALAM:潤滑 + UP:10
			;深愛＋反発刻印Lv3
			IF CFLAG:6953 == 3
				PRINTFORMW 「ひっ、んっ、あっ！」
				PRINTFORMW 「やっ、やめっ！　くぅ！」
			;反発刻印Lv3
			ELSEIF CFLAG:6953 == 2
				PRINTFORMW 「やっ、お尻はもういやぁ！」
				PRINTFORMW 皺をなぞる度に、早苗は体をくねらせて抵抗した
			;淫尻＋陥落
			ELSEIF TALENT:淫尻 == 1 && CFLAG:6953 == 1
				;深愛
				IF CFLAG:6805 >= 7
					IF RAND:4 == 0
						PRINTFORMW 「はあぁ！　駄目ぇ！」
						PRINTFORMW 「なぞるだけじゃ、足りないですからぁ！」
					ELSEIF RAND:3 == 0
						PRINTFORMW 「やっ、もっと、激しくっ！」
						PRINTFORMW 「バイブでも、何でもいいですからっ！　もっと掻き回してぇ！」
					ELSEIF RAND:2 == 0
						PRINTFORMW 「はっ、んっ、あぁっ……」
						PRINTFORMW 「なぞるだけじゃ、足りないですからぁ……」
					ELSE
						PRINTFORMW 「はーっ、はーっ……もっとして下さい……っ」
					ENDIF
				;それ以外
				ELSE
					IF RAND:4 == 0
						PRINTFORMW 「言う事ちゃんと聞きますっ！　聞きますからっ！」
						PRINTFORMW 「早苗のお尻、もっと弄ってぇ！」
					ELSEIF RAND:3 == 0
						PRINTFORMW 「あくっ！　なぞるだけじゃ！」
						PRINTFORMW 「ふっ、うっ、ああっ！」
					ELSEIF RAND:2 == 0
						PRINTFORMW 「はぁ、っはぁ……ご主人様、焦らさないで……っ」
						PRINTFORMW 「早苗のここ、鎮めて下さい……っ」
					ELSE
						PRINTFORMW 「ひゃうっ！　な、なぞるだけじゃ、足りない……っ」
						SIF TEQUIP:目装着器具 == 0
							PRINTFORMW 早苗は腰を震わせ、%CALLNAME:MASTER%を誘うように熱い視線を送った
					ENDIF
				ENDIF
			;陥落＋A感覚Lv3以上
			ELSEIF CFLAG:6953 == 1 && ABL:Ａ感覚 >= 3
				;深愛
				IF CFLAG:6805 >= 7
					IF RAND:4 == 0
						PRINTFORMW 「ふあぁ、駄目ぇ！　足りないですからぁ！」
						PRINTFORMW 早苗は泣きながら%CALLNAME:MASTER%に嘆願してきた
					ELSEIF RAND:3 == 0
						PRINTFORMW 「くぅ、あぁ……お願いぃ……お願いですからぁ……」
						PRINTFORMW 早苗の体はぷるぷると震え、快楽を求めて喘いだ
					ELSEIF RAND:2 == 0
						PRINTFORMW 「はぁ、っはぁ……早く、中に入ってきて……っ」
						PRINTFORMW 「奥まで、んっ、掻き回してっ」
					ELSE
						PRINTFORMW 「はっ、んっ、くぅ……」
						PRINTFORMW 「もっと、奥まで、来て下さい……っ」
					ENDIF
				;それ以外
				ELSE
					IF RAND:4 == 0
						PRINTFORMW 「ふぅ……はぁ……奥まで、挿れて下さい……っ」
					ELSEIF RAND:3 == 0
						PRINTFORMW 「はあぁ……駄目ぇ、駄目ですぅ……」
						PRINTFORMW 「中に、挿れてぇ……」
					ELSEIF RAND:2 == 0
						PRINTFORMW 「はーっ、はーっ……早く、下さい……っ」
						PRINTFORMW 奥まで責めてもらえるように、早苗は体を震わせておねだりした
					ELSE
						PRINTFORMW 「ご主人様っ……なぞるだけで終わらせないで……っ」
					ENDIF
				ENDIF
			;陥落＋潤滑Lv2以上
			ELSEIF CFLAG:6953 == 1 && LOCAL >= PALAMLV:2
				;深愛
				IF CFLAG:6805 >= 7
					PRINTFORMW 「はぁ、っはぁ……準備、出来ましたから……っ」
					PRINTFORMW 「早く、中に挿れて下さい……っ！」
				;それ以外
				ELSE
					PRINTFORMW 「うぐっ……もう十分濡れてますから……っ」
					PRINTFORMW 「なぞらないで……挿れて……っ」
