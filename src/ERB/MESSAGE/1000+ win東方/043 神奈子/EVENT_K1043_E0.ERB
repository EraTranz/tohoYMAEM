;========================================================
@KOJO_1043_MESSAGE_SUB_EVENT_BEFORE
;眠ってる場合は戻る
SIF EQUIP:睡眠薬
	RETURN 1
;調教中かつ口枷がある場合は戻る
SIF TFLAG:999 && TEQUIP:口装着器具
	CALL K1043_GAG
;調教中かつ失神中・失神回復時は戻る
SIF TFLAG:999 && (TFLAG:899 >= 1 || TFLAG:898 == 1)
	RETURN 1
;精神崩壊の場合は戻る
SIF TALENT:精神崩壊
	RETURN 1

