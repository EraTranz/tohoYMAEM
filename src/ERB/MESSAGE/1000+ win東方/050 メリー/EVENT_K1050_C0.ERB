@KOJO_1050_TRAIN_MESSAGE_S_COM_0
;初回実行時
IF TFLAG:95 == 1
	;助手蓮子
	IF ASSI > 0 && ASSIPLAY
		IF NO:ASSI == 1049
			;屈服刻印Lv2以上
			IF MARK:屈服刻印 >= 2
				PRINTFORMW 『メリー……んっ……』
				PRINTFORMW 「れ、蓮子……んっ、んんんっ！ ……ぷはっ、ぁ、はぁ……」
