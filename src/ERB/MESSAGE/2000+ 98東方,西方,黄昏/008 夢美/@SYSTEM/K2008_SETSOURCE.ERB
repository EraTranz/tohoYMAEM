@K2008_SETSOURCE
#DIM _COUNT
;失神中は気がつかないのでOK
FOR _COUNT, 0, VARSIZE("SOURCE")
	SIF SOURCE:_COUNT
		SETBIT CFLAG:__K2008N_CFLAG_SOURCEHISTORY__, _COUNT
NEXT
RETURN 0
