;YM→YMAEコンバータによって作成されました
;-------------------------------------------------
;アナルおねだり
;CFLAG:__K2008N_CFLAG_SELECTCOM__を使用可
;-------------------------------------------------

;-------------------------------------------------
;RANK = 5
;-------------------------------------------------
@K2008_RANK5_COM_45
;前コマンドで分岐
SELECTCASE PREVCOM
	CASE 2, 180, 181, 187
		;アナル愛撫、アナル舐め、アナル綿棒、顔面騎乗アナル
		PRINTFORMW 「もっとぉ……　もっと奥までかき回してよ」
		PRINTFORMW 「アナルがせつなくてヒクヒクしちゃうんだから」
	CASE 30, 31, 32, 33, 34
		;アナルセックス系
		PRINTFORMW 「お尻の穴に、もーっとズポズポするのも、素敵よね」
		PRINTFORMW 「え？　入れる前にお腹を息んで開いてみろ？　う～ん……」
		IF K2008F_SAMENCOUNT("GET", 2) >= 10
			CALL K2008_SE("液便", 2)
			PRINTFORMW 「あぁん、注いでもらったのが漏れちゃった」
			IF RAND:2 > 0 && (K2008F_DEVRANK("排泄") >= 2 || K2008F_CFLAGCOM("空気浣腸") > 5)
				PRINTFORMW 「何だか、もっと、いっぱい漏れちゃいそうなんだけど」
				PRINTFORMW 「……とほほ、ああもう、どうなってもしらないんだから！　ンンーーッ！！」
				CALLFORM K2008_SE(K2008FS_RANDS("放屁", "強放屁", "液便"))
			ENDIF
		ELSE
			PRINTFORMW 「ンッ……　こ、こう？」
			PRINTFORMW 「もっとって、ん、ふーー、んッ、ンーーッ！」
			PRINTFORMW 力いっぱいに尻口を開いて、直腸をさらけ出そうと健気にがんばっている
		ENDIF
	CASE 45
		;アナルおねだり
		PRINTFORMW 「んんっ、おねだり足りなかったかしら」
		SIF RAND:2 > 0 && EXP:Ａ拡張経験 > EXPLV:4
			PRINTFORMW 「ほーら、お尻の穴がこんなに開いちゃうの」
		SIF RAND:2 > 0 && EXP:Ａ経験 < EXPLV:3 && EXP:Ａ快楽経験 < EXPLV:3
			PRINTFORMW 「けど……　お尻の穴なんて本当にいいの？」
	CASE 174 * K2008F_CASEIF(TEQUIP:Ａ系装着器具 == 0)
		;浣腸（取り外し後のみ）
		PRINTFORML 「ほらほらー、お尻の中も綺麗になったわよ」
		PRINTFORMW 「次は何を入れてくれるのかしら？」
		SIF K2008F_DEVRANK("排泄") >= 3
			PRINTFORMW 「あ、またお浣腸でもいいわよ。いっぱい出すから、いっぱい入れてね！」
	CASE 175 * K2008F_CASEIF(TEQUIP:Ａ系装着器具 == 0)
		;空気浣腸（取り外し後のみ）
		PRINTFORML 「ほらほら、お尻の穴がお留守になったから次の玩具いれてよ」
		PRINTFORMW 「早く栓してくれないと……　ああ゛ぁ゛！」
		CALL K2008_SE("強放屁", 2)
		PRINTFORMW 「……んふふ、弄ってくれないと、また出ちゃうかもしれないわよ？」
	CASEELSE
		CALL K2008_COM_GENERAL
ENDSELECT
