;YM→YMAEコンバータによって作成されました
;========================================================
;MARKのLV○を取得した時の口上
;-------------------------------------------------
;■引数
;ARG
;	取得直前でのMARKのLv（実体はCFLAG:調教説教薀蓄受講数xxです）
;	★必ず使わなくてもARGは宣言すること。
;-------------------------------------------------
;■備考
;	CFLAG:調教説教薀蓄受講数xxの操作はシステム側で行うので不要です。
;	過去に取得→消去を行っていても再度呼び出されます。
;========================================================

;=================================================
;苦痛刻印Lv1
;=================================================
@K2008_MARK0_LV1(ARG)
IF K2008F_RANK() <= 2
	PRINTFORMW 夢美の心に、痛みへの恐怖心が少しだけ刻まれたようだ
ELSE
	PRINTFORMW 夢美の心に、痛みへの好奇心が少しだけ刻まれたようだ
ENDIF
PRINTFORML 

;-------------------------------------------------
;RANK = 0
;-------------------------------------------------
@K2008_RANK0_MARK0_LV1(ARG)
PRINTFORMW 「イッ、痛い痛い！　お願いやめてっ、ンギィ！？」
PRINTFORMW 「するっ、するから！　大人しくするから許して！」
CALL K2008_MARK0_LV1, ARG

;-------------------------------------------------
;RANK = 1
;-------------------------------------------------
@K2008_RANK1_MARK0_LV1(ARG)
CALL K2008_RANK0_MARK0_LV1, ARG


;=================================================
;苦痛刻印Lv2
;=================================================
@K2008_MARK0_LV2(ARG)
IF K2008F_RANK() <= 2
	PRINTFORMW 夢美の心に、苦痛への逃避心が刻み込まれたようだ
ELSE
	PRINTFORMW 夢美の心に、苦痛への好奇心が刷り込まれたようだ
ENDIF
PRINTFORML 

;-------------------------------------------------
;RANK = 0
;-------------------------------------------------
@K2008_RANK0_MARK0_LV2(ARG)
;快楽がそこそこ伴う
IF K2008F_HOWMANY_SOURCE_PLEASURE("GETNOW") >= 3
	PRINTFORMW 「お願い、ぐすっ、お願い……します、優しく、してください」
	PRINTFORMW 「ううぅ……　はい、これなら、なんとか……　ンッ、素直じゃないって、何がッ、ンヒィ！？」
	PRINTFORMW 「言う、言うからストップ！　い、痛くしないで、お願い！」
	PRINTFORMW 
	PRINTFORMW 「痛いの無ければ、き、気持ちよかったわ……　素直になるから、痛いのやめて……」
ELSE
	PRINTFORMW 「こ、このぉ！　これ以上好き勝手にはさせッ、ぁぁああアア゛！？」
	PRINTFORMW 「壊れる壊れる！　お願い止めて！　やめてぇええ！！」
	PRINTFORMW 
	PRINTFORMW 「はっ、アッ、はっ、ひっ、やだ、やだヤダ、ねえお願い！」
	PRINTFORMW 「なる、なるから！　大人しく従うから……」
ENDIF
CALL K2008_MARK0_LV2, ARG

;-------------------------------------------------
;RANK = 1
;-------------------------------------------------
@K2008_RANK1_MARK0_LV2(ARG)
CALL K2008_RANK0_MARK0_LV2, ARG

;=================================================
;苦痛刻印Lv3
;=================================================
@K2008_MARK0_LV3(ARG)
PRINTFORMW 夢美の深層意識にあった芯が一本、ポキリと折れてしまったようだ
IF K2008F_RANK() <= 2
	PRINTFORMW 形容しがたい苦痛からの逃避衝動が、夢美に服従心を深く刻み込んだ
ELSE
	PRINTFORMW 形容しがたい苦痛を受けたことで、%CALLNAME:MASTER%への服従心が刷り込まれたようだ
ENDIF
;PRINTFORMW 形容しがたい苦痛からの逃避衝動が、夢美に服従心を深く刻み込んだ
PRINTFORML 

;-------------------------------------------------
;RANK = 0
;-------------------------------------------------
@K2008_RANK0_MARK0_LV3(ARG)
;快楽がそこそこ伴う
IF K2008F_HOWMANY_SOURCE_PLEASURE("GETNOW") >= 3
	PRINTFORMW 「あア゛、ンギィッ！！　あ゛ァ゛ーッ、ひゅー、んあぁはぁ、はっ、は、はーっ」
	PRINTFORMW 「あ゛ー、ア゛ァぁ……　た、助けて、死ぬっ、これ以上されたら死んじゃ、ヴゥッ」
	PRINTFORMW 「イ、痛いの以外なら何してもいいから、何でもするからぁ！」
	PRINTFORMW 
	PRINTFORMW 「はっはいぃ！　気持ちいいです、いまとっても気持ちいいです！」
	PRINTFORMW 「だらしない顔、します！　スケベな声いっぱい出しますから！」
	PRINTFORMW 「い、痛いのだけは許してッ、許してください、お願いします……」
ELSE
	PRINTFORMW 「こ、このぉ！　こんなの許すとでも思っ、ぁぁああアア゛！？」
	PRINTFORMW 「しぬぅ、死んじゃう！　お願い、やめてぇええ！！」
	PRINTFORMW 
	PRINTFORMW 「ごめんなさいごめんなさい……　生意気言ってごめんなさい……」
	PRINTFORMW 「え、許してくれるの？　あぁ、ありがとうございま、スウヴ！？」
	PRINTFORMW 「やだやだ、やめて！　死んじゃうから！　本当にこれ以上は死んじゃうって、あヴァ！？」
	CALL K2008_VOICE("激痛")
ENDIF
CALL K2008_MARK0_LV3, ARG

;-------------------------------------------------
;RANK = 1
;-------------------------------------------------
@K2008_RANK1_MARK0_LV3(ARG)
CALL K2008_RANK0_MARK0_LV3, ARG

