;YM→YMAEコンバータによって作成されました
;================================================
;PALAMがLV○に到達した時の口上
;------------------------------------------------
;■引数
;ARG
;	これまでの調教で上がった最大のPALAMLV
;	★必ず使わなくてもARGは宣言すること。
;------------------------------------------------
;■RETURN時の注意
;	RETURN 0	：　以後のPALAMLV到達口上も表示させる
;	RETURN 1	：　以後のPALAMLV到達口上は表示しない（RETURNしない場合のデフォルト動作）
;------------------------------------------------
;■備考
;	PALAMLVに到達するたびに何度でも呼び出されます。
;================================================

;=================================================
;潤滑がLV1に到達した
;=================================================
;-------------------------------------------------
;RANK = 0
;-------------------------------------------------
@K2008_RANK0_PALAM10_LV1(ARG)
SIF ARG >= 1
	RETURN 0
PRINTFORML 
PRINTFORMW ――恥部が湿り始める感覚に戸惑っているようだ。
SELECTCASE SELECTCOM
	CASE 0, 1, 5, 126, 180, 187, 331, 335, 330, 380, 602, 603, 606, 611
		;舐める系、数が多いので番号決め打ち
		PRINTFORMW 「やぁ……なにこれ、舐めまわすから唾液でベトベト……」
	CASEELSE
		PRINTFORMW （あ、あれ？　私お漏らしして……ないよね？　なんで濡れてるんだろう？）
ENDSELECT

;-------------------------------------------------
;RANK = 1
;-------------------------------------------------
@K2008_RANK1_PALAM10_LV1(ARG)
CALL K2008_RANK0_PALAM10_LV1, ARG


;=================================================
;潤滑がLV2に到達した
;=================================================
;-------------------------------------------------
;RANK = 0
;-------------------------------------------------
@K2008_RANK0_PALAM10_LV2(ARG)
SIF ARG >= 2
	RETURN 0
PRINTFORML 
PRINTFORMW ――恥部が湿っていく感覚に戸惑っているようだ。
SELECTCASE SELECTCOM
	CASE 0, 1, 5, 126, 180, 187, 331, 335, 330, 380, 602, 603, 606, 611
		;舐める系、数が多いので番号決め打ち
		PRINTFORMW 「うぅ、少し舐めすぎ……　唾液がたれてきちゃってるし」
	CASEELSE
		PRINTFORMW （ぬ、ぬるぬるが増えてる。私どうなっちゃってるの……）
ENDSELECT

;-------------------------------------------------
;RANK = 1
;-------------------------------------------------
@K2008_RANK1_PALAM10_LV2(ARG)
CALL K2008_RANK0_PALAM10_LV2, ARG



;=================================================
;潤滑がLV3に到達した
;=================================================
;-------------------------------------------------
;RANK = 0
;-------------------------------------------------
@K2008_RANK0_PALAM10_LV3(ARG)
SIF ARG >= 3
	RETURN 0
;初手ローションの場合は一発でここに来る
IF SELECTCOM == K2008F_GETNUM_TRAIN("ローション")
	PRINTFORML 
	PRINTFORMW ――ローションを垂らすと肌に沿って垂れ広がっていく。
	PRINTFORMW 「イヤぁ……　ヌルヌルでベチョベチョで気持ち悪い……」
;順当に上がってきた（ペロペロ唾液だけじゃここまでは普通いかない）
ELSEIF ARG == 2
	PRINTFORML 
	PRINTFORMW ――恥部周りは生温かい体液でびしょびしょになりつつある。
	PRINTFORMW 「こ、これはただの生体反応なのよ」
	PRINTFORMW 「そう、汗をかくのと同じで、こんな風に濡れてもおかしくないわ……きっと」
ENDIF

;-------------------------------------------------
;RANK = 1
;-------------------------------------------------
@K2008_RANK1_PALAM10_LV3(ARG)
CALL K2008_RANK0_PALAM10_LV3, ARG

@K2008_RANK2_PALAM10_LV3(ARG)
SIF ARG >= 3
	RETURN 0
;ローションの場合
IF SELECTCOM == K2008F_GETNUM_TRAIN("ローション")
	PRINTFORMW 「ひゃぁ！？　冷たいじゃない！」
	PRINTFORMW 「……人肌に暖めておいてくれると良かったんだけどなぁ」
;順当に上がってきた（ペロペロ唾液だけじゃここまでは普通いかない）
ELSEIF ARG == 2
	PRINTFORML 
	PRINTFORMW ――恥部周りは生温かい体液でびしょびしょになりつつある。
	PRINTFORMW 「こ、これはただの生体反応なのよ」
	PRINTFORMW 「そう、汗をかくのと同じで、こんな風に濡れてもおかしくないわ……きっと」
ENDIF

;=================================================
;潤滑がLV4に到達した
;=================================================
;-------------------------------------------------
;RANK = 0
;-------------------------------------------------
@K2008_RANK0_PALAM10_LV4(ARG)
;LV5に到達するまでは毎回表示
SIF ARG > 4
	RETURN 0
;快Ｖか快Ａが増加している（何か入ってる／弄っている）
IF UP:1 > 0 || UP:2 > 0
	PRINTFORML 
	PRINTFORMW ――小さく動くたびに愛液が糸を引きながら増えていく。
	PRINTFORMW 「な、中からトロトロが溢れてっ　んっ、止まらないわ」
;ローション
ELSEIF SELECTCOM == K2008F_GETNUM_TRAIN("ローション")
	PRINTFORML 
	PRINTFORMW 「ちょっと、いくらなんでも掛け過ぎよ……」
;それ以外
ELSE
	PRINTFORML 
	PRINTFORMW ――大陰唇の内側は愛液をたたえてぬらぬらとしている。
	PRINTFORMW 「ち、違うわ！　濡れやすいなんて違うの！！」
ENDIF

;-------------------------------------------------
;RANK = 1
;-------------------------------------------------
@K2008_RANK1_PALAM10_LV4(ARG)
CALL K2008_RANK0_PALAM10_LV4, ARG

;-------------------------------------------------
;RANK = 2
;-------------------------------------------------
@K2008_RANK2_PALAM10_LV4(ARG)
;1回だけ（ローションの場合も表示しない）
SIF ARG >= 4 || SELECTCOM == K2008F_GETNUM_TRAIN("ローション")
	RETURN 0
PRINTFORML 
PRINTFORMW ――大陰唇の内側は愛液をたたえてぬらぬらとしている。
PRINTFORMW 「トロトロがもう止まらないの……　疼いちゃったみたい」


