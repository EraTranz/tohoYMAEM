;YM→YMAEコンバータによって作成されました
;================================================
;PALAMがLV○に到達した時の口上
;------------------------------------------------
;■引数
;ARG
;	これまでの調教で上がった最大のPALAMLV
;	★必ず使わなくてもARGは宣言すること。
;------------------------------------------------
;■RETURN時の注意
;	RETURN 0	：　以後のPALAMLV到達口上も表示させる
;	RETURN 1	：　以後のPALAMLV到達口上は表示しない（RETURNしない場合のデフォルト動作）
;------------------------------------------------
;■備考
;	PALAMLVに到達するたびに何度でも呼び出されます。
;================================================

;=================================================
;快AがLV1に到達した
;=================================================
;-------------------------------------------------
;RANK = 0
;-------------------------------------------------
@K2008_RANK0_PALAM2_LV1(ARG)
SIF ARG >= 1
	RETURN 0
SIF ABL:Ａ感覚 >= 2
	RETURN 0

PRINTFORML 
SELECTCASE SELECTCOM
	;Aを弄る系
	CASE 2, 180, 181, 184, 187
		PRINTFORMW 「へ、変なところばかり弄るのが好きなのね」
		PRINTFORMW 「……気持ちいいわけないじゃない。頭は大丈夫？」
	;Aセックス、Aバイブ等挿入
	CASE 13, 30, 31, 32, 33, 34, 37, 38, 170, 171, 172, 173, 174, 175, 179, 232, 233, 235, 313, 433, 436, 438, 439, 442
		PRINTFORMW 「は、早く抜きなさい！　気持ち悪いのよ！」
ENDSELECT

;-------------------------------------------------
;RANK = 1
;-------------------------------------------------
@K2008_RANK1_PALAM2_LV1(ARG)
CALL K2008_RANK0_PALAM2_LV1, ARG

;-------------------------------------------------
;RANK = 2
;-------------------------------------------------
@K2008_RANK2_PALAM2_LV1(ARG)
CALL K2008_RANK0_PALAM2_LV1, ARG



;=================================================
;快AがLV2に到達した
;=================================================
;-------------------------------------------------
;RANK = 0
;-------------------------------------------------
@K2008_RANK0_PALAM2_LV2(ARG)
SIF ARG >= 2
	RETURN 0
SIF ABL:Ａ感覚 >= 2
	RETURN 0
PRINTFORML 
SELECTCASE SELECTCOM
	;Aを弄る系
	CASE 2, 180, 181, 184, 187
		PRINTFORMW 「な、なに？　……いい加減そんなところ弄るのやめてよ」
	;Aセックス、Aバイブ等挿入
	CASE 13, 30, 31, 32, 33, 34, 37, 38, 170, 171, 172, 173, 174, 175, 179, 232, 233, 235, 313, 433, 436, 438, 439, 442
		PRINTFORMW 「んぎッ　は、早く抜いて！　奥に入れすぎよ！！」
ENDSELECT

;-------------------------------------------------
;RANK = 1
;-------------------------------------------------
@K2008_RANK1_PALAM2_LV2(ARG)
CALL K2008_RANK0_PALAM2_LV2, ARG

;-------------------------------------------------
;RANK = 2
;-------------------------------------------------
@K2008_RANK2_PALAM2_LV2(ARG)
CALL K2008_RANK0_PALAM2_LV2, ARG


;=================================================
;快AがLV3に到達した
;=================================================
;-------------------------------------------------
;RANK = 0
;-------------------------------------------------
@K2008_RANK0_PALAM2_LV3(ARG)
SIF NOWEX:Ａ絶頂 > 0
	RETURN 0
SIF ARG >= 3
	RETURN 0
PRINTFORML 
SELECTCASE ARG
	CASE 0, 1
		PRINTFORMW 「ンッ！？　い、今のなに！？　私の体どうなっちゃったの……」
	CASE 2
		PRINTFORMW 「んあっ！？　……まさか、か、開発されちゃった……の？」
		PRINTFORMW 「お尻で気持ちよくなるなんてやだぁ……」
ENDSELECT

;-------------------------------------------------
;RANK = 1
;-------------------------------------------------
@K2008_RANK1_PALAM2_LV3(ARG)
CALL K2008_RANK0_PALAM2_LV3, ARG

;-------------------------------------------------
;RANK = 2
;-------------------------------------------------
@K2008_RANK2_PALAM2_LV3(ARG)
CALL K2008_RANK0_PALAM2_LV3, ARG

;-------------------------------------------------
;RANK = 3
;-------------------------------------------------
@K2008_RANK3_PALAM2_LV3(ARG)
SIF ARG >= 3
	RETURN 0
PRINTFORML 
PRINTFORMW 「あっ、ぁああ！！　お尻なのにき、気持ちいい！？」
PRINTFORMW 「だめっ、ゾクゾクして感じちゃうッ！！」
