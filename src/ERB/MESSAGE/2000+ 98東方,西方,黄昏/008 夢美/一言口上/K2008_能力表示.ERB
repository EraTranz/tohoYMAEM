@K2008_ONEWORD_SHOW_STATUS_0
SIF __K2008N_CAN_CALL__
	RETURN 1
PRINTFORMW テンプレ


;-------------------------------------------------
;特に指定なし
;-------------------------------------------------
@K2008_ONEWORD_SHOW_STATUS_1
SIF __K2008N_CAN_CALL__
	RETURN 1
SELECTCASE K2008F_RANK()
	CASE 0 TO 1
		PRINTDATAL
			DATA 　　　「えーと、アレをこうして……　あらら？　材料が足りないだなんて」
		ENDDATA
	CASE 2 TO 3
		PRINTDATAL
			DATA 　　　「ねえ、なんで私なんか買ったの？」
		ENDDATA
	CASE 4 TO 5
		PRINTDATAL
			DATA 　　　「いちど吹っ切れちゃうと、こんな生活も悪くないわね」
		ENDDATA
ENDSELECT


