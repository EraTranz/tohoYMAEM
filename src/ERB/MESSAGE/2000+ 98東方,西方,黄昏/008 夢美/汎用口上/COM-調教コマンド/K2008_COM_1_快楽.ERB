;YM→YMAEコンバータによって作成されました
;========================================================
;（専用がないという意味で）汎用コマンド口上
;========================================================

;-------------------------------------------------
;快楽系
;ARG:0	= 快楽系の評価値
;ARG:1	= 好感系の評価値
;ARG:2	= 拒絶系の評価値
;ARG:3	= 苦痛系の評価値
;-------------------------------------------------

;-------------------------------------------------
;RANK = 0
;-------------------------------------------------
@K2008_RANK0_COM_GENERAL_PLEASURE(ARG:0, ARG:1, ARG:2, ARG:3)

;快楽＞拒絶＋苦痛の場合
IF ARG:0 > ARG:2 + ARG:3
	;快楽限界超過（淫乱で判定値+1、淫魔で+1、即落ちで-1）
	IF ARG:0 >= 5 + (TALENT:淫乱 > 0) + (TALENT:淫魔 > 0) - (TALENT:即落ち > 0)
		SELECTCASE RAND:3
			CASE 2
				PRINTFORMW 「あひっ、んくっ、いひぃ、いひよぉ……　んぁっ、ぁ、ぁあっ！」
			CASE 1
				PRINTFORMW 「き、気持ちよくなんか、こんなのっ、こんなのぉおおお」
			CASEELSE
				PRINTFORMW 「んはっ、はっ、はひっ、ひぁっ、ひゃぁああ！」
		ENDSELECT
	;快楽が大きめ
	ELSEIF ARG:0 >= 3
		SELECTCASE RAND:4
			CASE 3
				PRINTFORMW 「んあぁ、んっ、ンッ！　はふぅ……　じろじろ見ないでよ」
			CASE 2
				PRINTFORMW 「ふぁ、ん、んぅっ……　ッ、こっち見ないでよ！！」
			CASE 1
				PRINTFORMW 「あんっ、ん、んくっ……　な、なによ」
			CASEELSE
				PRINTFORMW 「こんなのっ……　何よ！」
		ENDSELECT
	ELSE
		SELECTCASE RAND:4
			CASE 3
				PRINTFORMW 「まったく、こんなの不愉快なだけだわ」
			CASE 2
				PRINTFORMW 「ぜんぜん駄目ね。気持ちいいなんて理解できないわ」
			CASE 1
				PRINTFORMW 「何よ、いまさら快楽で堕そうっていうの？」
			CASEELSE
				PRINTFORMW 「はしたなく喘ぐ娘が好きなら他所をあたりなさい」
		ENDSELECT
	ENDIF

;拒絶／苦痛が大きな快楽の場合
ELSE
	SELECTCASE RAND:3
		CASE 2
			PRINTFORMW 「痛いのが気持ちいいかって？」
			PRINTFORMW 「大丈夫？　ついに頭にウジでも湧いちゃったの？」
		CASE 1
			PRINTFORMW 「気に入らないとすぐこういう手段に出る。芸がない上に無様ね」
		CASEELSE
			PRINTFORMW 「一度自分に同じことをしてみたら？　きっと新天地が見えるわよ」
	ENDSELECT
ENDIF

