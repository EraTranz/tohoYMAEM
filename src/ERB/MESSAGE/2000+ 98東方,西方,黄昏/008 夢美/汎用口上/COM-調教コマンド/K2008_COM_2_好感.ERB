;YM→YMAEコンバータによって作成されました
;========================================================
;（専用がないという意味で）汎用コマンド口上
;========================================================

;-------------------------------------------------
;好感系
;ARG:0	= 快楽系の評価値
;ARG:1	= 好感系の評価値
;ARG:2	= 拒絶系の評価値
;ARG:3	= 苦痛系の評価値
;-------------------------------------------------

;-------------------------------------------------
;RANK = 0
;-------------------------------------------------
@K2008_RANK0_COM_GENERAL_FAVOR(ARG:0, ARG:1, ARG:2, ARG:3)

;快楽／苦痛を伴わない場合
IF ARG:0 == 0 && ARG:3 == 0
	SELECTCASE RAND:3
		CASE 2
			PRINTFORMW 「何よ、私に何かさせたいの？」
		CASE 1
			PRINTFORMW 「時間の無駄だわ。さっさと出て行って、私に一人の時間を頂戴」
		CASEELSE
			PRINTFORMW 「さっさと飽きて帰って頂戴。面倒くさいんだから」
	ENDSELECT
;快楽≧拒絶および快楽≧苦痛の場合
ELSEIF ARG:0 >= ARG:2 && ARG:0 >= ARG:3
	SELECTCASE RAND:3
		CASE 2
			PRINTFORMW 「なによ。いっぱしのことはできるんじゃない」
		CASE 1
			PRINTFORMW 「いまさら何の真似よ。改心でもしたの？」
		CASEELSE
			PRINTFORMW 「どこが良いんだかさっぱりだわ」
	ENDSELECT
;上記以外
ELSE
	SELECTCASE RAND:2
		CASE 1
			PRINTFORMW 「今度やったら苺クロスでボコボコにしてあげるわ。楽しみにしてなさい。」
		CASEELSE
			PRINTFORMW 「窮してこんな真似しかできなくなったのね。哀れむ気にもなれないわ」
	ENDSELECT
ENDIF

;-------------------------------------------------
;RANK = 1
;-------------------------------------------------
@K2008_RANK1_COM_GENERAL_FAVOR(ARG:0, ARG:1, ARG:2, ARG:3)

;快楽／苦痛を伴わない場合
IF ARG:0 == 0 && ARG:3 == 0
	SELECTCASE RAND:7
		CASE 6
			PRINTFORMW 「優しく扱ってれば大人しくなるとでも？　甘いわね」
		CASE 5
			PRINTFORMW 「ふふ、こんな事が効果的とか勘違いしてないかしら？」
		CASE 4
			PRINTFORMW （……何の狙いがあるのかしら、よく分からないわね）
		CASE 3
			PRINTFORMW 「あんまり慣れ慣れしくしないで……」
		CASE 2
			PRINTFORMW 「ふんっ、いまさらこれで懐柔しようって魂胆かしら」
		CASE 1
			PRINTFORMW 「見くびらないでよね。こんなのどうってことないわ」
		CASEELSE
			PRINTFORMW 「お人形さん遊びがしたければ他所に行ってね」
	ENDSELECT
;快楽＞拒絶および快楽＞苦痛の場合
ELSEIF ARG:0 > ARG:2 && ARG:0 > ARG:3
	SELECTCASE RAND:3
		CASE 2
			PRINTFORMW 「素敵、とはこの程度じゃ流石に言えないわね」
		CASE 1
			PRINTFORMW 「はいはい、大人しくしてればいいんでしょ」
		CASEELSE
			PRINTFORMW 「こんな真似しても服従なんかしないわよ」
	ENDSELECT
;上記以外
ELSE
	SELECTCASE RAND:3
		CASE 2
			PRINTFORMW 「窮鼠猫を……　って理解できる？」
		CASE 1
			PRINTFORMW 「いい加減にしなさい。後が怖いわよ」
		CASEELSE
			PRINTFORMW 「くぅっ、こんなの何がいいのよ」
	ENDSELECT
ENDIF

