;YM→YMAEコンバータによって作成されました
;========================================================
;（専用がないという意味で）汎用コマンド口上
;その２　ボールギャグ／猿轡口上。
;========================================================

;=================================================
;快楽系
;ARG:0	= 快楽系の評価値
;ARG:1	= 好感系の評価値
;ARG:2	= 拒絶系の評価値
;ARG:3	= 苦痛系の評価値
;=================================================

;-------------------------------------------------
;RANK = 0
;-------------------------------------------------
@K2008_RANK0_COM_GENERAL_WITH_GAG1(ARG:0, ARG:1, ARG:2, ARG:3)

SELECTCASE RAND:2
	CASE 1
		PRINTFORMW 口がつかえないので頭を右に左に振って、拒絶を表現しているつもりらしい。
	CASEELSE
		PRINTFORMW 嬌声を押し殺そうとしているのか、やけに口が力んでいる。
		PRINTFORMW そのせいで口周りが涎まみれだ。
ENDSELECT

;-------------------------------------------------
;RANK = 1
;-------------------------------------------------
@K2008_RANK1_COM_GENERAL_WITH_GAG1(ARG:0, ARG:1, ARG:2, ARG:3)

CALL K2008_RANK0_COM_GENERAL_WITH_GAG1(ARG:0, ARG:1, ARG:2, ARG:3)

;-------------------------------------------------
;RANK = 2
;-------------------------------------------------
@K2008_RANK2_COM_GENERAL_WITH_GAG1(ARG:0, ARG:1, ARG:2, ARG:3)

CALL K2008_RANK0_COM_GENERAL_WITH_GAG1(ARG:0, ARG:1, ARG:2, ARG:3)

;-------------------------------------------------
;RANK = 3
;-------------------------------------------------
@K2008_RANK3_COM_GENERAL_WITH_GAG1(ARG:0, ARG:1, ARG:2, ARG:3)

CALL K2008_RANK0_COM_GENERAL_WITH_GAG1(ARG:0, ARG:1, ARG:2, ARG:3)

;-------------------------------------------------
;RANK = 4
;-------------------------------------------------
@K2008_RANK4_COM_GENERAL_WITH_GAG1(ARG:0, ARG:1, ARG:2, ARG:3)

SELECTCASE RAND:2
	CASE 1
		PRINTFORMW 「ふぇあ♪　あふェ、ふぁア゛～♪」
	CASEELSE
		PRINTFORMW 「ア゛♪　ふーっフーッ、ふぇあアァ♪　いひぃ、いひぃふぉ」
ENDSELECT

;-------------------------------------------------
;RANK = 5
;-------------------------------------------------
@K2008_RANK5_COM_GENERAL_WITH_GAG1(ARG:0, ARG:1, ARG:2, ARG:3)

CALL K2008_RANK4_COM_GENERAL_WITH_GAG1(ARG:0, ARG:1, ARG:2, ARG:3)
