;YM→YMAEコンバータによって作成されました
;-------------------------------------------------
;アナルに射精
;-------------------------------------------------

;-------------------------------------------------
;RANK = 0
;-------------------------------------------------
@K2008_RANK0_EJAC_GENERAL_12
SELECTCASE RAND:3
	CASE 2
		PRINTFORMW 「満足した？　そう、じゃあさっさと抜いてよ」
		PRINTFORMW 「気持ち悪いこと、この上ないわ」
	CASE 1
		PRINTFORMW 「だ、出された……　お尻なんかに吐き出されるなんて……」
	CASEELSE
		PRINTFORMW 「このっ、汚いもの出さないでよ！」
ENDSELECT

;-------------------------------------------------
;RANK = 1
;-------------------------------------------------
@K2008_RANK1_EJAC_GENERAL_12
;RANK0に丸投げ
CALL K2008_RANK0_EJAC_GENERAL_12

;-------------------------------------------------
;RANK = 2
;-------------------------------------------------
@K2008_RANK2_EJAC_GENERAL_12
;RANK0に丸投げ
CALL K2008_RANK0_EJAC_GENERAL_12

;-------------------------------------------------
;RANK = 3
;-------------------------------------------------
@K2008_RANK3_EJAC_GENERAL_12
SELECTCASE RAND:3
	CASE 2
		PRINTFORMW 「ひあっ！？　根元まで入って、ンーッ！」
		PRINTFORMW 「ふ、ふーっ、ふー……　まだ出てる、ちょっと出しすぎよ」
	CASE 1
		PRINTFORMW 「んっ、ビュルビュルって出てる……　気持ちよかった？」
	CASEELSE
		PRINTFORMW 「出ちゃいそう？　いいわ、締めてあげる」
		PRINTFORMW 「それっ、んーーッ！　で、出てるぅ」
ENDSELECT

;-------------------------------------------------
;RANK = 4
;-------------------------------------------------
@K2008_RANK4_EJAC_GENERAL_12
SELECTCASE RAND:4
	CASE 3
		PRINTFORML 「あはッ、出てる出てる、おしり気に入ってくれた？　ん、嬉しい♪」
	CASE 2
		PRINTFORMW 「……あ、出ちゃう？　出しちゃうの？　お尻で孕ませちゃうんだ？」
		PRINTFORMW 「んひっ、お、奥に出さないとッ！　お仕置きだからっ！　ンンーーー！」
	CASE 1
		PRINTFORMW 「もっとっ、もっとビューって出してェ」
	CASEELSE
		PRINTFORMW 「んぁああっ、出てるッ　おしりに熱っついのがぁ……」
ENDSELECT

;-------------------------------------------------
;RANK = 5
;-------------------------------------------------
@K2008_RANK5_EJAC_GENERAL_12
IF NOWEX:Ａ絶頂 == 0
	;A絶頂なしはRANK4に丸投げ
	CALL K2008_RANK4_EJAC_GENERAL_12
ELSE
	SELECTCASE RAND:4
		CASE 3
			PRINTFORMW 「んぎぃッ、根元まで押し込んでぇ♪　ぶぴゅーってぇ」
			PRINTFORMW 「あ゛ァア゛、ケツハメだいしゅきぃ♪　もっとだひてぇええ！」
		CASE 2
			PRINTFORML 「あひっ、んほぉオヲー♪　どぷどぷ音たてて腸内射精されて！」
			PRINTFORMW 「ザーメン叩き込まれてっ、またイクっ、イッちゃう！」
		CASE 1
			PRINTFORML 「ンぉおオ！　ブりゅブりゅ精子流しこまてれりゅ！」
			PRINTFORMW 「ザーメン浣腸で直腸の奥まで蹂躙されたぁ♪」
		CASEELSE
			PRINTFORMW 「んぁああっ、出てるッ　%K2008FS_CALL_MY_A()%に熱っついのがぁ」
	ENDSELECT
ENDIF


;排卵誘発剤
IF TEQUIP:排卵誘発剤
	SELECTCASE RAND:2
		CASE 1
			PRINTFORMW 「お尻に種付けされちゃった。　次は”こっち”で孕ませて♪」
		CASEELSE
			PRINTFORMW 「けど、こっちじゃ孕めないわよ……　好きだからどっちでもいいけどね」
	ENDSELECT
ENDIF
