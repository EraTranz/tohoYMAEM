@K2008_RANK4_SP_ORGASM_GENERAL_11
SELECTCASE RAND:2
	CASE 1
		PRINTFORMW 「で、出る！　%K2008FS_CALL_MY_C()%から出るぅッ！！　－－－ッ！！」
	CASEELSE
		PRINTFORMW 「んっ、ンーーッ！！　ぁ、ぁひっ♪　ブピューって！　ザーメン出てりゅぅ♪」
ENDSELECT
;絶頂ランクが高い場合
IF K2008F_ORGASMRANK() >= 2
	SELECTCASE RAND:3
		CASE 2 * K2008F_CASEIF(TALENT:ふたなり == 2)
			PRINTFORMW 「タマタマっ、空っぽにならないでぇ♪　もっと出すからぁあ！！」
		CASE 1
			PRINTFORMW 「あひぃッ、ビュルビュルって♪　精液止まんないのぉ♪」
		CASEELSE
			PRINTFORMW 「ま、まだ出る！　んもぅ溜まりすぎ♪　あひぇ♪」
	ENDSELECT
ENDIF

