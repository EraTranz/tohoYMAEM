;YM→YMAEコンバータによって作成されました
;================================================
;汎用特殊絶頂
;放尿
;================================================
;-------------------------------------------------
;RANK無指定
;とりあえずSYSTXT_MESSAGE_PALAMCNG.ERBに丸投げする
;-------------------------------------------------
@K2008_SP_ORGASM_GENERAL_12
CALL SYSTXT_MESSAGE_PEE

;-------------------------------------------------
;RANK = 0
;-------------------------------------------------
@K2008_RANK0_SP_ORGASM_GENERAL_12
SELECTCASE K2008F_DEVRANK("放尿")
	CASE 0 TO 3
		SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「あぁあ！？　嫌ッ、トイレ行かせて、出ちゃ……　ああぁぁ」
			CASEELSE
				PRINTFORMW 「あ、み、見ちゃだめ！　おしっこ見ないでよぉ」
		ENDSELECT
	CASE 4 TO 5
		SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「こんなのッ、全然へいき、我慢できッ、んん！？」
				PRINTFORMW 「ンー！　ま、まだ頑張れ……　も、もうダメぇ」
			CASEELSE
				PRINTFORMW 「あ、やだちょっとまって、待ってって！」
				PRINTFORMW 「……でちゃう」
				PRINTFORMW 「おしっこ出ちゃうって言ってるの！　もうっ、この、バカァ！」
		ENDSELECT
ENDSELECT

