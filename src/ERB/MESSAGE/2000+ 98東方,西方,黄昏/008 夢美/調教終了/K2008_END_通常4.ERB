;YM→YMAEコンバータによって作成されました
;=================================================
;RANK = 4
;とりあえずRANK5からコピペして体裁を繕う
;=================================================

;-------------------------------------------------
;来てすぐ帰った（予約関数）
;-------------------------------------------------
@K2008_RANK4_END0(ARGS = "")
;条件を記入する
SIF __K2008N_CAN_CALL__
	RETURN K2008F_COMCOUNT() <= 0

;体力切れの場合だけ別扱い
IF BASE:体力 < 500
	PRINTFORMW 「ごめんなさい……　ちょっと、起き上がる力も出ないの」
	PRINTFORMW 「少しだけ休ませて、ね？」
ELSE
	PRINTFORMW 「え、もう帰っちゃうの？」
	PRINTFORMW 「――今日は来てくれただけで我慢してあげる」
	PRINTFORMW 「だから次は……　期待していいのよね？」
ENDIF


;-------------------------------------------------
;汎用
;-------------------------------------------------
@K2008_RANK4_END1(ARGS = "")
SIF __K2008N_CAN_CALL__
	RETURN 1

PRINTFORMW 「もう終わりなのね。　楽しい時間ってすぐ過ぎちゃうわ」
PRINTFORMW 「ねえ、次はいつ来てくれるの？」
PRINTFORMW 「お早いご来訪、お待ちしてるわ♪」


;-------------------------------------------------
;快Ｃが絶頂寸前
;-------------------------------------------------
@K2008_RANK4_END10(ARGS = "")
SIF __K2008N_CAN_CALL__
	RETURN PALAM:快Ｃ == PALAMLV:4 - 1

IF K2008F_CAN_EJAC(TARGET)
	PRINTFORMW 「お、終わる前に、おちんちんイかせてください」
ELSE
	PRINTFORMW 「クリちゃん、最後に弄らせてください」
ENDIF
PRINTFORMW 「お股がムズムズして落ち着かないの」


;-------------------------------------------------
;快Ｖが絶頂寸前
;-------------------------------------------------
@K2008_RANK4_END11(ARGS = "")
SIF __K2008N_CAN_CALL__
	RETURN PALAM:快Ｖ == PALAMLV:4 - 1

PRINTFORMW 「あ……　お、終わりなの？」
PRINTFORMW 「最後に、おまんこイかせてください……　子宮が疼いちゃってるの」


;-------------------------------------------------
;快Ａが絶頂寸前
;-------------------------------------------------
@K2008_RANK4_END12(ARGS = "")
SIF __K2008N_CAN_CALL__
	RETURN PALAM:快Ａ == PALAMLV:4 - 1

PRINTFORMW 「あのね、終わる前にちょっとだけ、お願いがあるの」
PRINTFORMW 「お尻の穴、弄って欲しいんだけど……　だめ？」
PRINTFORMW 「ちょっと、ちょっとだけでいいから！　むず痒いのスッキリさせて！！」


;-------------------------------------------------
;快Ｂが絶頂寸前
;-------------------------------------------------
@K2008_RANK4_END13(ARGS = "")
SIF __K2008N_CAN_CALL__
	RETURN PALAM:快Ｂ == PALAMLV:4 - 1

PRINTFORMW 「……ねえ、終わる前に一つお願い聞いてくれる？」
PRINTFORMW 「胸が張っちゃって、ドキドキが苦しいの」
PRINTFORMW 「優しくマッサージして……　くれるわよね」


;-------------------------------------------------
;発情している（淫魔持ちでない場合は、残体力1000以上も）
;-------------------------------------------------
@K2008_RAN4_END23(ARGS = "")
SIF __K2008N_CAN_CALL__
	RETURN PALAM:欲情 > PALAMLV:10 && (TALENT:淫魔 > 0 || BASE:体力 >= 1000)

PRINTFORMW 「えへへぇ♪　ねえ、次は何するの？」
PRINTFORMW 「え、これで終わり？　もうちょっと頑張りたかったわ」
PRINTFORMW 「……火照っちゃった体、どうしてくれるのよ」
PRINTFORMW 「次は！　私がバテちゃうまで頑張って頂戴ね」


;-------------------------------------------------
;焦らしたまま終わった
;-------------------------------------------------
@K2008_RANK4_END97(ARGS = "")
SIF __K2008N_CAN_CALL__
	RETURN TEQUIP:焦らしプレイ > 0

PRINTFORMW 「焦らしたまま……　終わりなの？」
;寸止め蓄積中
SIF 10 < (EX:Ｃ寸止め累積数 + EX:Ｖ寸止め累積数 + EX:Ａ寸止め累積数 + EX:Ｂ寸止め累積数 + EX:射精寸止め累積数 + EX:噴乳寸止め累積数 + EX:放尿寸止め累積数)
	PRINTFORMW 「――触っただけで弾けちゃいそうなのに」
PRINTFORML 
PRINTFORMW 「……ずるいんだ」

;-------------------------------------------------
;時間切れ
;-------------------------------------------------
@K2008_RANK4_END98(ARGS = "")
SIF __K2008N_CAN_CALL__
	RETURN K2008F_COMCOUNT() >= 100

PRINTFORMW 「んー！！　満足♪　満足♪」
PRINTFORMW 「楽しいと時間、つい忘れちゃうわね」
PRINTFORMW 「またよろしくね、%CALLNAME:MASTER%！！」




