;YM→YMAEコンバータによって作成されました
;=================================================
;RANK = 5
;=================================================

;-------------------------------------------------
;来てすぐ帰った（予約関数）
;-------------------------------------------------
@K2008_RANK5_END0(ARGS = "")
;条件を記入する
SIF __K2008N_CAN_CALL__
	RETURN K2008F_COMCOUNT() <= 0

;体力切れの場合だけ別扱い
IF BASE:体力 < 500
	PRINTFORMW 「ごめんなさい……　ちょっと、起き上がる力も出ないの」
	PRINTFORMW 「少しだけ休ませて、ね？」
ELSE
	PRINTFORMW 「え、もう帰っちゃうの？」
	PRINTFORMW 「――今日は来てくれただけで我慢してあげる」
	PRINTFORMW 「だから次は……　期待していいのよね？」
ENDIF


;-------------------------------------------------
;汎用
;-------------------------------------------------
@K2008_RANK5_END1(ARGS = "")
SIF __K2008N_CAN_CALL__
	RETURN 1

PRINTFORMW 「もう終わりなのね。　楽しい時間ってすぐ過ぎちゃうわ」
PRINTFORMW 「ねえ、次はいつ来てくれるの？」
PRINTFORMW 「お早いご来訪、お待ちしてるわ♪」


;-------------------------------------------------
;快Ｃが絶頂寸前
;-------------------------------------------------
@K2008_RANK5_END10(ARGS = "")
SIF __K2008N_CAN_CALL__
	RETURN PALAM:快Ｃ == PALAMLV:4 - 1

IF K2008F_CAN_EJAC(TARGET)
	PRINTFORMW 「お、終わる前に、おちんちんイかせてください」
ELSE
	PRINTFORMW 「クリちゃん、最後に弄らせてください」
ENDIF
PRINTFORMW 「お股がムズムズして落ち着かないの」


;-------------------------------------------------
;快Ｖが絶頂寸前
;-------------------------------------------------
@K2008_RANK5_END11(ARGS = "")
SIF __K2008N_CAN_CALL__
	RETURN PALAM:快Ｖ == PALAMLV:4 - 1

PRINTFORMW 「あ……　お、終わりなの？」
PRINTFORMW 「最後に、おまんこイかせてください……　子宮が疼いちゃってるの」


;-------------------------------------------------
;快Ａが絶頂寸前
;-------------------------------------------------
@K2008_RANK5_END12(ARGS = "")
SIF __K2008N_CAN_CALL__
	RETURN PALAM:快Ａ == PALAMLV:4 - 1

PRINTFORMW 「あのね、終わる前にちょっとだけ、お願いがあるの」
PRINTFORMW 「お尻の穴、弄って欲しいんだけど……　だめ？」
PRINTFORMW 「ちょっと、ちょっとだけでいいから！　むず痒いのスッキリさせて！！」


;-------------------------------------------------
;快Ｂが絶頂寸前
;-------------------------------------------------
@K2008_RANK5_END13(ARGS = "")
SIF __K2008N_CAN_CALL__
	RETURN PALAM:快Ｂ == PALAMLV:4 - 1

PRINTFORMW 「……ねえ、終わる前に一つお願い聞いてくれる？」
PRINTFORMW 「胸がピクピクってなって、ドキドキして、苦しいの」
PRINTFORMW 「優しくマッサージして……　くれるわよね」

;-------------------------------------------------
;射精寸前
;-------------------------------------------------
@K2008_RANK5_END21(ARGS = "")
SIF __K2008N_CAN_CALL__
	RETURN (BASE:射精 == MAXBASE:射精 - 1) && ABL:射精中毒 > 0

PRINTFORMW 「おちんぽ、パンパンに膨らんでるのに終わりなんて」
PRINTFORMW 「ねえ、射精させてよ。　いいでしょ？　ほらほら」

;-------------------------------------------------
;噴乳寸前
;-------------------------------------------------
@K2008_RANK5_END22(ARGS = "")
SIF __K2008N_CAN_CALL__
	RETURN (BASE:母乳 == MAXBASE:母乳 - 1) && ABL:噴乳中毒 > 0

PRINTFORMW 「おっぱいも乳首もパンパンに膨らんでる……」
PRINTFORMW 「楽しい楽しい乳搾りの延長戦とか、どう？　興味ない？」

;-------------------------------------------------
;放尿寸前
;-------------------------------------------------
@K2008_RANK5_END23(ARGS = "")
SIF __K2008N_CAN_CALL__
	RETURN (BASE:尿意 == MAXBASE:尿意 - 1) && ABL:露出癖 >= 3 && K2008F_DEVRANK("放尿") >= 3

PRINTFORMW 「ねね、ちょっとトイレまで付き合ってくれない？」
PRINTFORMW 「いいじゃない、一息つくときの連れションって、やってみたいの」

;-------------------------------------------------
;発情している（淫魔持ちでない場合は、残体力1000以上も）
;-------------------------------------------------
@K2008_RANK5_END31(ARGS = "")
SIF __K2008N_CAN_CALL__
	RETURN PALAM:欲情 > PALAMLV:10 && (TALENT:淫魔 > 0 || BASE:体力 >= 1000)

PRINTFORMW 「えへへぇ♪　ねえ、次は何するの？」
PRINTFORMW 「え、これで終わり？　もうちょっと頑張りたかったわ」
PRINTFORMW 「……火照っちゃった体、どうしてくれるのよ」
PRINTFORMW 「次は！　私がバテちゃうまで頑張って頂戴ね」

;-------------------------------------------------
;膣内に大量の精液が溜まっている
;-------------------------------------------------
@K2008_RANK5_END32(ARGS = "")
#DIM _SAMEN
_SAMEN = K2008F_SAMENCOUNT("GET", 1)
;条件を記入する
SIF __K2008N_CAN_CALL__
	RETURN _SAMEN >= 8 && K2008F_DEVRANK("V") >= 3

PRINTFORMW 「はーっ、はーっ、%K2008FS_CALL_MY_V()%に精液出しすぎぃ」
CALL K2008_SE("液漏れ")
SIF _SAMEN >= 12
	PRINTFORMW 「ほら見える？　汚い音立てて白濁液が吹き出てるの」
SIF _SAMEN >= 16
	PRINTFORMW 「んぉお゛、溜まりすぎてザーメン潮吹きできてるぅ♪」
SIF _SAMEN >= 20
	PRINTFORMW 「ブビューって！　マンコから射精止まんないぃい！！」
SIF TEQUIP:排卵誘発剤 > 0
	PRINTFORMW 「こんなの絶対妊娠したぁ、精子が卵子の争奪戦して暴れてる♪」
SIF TEQUIP:緊急避妊薬 >0
	PRINTFORMW 「こんなの避妊むりぃ、受精卵ぜったい着床してるわよぉ」
SIF TALENT:妊娠 > 0
	PRINTFORMW 「赤ちゃん孕んでるのに、子宮を精液タンクにされたちゃったぁ」

;-------------------------------------------------
;腸内に大量の精液が溜まっている（夢美に関していえば、VよりAのほうが回数を稼ぎやすい）
;-------------------------------------------------
@K2008_RANK5_END33(ARGS = "")
#DIM _SAMEN
_SAMEN = K2008F_SAMENCOUNT("GET", 2)
SIF __K2008N_CAN_CALL__
	RETURN _SAMEN >= 10 && K2008F_DEVRANK("A") >= 3

PRINTFORMW 「はーっ、はーっ、%K2008FS_CALL_MY_A()%に精液出しすぎぃ」
CALL K2008_SE("液漏れ")
SIF _SAMEN >= 15
	PRINTFORMW 「あうぅ……　%K2008FS_CALL_MY_A()%ゆるゆるにされたから流れ出ちゃうじゃない」
SIF _SAMEN >= 20
	PRINTFORMW 「ん゛ぁあ゛ー！　肛門からザーメン噴射とまりゃにゃい♪」
SIF _SAMEN >= 30
	PRINTFORMW 「も、だめぇ！　これザーメン以外のも絶対でてるっ、腸液まみれのが漏れちゃってる！」
SIF _SAMEN >= 40
	PRINTFORMW 「ぁ゛あ゛～♪　ケツ穴の中身全部ぶちまけちゃったぁ♪」
SIF K2008F_DEVRANK("排泄") >= 3
	PRINTFORMW 「お%K2008FS_CALL_YOUR_C()%浣腸スゴすぎぃ。　こんなの教え込まれたら、もう一人でトイレ行けないじゃない」

;-------------------------------------------------
;尿道／膀胱内に大量の精液が溜まっている（かなりレア）
;-------------------------------------------------
@K2008_RANK5_END34(ARGS = "")
#DIM _SAMEN
_SAMEN = K2008F_SAMENCOUNT("GET", 3)
SIF __K2008N_CAN_CALL__
	RETURN _SAMEN >= 4 && EXP:Ｕ拡張経験 >= EXPLV:5

PRINTFORMW 「おしっこ穴までザーメンタンクにされちゃったぁ♪」
SIF _SAMEN >= 6
	PRINTFORMW 「あ゛あ゛～　白くてネバネバする小便垂れ流しでき゛も゛ち゛い゛い゛～♪」
SIF _SAMEN >= 10
	PRINTFORMW 「ん゛ンー！！　膀胱のザーメン全部ひり出しちゃう゛ぅ゛～」
SIF BASE:尿意 == (MAXBASE:尿意 - 1)
	PRINTFORMW 「あっ！　真っ黄色のオシッコまで噴水したぁ♪」

;-------------------------------------------------
;最後のコマンドの実行回数がやたらと多い
;-------------------------------------------------
@K2008_RANK5_END41(ARGS = "")
SIF __K2008N_CAN_CALL__
	RETURN K2008F_HOWTIMES_COM( K2008F_COMHISTORY("GETNOW") ) >= EXPLV:3

PRINTFORMW 「んっ、最後の……　いっぱいしちゃったけど、好きだったりする？」
IF K2008F_HOWMANY_SOURCE_PLEASURE("GETNOW") >= 4
	PRINTFORMW 「私も気持ちよかったから、次もいっぱいしてくれるとありがたいわ」
ELSEIF K2008F_HOWMANY_SOURCE_FAVOR("GETNOW") >= 4
	PRINTFORMW 「んふふ～、じゃあ今度はもっと過激にやってあげるわ」
ELSEIF K2008F_HOWMANY_SOURCE_REFUSAL("GETNOW") >= 4
	PRINTFORMW 「私はちょっと苦手だけど、けど、%CALLNAME:MASTER%が好きなら頑張ってみる！」
ELSEIF K2008F_HOWMANY_SOURCE_PAIN("GETNOW") >= 4
	PRINTFORMW 「ふ～ん？　そんなに私を虐めるのが好きなんだ」
	PRINTFORMW 「ふふふ、さーて、パイプ椅子はどこにしまったかしらぁ～」
ENDIF

;-------------------------------------------------
;焦らしたまま終わった
;-------------------------------------------------
@K2008_RANK5_END97(ARGS = "")
SIF __K2008N_CAN_CALL__
	RETURN TEQUIP:焦らしプレイ > 0

PRINTFORMW 「焦らしたまま……　終わりなの？」
;寸止め蓄積中
SIF 10 < (EX:Ｃ寸止め累積数 + EX:Ｖ寸止め累積数 + EX:Ａ寸止め累積数 + EX:Ｂ寸止め累積数 + EX:射精寸止め累積数 + EX:噴乳寸止め累積数 + EX:放尿寸止め累積数)
	PRINTFORMW 「――触っただけで弾けちゃいそうなのに」
PRINTFORML 
PRINTFORMW 「……ずるいんだ」

;-------------------------------------------------
;時間切れ
;-------------------------------------------------
@K2008_RANK5_END98(ARGS = "")
SIF __K2008N_CAN_CALL__
	RETURN K2008F_COMCOUNT() >= 100

PRINTFORMW 「んー！！　満足♪　満足♪」
PRINTFORMW 「楽しいと時間、つい忘れちゃうわね」
PRINTFORMW 「またよろしくね、%CALLNAME:MASTER%！！」

