;YM→YMAEコンバータによって作成されました
;========================================================
;調教者のステータス等に応じて呼称を変化させる
;========================================================


;=================================================
;■相手の名前
;-------------------------------------------------
;引数は通常省略
;=================================================
@K2008FS_CALL_YOUR_NAME(ARG = __INT_MIN__)
#FUNCTIONS
#DIMS _CALLNAME
_CALLNAME = 

SELECTCASE ARG
	CASE __INT_MIN__
		;未指定の場合、助手か主人（つまりPLAYER）
		IF ASSIPLAY
			_CALLNAME = %CALLNAME:ASSI%
		ELSE
			_CALLNAME = %CALLNAME:MASTER%
		ENDIF
	CASE MASTER
		;主人の場合
		_CALLNAME = %CALLNAME:MASTER%
	CASE ASSI
		;助手の場合
		SIF ASSI <= 0
			THROW 【K2008エラー】助手が居ないのにASSIを指定してはいけません
		_CALLNAME = %CALLNAME:ASSI%
	CASEELSE
		THROW 【K2008エラー】未定義のキャラクタ変数 {ARG} が指定されました
ENDSELECT

RETURNF _CALLNAME


;=================================================
;■自分の陰核／陰茎（C）
;=================================================
@K2008FS_CALL_MY_C()
#FUNCTIONS
#DIMS _CALLNAME
_CALLNAME = 

;オトコ
IF TALENT:オトコ
	SELECTCASE K2008F_RANK()
		CASE 0, 1
			_CALLNAME = ペニス
		CASE 2, 3
			_CALLNAME = %K2008FS_RANDS("おちんちん", "ペニス")%
		CASE 4, 5
			_CALLNAME = %K2008FS_RANDS("おちんぽ", "おちんちん", "ちんちん", "ちんぽ")%
	ENDSELECT
;ふたなり
ELSEIF TALENT:ふたなり
	SELECTCASE K2008F_RANK()
		CASE 0, 1
			_CALLNAME = おちんちん
		CASE 2, 3
			_CALLNAME = %K2008FS_RANDS("おちんちん", "ちんちん", "ペニス")%
		CASE 4, 5
			_CALLNAME = %K2008FS_RANDS("おちんぽ", "おちんちん", "ちんちん", "メスちんぽ", "めすちんぽ")%
	ENDSELECT
;肥大陰核（または巨根判定）
ELSEIF TALENT:肥大陰核 || BASE:陰核 >= 150
	SELECTCASE K2008F_RANK()
		CASE 0, 1
			_CALLNAME = クリトリス
		CASE 2, 3
			_CALLNAME = %K2008FS_RANDS("勃起クリ", "勃起クリトリス")%
		CASE 4, 5
			_CALLNAME = %K2008FS_RANDS("クリちんぽ", "巨クリちゃん", "でかクリ")%
	ENDSELECT
;クリトリス
ELSE
	SELECTCASE K2008F_RANK()
		CASE 0, 1
			_CALLNAME = クリトリス
		CASE 2, 3
			_CALLNAME = %K2008FS_RANDS("クリちゃん", "クリトリス", "お豆", "お豆ちゃん")%
		CASE 4, 5
			_CALLNAME = %K2008FS_RANDS("クリちゃん", "クリトリス", "お豆", "お豆ちゃん")%
	ENDSELECT
ENDIF

RETURNF _CALLNAME

