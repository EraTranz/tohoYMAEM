;=================================================
;■自分の膣
;=================================================
@K2008FS_CALL_MY_V()
#FUNCTIONS
#DIMS _CALLNAME
_CALLNAME = 

IF K2008F_HOWMANY_SOURCE_PLEASURE("GETNOW") >= 4
	SELECTCASE K2008F_RANK()
		CASE 0, 1
			_CALLNAME = %K2008FS_RANDS("あそこ", "アソコ")%
		CASE 2, 3
			_CALLNAME = %K2008FS_RANDS("おまんこ", "まんこ")%
		CASE 4, 5
			IF TALENT:処女 == 1
				_CALLNAME = 未開通な
			ELSEIF TALENT:処女 == 2
				_CALLNAME = 新古品の
			ENDIF
			SIF K2008F_DEVRANK("V") >= 4
				_CALLNAME = %_CALLNAME%ガバガバ
			_CALLNAME = %_CALLNAME%%K2008FS_RANDS("雌まんこ", "肉壺")%
	ENDSELECT
ELSE
	SELECTCASE K2008F_RANK()
		CASE 0, 1
			_CALLNAME = %K2008FS_RANDS("あそこ", "アソコ")%
		CASE 2, 3
			_CALLNAME = %K2008FS_RANDS("膣", "あそこ", "ヴァギナ")%
		CASE 4, 5
			_CALLNAME = %K2008FS_RANDS("雌まんこ", "おまんこ", "雌まんこ", "雌マンコ", "肉壺")%
	ENDSELECT
ENDIF

RETURNF _CALLNAME

