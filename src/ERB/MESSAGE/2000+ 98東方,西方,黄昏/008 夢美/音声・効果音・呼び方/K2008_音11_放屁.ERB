;YM→YMAEコンバータによって作成されました
;=================================================
;■軽放屁・空気漏れ音（ランク分けなし）
;-------------------------------------------------
;ARGで表示長（反復回数）の指定が可能
;=================================================
@K2008FS_SE11_TEXT(ARG = 0)
#FUNCTIONS
#DIM _COUNT
#DIM _REP
#DIMS _SETEXT

_SETEXT =

SELECTCASE ARG
	CASE 0
		_REP = K2008F_RANK() + RAND:(2 + K2008F_RANK())
		_REP = MIN(5, _REP)
		_REP = MAX(2, _REP)
	CASE IS <= 0
		THROW 【K2008エラー】@K2008_SE11_TEXTの引数は正の値が必要です。
	CASEELSE
		_REP = ARG
ENDSELECT

FOR _COUNT, 0, _REP
	_SETEXT += K2008FS_SPLITRANDS("ぱすっ,パプッ,ピプッ,ぷ,プ,ぶ,ブ,ぷぅ,プゥ,ぶぅ,ブゥ,ぷしゅっ,プシュッ,ぷすぅ,ブスゥ,ぷすー,プスー,ぷぱっ,プパッ,ぷぴぃ,プピー,ぷぴっ,プピッ,ぷぴゅ,プピュ,ぷぷぅ,ププゥ,ぶぶぅ,ブブゥ,,ぶぷー,ブプー,ぷぷぷー,プププー,ぷっ,プッ,ぶっ,ブッ")

	SELECTCASE _COUNT
		CASE _REP - 1
			;文末
			_SETEXT += K2008FS_SPLITRANDS("……,")
		CASEELSE
			;区切り文字
			_SETEXT += K2008FS_SPLITRANDS("、,　,　,")
	ENDSELECT
NEXT

RETURNF _SETEXT

