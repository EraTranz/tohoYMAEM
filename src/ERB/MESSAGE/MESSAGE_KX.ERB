;-------------------------------------------------
;売却後エピソードの表示内容のセット
;-------------------------------------------------
;ARG＝表示予定キャラの登録番号
@KOJO_X_SET_SLAVE_AFTERWARDS, ARG
;REVERSALモードで調教対象があなたのときも戻る
;SIF FLAG:4 == 3 && NO:ARG == 1000
;	RETURN 0
;口上を表示できるか判定
SIF KOJO_X_MESSAGE_DISPLAYCHECK(ARG) == 0
	RETURN 0

;[嫉妬]もしくは[精神崩壊]の場合はエピソードなし
IF TALENT:ARG:嫉妬 || TALENT:ARG:精神崩壊
	RETURN 0
;歌唱技能5＋[人気]で乱数
ELSEIF ABL:ARG:歌唱技能 >= 5 && TALENT:ARG:人気 && RAND:3 == 0
	RETURN 1 + RAND:2
;料理技能5で乱数
ELSEIF ABL:ARG:料理技能 >= 5 && RAND:3 == 0
	RETURN 3 + RAND:2
;自慰中毒5で乱数
ELSEIF ABL:ARG:自慰中毒 >= 5 && RAND:3 == 0
	RETURN 5 + RAND:2
;露出癖5で[恥じらい]なしで乱数
ELSEIF ABL:ARG:露出癖 >= 5 && TALENT:ARG:恥じらい == 0 && RAND:3 == 0
	RETURN 7 + RAND:2
;サドっ気5＋[サド]が[ドＳ]で乱数
ELSEIF ABL:ARG:サドっ気 >= 5 && (TALENT:ARG:サド || TALENT:ARG:ドＳ) && RAND:3 == 0
	RETURN 9 + RAND:2
;従順5＋奉仕5＋[献身的]or[鼓舞]で乱数
ELSEIF ABL:ARG:従順 >= 5 && ABL:ARG:奉仕精神 && (TALENT:ARG:献身的 || TALENT:ARG:鼓舞) && RAND:3 == 0
	RETURN 11 +RAND:2
;判定を抜けた場合はエピソードを表示しない
ELSE
	RETURN 0
ENDIF

