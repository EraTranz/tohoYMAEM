;=============================================================================
;自動喘ぎ関数及びその子供関数
;=============================================================================
;簡易喘ぎ声をある程度シチュに応じて自動生成
;現在、調整不足のためネタ機能の域を出ないと思います
;口上未実装のキャラに自動で喘ぎ声を付けるようにしてもいいですが
;その場合素質を見て性格分岐させたいところ。
;--------------------------------------------------
;自動喘ぎ関数
;--------------------------------------------------
;喘ぎの種類判別処理
@KOJO_X_TRAINVOICE_2(ARG)
;NONE
SIF ARG < 0
	RETURN 0
	
TWAIT 75, 1

;「人格設定」
CALL DEFINE_JINKAKU_SELECT, TARGET

;LOCALに喘ぎ回数を入れる処理
;LOCAL:1に行為レベルをとりあえず0～3で規定

LOCAL = 0
LOCAL:1 = 0
LOCAL:2 = 0

;コマンドごとの喘ぎ数・基本行為レベル(激しさ数値)を設定
;………………………………………………
;愛撫・性交系
;………………………………………………
;最も軽い　C、V、A、B
IF GROUPMATCH(ARG, 0, 1, 2, 3) && RAND:3 == 0
	LOCAL = 1
	LOCAL:1 = 0
;軽め　C、V、A、B
ELSEIF GROUPMATCH(ARG, 0, 1, 2, 3, 6, 7) && RAND:2 == 0
	LOCAL = 2
	LOCAL:1 = 1
;ちょい重め　C、V、A、B
ELSEIF GROUPMATCH(ARG, 0, 1, 2, 3, 7)
	LOCAL = 3
	LOCAL:1 = 2
;本番　セックス系全部
ELSEIF GROUPMATCH(ARG, 4, 5)
	LOCAL = 3
	LOCAL:1 = 3
ENDIF

;どれくらい感じてるかチェック
IF LOCAL
	;欲情が大きいと喘ぎ数は最大で＋3まで増えます
	IF ABL:欲望 > 4 && RAND:3
		LOCAL += 3
	ELSEIF ABL:欲望 > 3 && RAND:2
		LOCAL += 2
	ELSEIF ABL:欲望 > 2
		LOCAL += 1
	ENDIF

	;ソースに対応した感覚が2以上なら1、4以上なら2。高い方を優先
	IF (ARG == 0 && ABL:Ｃ感覚 >= 4) || (ARG == 7 && ABL:Ｃ感覚 >= 4) || (ARG == 1 && ABL:Ｖ感覚 >= 4) || (ARG == 4 && ABL:Ｖ感覚 >= 4) || (ARG == 2 && ABL:Ａ感覚 >= 4) || (ARG == 5 && ABL:Ａ感覚 >= 4) || (ARG == 3 && ABL:Ｂ感覚 >= 4)
		LOCAL:2 = 2
	ELSEIF (ARG == 0 && ABL:Ｃ感覚 >= 2) || (ARG == 7 && ABL:Ｃ感覚 >= 2) || (ARG == 1 && ABL:Ｖ感覚 >= 2) || (ARG == 4 && ABL:Ｖ感覚 >= 2) || (ARG == 2 && ABL:Ａ感覚 >= 2) || (ARG == 5 && ABL:Ａ感覚 >= 2) || (ARG == 3 && ABL:Ｂ感覚 >= 2)
		LOCAL:2 = 1
	ENDIF
	;感じていない口上へ
	IF LOCAL:2 == 0
		TRYCALL KOJO_X_TRAINVOICE_TYPE_B_2, LOCAL, LOCAL:1, ARG
	;感覚4以上だと激しさ+1
	ELSE
		SIF LOCAL:2 == 2
			LOCAL:1 += 1
		TRYCALL KOJO_X_TRAINVOICE_TYPE_A_2, LOCAL, LOCAL:1, ARG
	ENDIF
	PRINTL 
	TWAIT 75, 1
	PRINTL 
	RETURN 0
ENDIF

