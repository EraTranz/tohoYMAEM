@AUTO_ABLUP
#LOCALSIZE 2
LOCAL:1 = 0

;Ｃ感覚
$AUTO_ABLUP_0
CALL CHECK_ISABLUP, 0
IF RESULT == 1
	IF ABL:Ｃ感覚 < 5
		JUEL:快Ｃ -= COST0(0)
		ABL:Ｃ感覚 += 1
		LOCAL:1 = 1
		GOTO AUTO_ABLUP_0
	ENDIF
ELSE
	SIF LOCAL:1 == 1
		PRINTFORML %ABLNAME:0%のレベルが{ABL:Ｃ感覚}になりました。
ENDIF

