LOCAL:1 = 0

;従順
$AUTO_ABLUP_10
CALL CHECK_ISABLUP, 10
IF RESULT == 1
	IF ABL:従順 < 5
		;恐怖の珠で上げる
		IF JUEL:恐怖 >= COST10(0)
			JUEL:恐怖 -= COST10(0)
			GOTO AUTO_ABLUP_10_2
		ENDIF
		;恭順の珠で上げる
		IF JUEL:恭順 >= COST10(1)
			JUEL:恭順 -= COST10(1)
			GOTO AUTO_ABLUP_10_2
		ENDIF
		;欲情の珠で上げる
		IF COST10(2) > 0
			IF JUEL:欲情 >= COST10(2)
				JUEL:欲情 -= COST10(2)
				GOTO AUTO_ABLUP_10_2
			ENDIF
		ENDIF
		;屈服の珠で上げる
		IF COST10(3) > 0
			IF JUEL:屈服 >= COST10(3)
				JUEL:屈服 -= COST10(3)
				GOTO AUTO_ABLUP_10_2
			ENDIF
		ENDIF
		$AUTO_ABLUP_10_2
		ABL:従順 += 1
		LOCAL:1 = 1
		GOTO AUTO_ABLUP_10
	ENDIF
ELSE
	SIF LOCAL:1 == 1
		PRINTFORML %ABLNAME:10%のレベルが{ABL:従順}になりました。
ENDIF

LOCAL:1 = 0

;欲望
$AUTO_ABLUP_11
CALL CHECK_ISABLUP, 11
IF RESULT == 1
	IF ABL:欲望 < 5
		;欲情の珠で上げる
		IF JUEL:欲情 >= COST12(0)
			JUEL:欲情 -= COST11(0)
			GOTO AUTO_ABLUP_11_2
		ENDIF
		;サドっ気が3以上だと先導の珠でも上昇させることができる
		IF ABL:サドっ気 >= 3
			IF JUEL:欲情 >= COST11(1) && JUEL:先導 >= COST11(0)
				JUEL:欲情 -= COST11(1)
				JUEL:先導 -= COST11(0)
				GOTO AUTO_ABLUP_11_2
			ENDIF
		ENDIF
		$AUTO_ABLUP_11_2
		ABL:欲望 += 1
		LOCAL:1 = 1
		GOTO AUTO_ABLUP_11
	ENDIF
ELSE
	IF LOCAL:1 == 1
		PRINTFORML %ABLNAME:11%のレベルが{ABL:欲望}になりました。
		SIF ABL:欲望 >= 3 && (TALENT:抑圧 || TALENT:抵抗)
			CALL YOKUBO_UP_CHECK, 1
	ENDIF
ENDIF


