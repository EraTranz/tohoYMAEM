@GET_GENRE(ARG)
#FUNCTION
RETURNF VIDEO_GENRE("GET", ARG)

@COUNT_GENRE(ARG)
#FUNCTION
RETURNF VIDEO_GENRE("PLUS", ARG)

@CLEAR_GENRE
#FUNCTION
RETURNF VIDEO_GENRE("CLEAR")

@VIDEO_GENRE(ARGS, ARG)
#FUNCTION
IF ARGS == "PLUS"
	LOCAL:ARG += 1
	RETURNF ARG:1
ELSEIF ARGS == "GET"
	RETURNF LOCAL:ARG
ELSEIF ARGS == "CLEAR"
	VARSET LOCAL, -1
	RETURNF -1
ELSE
	RETURNF -1
ENDIF
