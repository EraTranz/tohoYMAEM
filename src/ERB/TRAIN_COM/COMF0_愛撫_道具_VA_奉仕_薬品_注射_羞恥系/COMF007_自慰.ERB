	LOCALS = %LOCALS%公開
SIF TEQUIP:野外プレイ
	LOCALS = %LOCALS%野外
SIF TEQUIP:読心
	LOCALS = %LOCALS%読心
SIF TEQUIP:シャワープレイ
	LOCALS = %LOCALS%シャワー
IF TEQUIP:Ｖ系装着器具 == 2 && TEQUIP:Ａ系装着器具 == 2
	LOCALS = %LOCALS%二穴要石
ELSEIF TEQUIP:Ｖ系装着器具 == 1 && TEQUIP:Ａ系装着器具 == 1
	LOCALS = %LOCALS%二穴バイブ
ELSEIF TEQUIP:Ｖ系装着器具 == 1
	LOCALS = %LOCALS%バイブ
ELSEIF TEQUIP:Ｖ系装着器具 == 2
	LOCALS = %LOCALS%要石
ELSEIF TEQUIP:Ａ系装着器具 == 1
