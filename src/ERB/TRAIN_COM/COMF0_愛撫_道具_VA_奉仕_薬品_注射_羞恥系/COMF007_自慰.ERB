ELSEIF TEQUIP:Ｃ系装着器具 == 6
	LOCALS = %LOCALS%電撃センズリ
ENDIF
IF TEQUIP:Ｖ系装着器具 || TEQUIP:Ａ系装着器具 || TEQUIP:ビデオ撮影 || (TEQUIP:野外プレイ && TFLAG:151 < 4) || TEQUIP:シャワープレイ || TEQUIP:読心
	LOCALS = %LOCALS%オナニー
ELSEIF TEQUIP:野外プレイ && TFLAG:151 == 4 && ABL:露出癖 == 5
	LOCALS = %LOCALS%オナニーショー
ELSEIF TEQUIP:Ｃ系装着器具 == 2
	LOCALS = %LOCALS% 
ELSE
	LOCALS = %LOCALS%自慰
ENDIF
PRINTFORML %LOCALS%

