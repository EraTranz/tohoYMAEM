@COM24
;………………………………………………
;コマンドの派生判定
;………………………………………………
;御所車派生分岐を追加
;COM_ABLE以降は子宮口攻め分岐から引っ張ってきました
;強制実行時はここは絶賛スルー
IF TFLAG:699 == 0
	CALL COM_ABLE430
	IF RESULT == 1
		IF FLAG:14 & 8
			PRINTL 御所車にする？
			PRINTL [0] - する
			PRINTL [1] - しない
			$INPUT_LOOP
			INPUT
			IF RESULT == 0
				JUMP COM430
			ELSEIF RESULT == 1
				
			ELSE
				CLEARLINE 1
				REUSELASTLINE
				GOTO INPUT_LOOP
			ENDIF
		ELSE
			;PALAM:欲情をみる
			IF PALAM:欲情 < PALAMLV:1
				LOCAL = 1
			ELSEIF PALAM:欲情 < PALAMLV:2
				LOCAL = 2
			ELSEIF PALAM:欲情 < PALAMLV:3
				LOCAL = 3
			ELSEIF PALAM:欲情 < PALAMLV:4
				LOCAL = 4
			ELSE
				LOCAL = 5
			ENDIF
			LOCAL:1 = RAND:11
			LOCAL *= LOCAL:1
			LOCAL:1 = RAND:11
			LOCAL += (LOCAL:1)*ABL:Ｖ感覚
			;LOCALの値は0～100
			;騎乗位だと50以上で御所車
			IF LOCAL >= 50
				JUMP COM430
			ENDIF
		ENDIF
	ENDIF
ENDIF

PRINTL 騎乗位

