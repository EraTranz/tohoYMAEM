TCVAR:Ｖ経験 += LOCAL:1
;Ｖ調律経験
TCVAR:Ｖ調律経験 += (PLAYER == MASTER) ? LOCAL:1 # 0
;Ａ経験
TCVAR:Ａ経験 += LOCAL:2
IF ASSIPLAY == 0 && EXIST_BAR(MASTER)
	CFLAG:主人にＶ捧げ回数 += LOCAL:1
	CFLAG:主人にＡ捧げ回数 += LOCAL:2
ENDIF
;口上分岐のための
;ここで奴隷の具合によって地の文を変えたいところ
TFLAG:99 = 2
CALL DISPLAY_KOJO_TRAIN_MESSAGE_F_COM

RETURN 1

