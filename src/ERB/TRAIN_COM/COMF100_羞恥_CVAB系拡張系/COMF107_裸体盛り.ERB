;-------------------------------------------------
;裸体盛り
;-------------------------------------------------
@COM107
IF TALENT:オトコ
	PRINTL 男体盛り

	;戦闘判定
	CALL COM_TRY_BATTLE, 107
	;実行できない
	SIF RESULT == 1
		RETURN 1

	TSTR:0 = 男体盛り
ELSE
	PRINTL 女体盛り

	;戦闘判定
	CALL COM_TRY_BATTLE, 107
	;実行できない
	SIF RESULT == 1
		RETURN 1

	TSTR:0 = 女体盛り
ENDIF

;裸体盛り終了
IF TEQUIP:裸体盛り
	TEQUIP:裸体盛り = 0
	DOWNBASE:0 += 15
	DOWNBASE:1 += 50
	SOURCE:欲情追加 = ABL:欲望 * 300
	SOURCE:露出 = ABL:露出癖 * 500
	TFLAG:153 = 0
	CALL DISPLAY_KOJO_TRAIN_MESSAGE_F_COM
	RETURN 1
ENDIF

;特別メニューの有無をサーチしておく
TFLAG:153 = 0
STRLENS CSTR:2
SIF RESULT >= 1
	TFLAG:153 = 90

IF FLAG:14 & 8
	PRINTL 盛りつけは何にする？
	SIF ABL:PLAYER:料理技能 >= 5 && TFLAG:153 == 90
		PRINTFORML [1] - %CALLNAME:TARGET%の%CSTR:2%
	PRINTFORML [2] - %CALLNAME:TARGET%の海鮮盛り
	PRINTFORML [3] - %CALLNAME:TARGET%のスイートケーキ
	PRINTFORML [4] - %CALLNAME:TARGET%の新鮮サラダ
	$INPUT_LOOP
	INPUT
	IF RESULT >= 2 && RESULT <= 4
		TEQUIP:裸体盛り = RESULT
	ELSEIF RESULT == 1 && ABL:PLAYER:料理技能 >= 5 && TFLAG:153 == 90
		TEQUIP:裸体盛り = 1
	ELSE
		CLEARLINE 1
		REUSELASTLINE
		GOTO INPUT_LOOP
	ENDIF
ELSE
	LOCAL:6 = RAND:4 + 1
	SIF LOCAL:6 == 1 && ABL:PLAYER:料理技能 >= 5 && TFLAG:153 == 90
		LOCAL:6 = 90
	SIF LOCAL:6 == 1
		LOCAL:6 = 2+RAND:3
	TEQUIP:裸体盛り = LOCAL
ENDIF

DOWNBASE:0 += 20
DOWNBASE:1 += 50

LOCAL:1 = 500
LOCAL:2 = 500
LOCAL:3 = ABL:MASTER:技巧 + ABL:ASSI:技巧
LOCAL:4 = ABL:MASTER:料理技能 + ABL:ASSI:料理技能
LOCAL:5 = MAX(LOCAL:4 /2, 1)

;料理の種類によって伸びるソースが変化
IF TEQUIP:裸体盛り == 1
	TIMES LOCAL:1 , 1.50
ELSEIF TEQUIP:裸体盛り == 2
	TIMES LOCAL:2 , 1.50
ELSEIF TEQUIP:裸体盛り == 3
	TIMES LOCAL:1 , 1.20
