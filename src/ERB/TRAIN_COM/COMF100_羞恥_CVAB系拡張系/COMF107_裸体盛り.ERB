ELSEIF ABL:露出癖 == 1
	SOURCE:中毒充足 += 50
	SOURCE:液体追加 += 50
	TIMES LOCAL:1, 1.20
ELSEIF ABL:露出癖 == 2
	SOURCE:中毒充足 += 150
	SOURCE:液体追加 += 150
	TIMES LOCAL:1, 1.40
ELSEIF ABL:露出癖 == 3
	SOURCE:中毒充足 += 400
	SOURCE:液体追加 += 400
	TIMES LOCAL:1, 1.60
ELSEIF ABL:露出癖 == 4
	SOURCE:中毒充足 += 750
	SOURCE:液体追加 += 750
	TIMES LOCAL:1, 2.00
ELSE
	SOURCE:中毒充足 += 1300
	SOURCE:液体追加 += 1300
	TIMES LOCAL:1, 3.00
ENDIF
