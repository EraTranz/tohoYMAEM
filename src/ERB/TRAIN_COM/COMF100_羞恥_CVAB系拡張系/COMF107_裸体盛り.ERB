SIF TFLAG:140 & 256
	SOURCE:不潔 += 6000

CALL DISPLAY_KOJO_TRAIN_MESSAGE_F_COM

IF LOCAL:4 > 0
	IF LOCAL:4 > 8 && ABL:PLAYER:料理技能 >= 4
		LOCAL:5 = LOCAL:4
		JUEL:ASSI:習得 += 500 * LOCAL:4
		PRINTFORML 習得の珠(%CALLNAME:ASSI%)＋{500*LOCAL:4}
	ELSEIF LOCAL:4 > 6 && ABL:PLAYER:90 >= 3
		JUEL:ASSI:習得 += 300 * LOCAL:4
		PRINTFORML 習得の珠(%CALLNAME:ASSI%)＋{300*LOCAL:4}
	ELSEIF LOCAL:4 > 4 && ABL:PLAYER:90 >= 2
		JUEL:ASSI:習得 += 200 * LOCAL:4
		PRINTFORML 習得の珠(%CALLNAME:ASSI%)＋{200*LOCAL:4}
	ELSE
		JUEL:ASSI:習得 += 100 * LOCAL:4
		PRINTFORML 習得の珠(%CALLNAME:ASSI%)＋{100*LOCAL:4}
	ENDIF
ELSE
	JUEL:ASSI:習得 += 10 * (LOCAL:3 +1)
	PRINTFORML 習得の珠(%CALLNAME:ASSI%)＋{10*(LOCAL:3 +1)}
ENDIF
TCVAR:ASSI:調理経験 += LOCAL:5
TCVAR:MASTER:調理経験 += LOCAL:5

IF TEQUIP:裸体盛り == 1
