;=============================================================================
;秘貝開帳
;=============================================================================
;--------------------------------------------------
;実行判定
;--------------------------------------------------
@COM_ABLE110
;個別設定コマンドフィルタ
SIF DITEMTYPE:999:110 > 0
	RETURN 0
;対象の性別が♀限定
SIF COM_ABLE_CHECK_FEMALE(TARGET) == 0
	RETURN 0
;従順が2未満だとダメ
SIF ABL:従順 < 2
	RETURN 0
;処女の場合、従順3以上または露出癖3以上または淫乱がないとダメ
SIF TALENT:処女 && ABL:従順 < 3 && ABL:露出癖 < 3 && TALENT:淫乱 == 0
	RETURN 0
;睡眠中は不可
SIF EQUIP:睡眠薬
	RETURN 0
;失神中は不可
SIF TFLAG:899 > 0
	RETURN 0
;Ｖ系装着具使用中は不可
SIF TEQUIP:Ｖ系装着器具 || TEQUIP:クスコ
	RETURN 0
;縄使用中は不可
SIF TEQUIP:緊縛関連
	RETURN 0
;乗馬中はダメ
SIF TEQUIP:三角木馬乗馬中
	RETURN 0
;お風呂場プレイ中はプレイマットが無いとダメ
SIF TEQUIP:お風呂場プレイ && ITEM:プレイマット == 0
	RETURN 0
;蒸し風呂と岩盤浴以外の○○風呂入浴中はダメ
SIF TEQUIP:特殊風呂入浴中 && TEQUIP:特殊風呂入浴中 != 12 && TEQUIP:特殊風呂入浴中 != 15
	RETURN 0
;オムツプレイ中はダメ
SIF TEQUIP:オムツプレイ
	RETURN 0
;褌着用中の場合はダメ
SIF TEQUIP:コスチュームプレイ == 17
	RETURN 0
;時止め中は無理
SIF TEQUIP:時間止め
	RETURN 0
;触手調教中はダメ
SIF TEQUIP:触手調教
	RETURN 0
;距離のチェック
SIF EQUIP:戦闘中 && COM_ABLE_BATTLE(TARGET, PLAYER, 110) == 0
	RETURN 0
RETURN 1

;--------------------------------------------------
;コマンド本体
;--------------------------------------------------
@COM110
;………………………………………………
;コマンドの派生判定
;………………………………………………
;奴隷が勝手にやった場合を除き
;前回と今回の調教者同じかつ、前回が秘貝開帳見せつけもしくは相互秘貝開帳の場合、相互秘貝開帳へ
IF ((PREVCOM == 117 || PREVCOM == 631) && JUDGE_TRAINER_CONSECUTIVE() == 1) && TFLAG:699 == 0
	SIF EXCOM_ABLE631() == 1
		JUMP COM631
ENDIF
PRINTL 秘貝開帳

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
;戦闘判定
CALL COM_TRY_BATTLE, 110
;実行できない
SIF RESULT == 1
	RETURN 1

IF TFLAG:699 == 0
	CALL COM_ORDER
	;実行できない
	SIF RESULT == 0
		RETURN 0
ENDIF

;-------------------------------------------------
;実行決定
;-------------------------------------------------
TSTR:0 = 秘貝開帳
CALL DISPLAY_KOJO_TRAIN_MESSAGE_F_COM

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
DOWNBASE:0 += 50
DOWNBASE:1 += 90

SOURCE:液体追加 = 10
SOURCE:露出 = 250
SOURCE:屈従 = 1500
SOURCE:逸脱 = 500

;ABL:露出癖をみる
IF ABL:露出癖 == 0
	SOURCE:露出 += 1500
	SOURCE:液体追加 = 50
	SOURCE:不潔 = 2000
ELSEIF ABL:露出癖 == 1
	SOURCE:露出 += 1250
	SOURCE:液体追加 = 100
	SOURCE:不潔 = 1000
ELSEIF ABL:露出癖 == 2
	SOURCE:露出 += 750
	SOURCE:液体追加 = 500
	SOURCE:不潔 = 500
ELSEIF ABL:露出癖 == 3
	SOURCE:露出 += 500
	SOURCE:液体追加 = 1000
	SOURCE:不潔 = 200
ELSEIF ABL:露出癖 == 4
	SOURCE:露出 += 250
	SOURCE:液体追加 = 1500
	SOURCE:不潔 = 100
ELSE
	SOURCE:液体追加 = 2000
	SOURCE:不潔 = 50
ENDIF

;EXP:自慰経験をみる
IF EXP:自慰経験 < EXPLV:1
	TIMES SOURCE:屈従, 0.20
	TIMES SOURCE:不潔, 1.50
ELSEIF EXP:自慰経験 < EXPLV:2
	TIMES SOURCE:屈従, 0.50
	TIMES SOURCE:不潔, 1.20
ELSEIF EXP:自慰経験 < EXPLV:3
	TIMES SOURCE:屈従, 0.80
	TIMES SOURCE:不潔, 1.00
ELSEIF EXP:自慰経験 < EXPLV:4
	TIMES SOURCE:屈従, 1.00
	TIMES SOURCE:不潔, 0.80
ELSEIF EXP:自慰経験 < EXPLV:5
	TIMES SOURCE:屈従, 1.20
	TIMES SOURCE:不潔, 0.50
ELSE
	TIMES SOURCE:屈従, 1.50
	TIMES SOURCE:不潔, 0.20
ENDIF

;PALAM:欲情をみる
IF PALAM:欲情 < PALAMLV:1
	TIMES SOURCE:液体追加 , 0.50
	TIMES SOURCE:不潔 , 1.80
ELSEIF PALAM:欲情 < PALAMLV:2
	TIMES SOURCE:液体追加 , 0.80
	TIMES SOURCE:不潔 , 1.50
ELSEIF PALAM:欲情 < PALAMLV:3
	TIMES SOURCE:液体追加 , 1.20
	TIMES SOURCE:不潔 , 1.20
ELSEIF PALAM:欲情 < PALAMLV:4
	TIMES SOURCE:液体追加 , 1.50
	TIMES SOURCE:不潔 , 0.80
ELSE
	TIMES SOURCE:液体追加 , 1.80
	TIMES SOURCE:不潔 , 0.50
ENDIF

;恥じらい・恥薄い
IF TALENT:恥じらい
	TIMES SOURCE:不潔 , 1.20
ELSEIF TALENT:恥薄い
	TIMES SOURCE:不潔 , 0.50
ENDIF

;Ｃ敏感、鈍感をみる
;快Ｃ自体のチェックは後でまとめてやる
;IF TALENT:Ｃ敏感
;	TIMES SOURCE:快Ｃ , 1.50
;	TIMES SOURCE:屈従 , 1.50
;	TIMES SOURCE:逸脱 , 1.50
;ELSEIF TALENT:Ｃ鈍感
;	TIMES SOURCE:快Ｃ , 0.60
;	TIMES SOURCE:屈従 , 0.60
;	TIMES SOURCE:逸脱 , 1.50
;ENDIF

;処女、貞操観念
IF TALENT:処女 == 1
	TIMES SOURCE:不潔 , 2.00
	TIMES SOURCE:屈従 , 2.00
	SOURCE:露出 += 2000
	SIF TALENT:貞操観念
		TIMES SOURCE:露出 , 2.00
ELSEIF TALENT:処女
	TIMES SOURCE:不潔 , 1.20
ENDIF

SIF ABL:露出癖 >= 3
	TCVAR:自慰経験 += 1

;奴隷のＶ⇔指の汚れが移動
STAIN:ヴァギナ |= STAIN:手
STAIN:手 |= STAIN:ヴァギナ

IF TALENT:オトコ == 0 && TALENT:PLAYER:オトコ == 0
	TCVAR:レズ経験 += 1
ELSEIF TALENT:オトコ && TALENT:PLAYER:オトコ
	TCVAR:ＢＬ経験 += 1
ENDIF

;露出快楽経験入手判定
TFLAG:100 |= 2
RETURN 1

