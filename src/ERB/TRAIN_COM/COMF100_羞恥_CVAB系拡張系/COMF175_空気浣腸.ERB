;=============================================================================
;空気浣腸
;=============================================================================
;--------------------------------------------------
;実行判定
;--------------------------------------------------
@COM_ABLE175
;個別設定コマンドフィルタ
SIF DITEMTYPE:999:175 > 0
	RETURN 0
;Ａ系拡張コマンドを有効にしてないとダメ
SIF (FLAG:15 & 4) == 0
	RETURN 0
;Ａ調教フィルタがオフになってないとダメ
SIF FLAG:2 & 2
	RETURN 0
;乗馬中はダメ
SIF TEQUIP:三角木馬乗馬中
	RETURN 0
;オムツプレイ中はダメ
SIF TEQUIP:オムツプレイ
	RETURN 0
;褌着用中の場合はダメ
SIF TEQUIP:コスチュームプレイ == 17
	RETURN 0
;触手調教中はダメ
SIF TEQUIP:触手調教
	RETURN 0
;解除はいつでも可能
SIF TEQUIP:Ａ系装着器具 == 8
	RETURN 1
;アイテムを持っていないとダメ
SIF ITEM:空気ポンプ == 0
	RETURN 0
;調教対象が小人体型の場合は主人が禁断の知識持ちじゃないとダメ
SIF TALENT:小人体型 && TALENT:MASTER:禁断の知識 == 0
	RETURN 0
;機械だとダメ
SIF TALENT:機械
	RETURN 0
;Ａ経験が50未満じゃできない
SIF EXP:Ａ経験 < 50
	RETURN 0
;露出癖、欲望、従順の合計が10ないとできない
SIF ABL:従順 + ABL:欲望 + ABL:露出癖 < 10
	RETURN 0
;クスコ挿入中はダメ
SIF TEQUIP:クスコ
	RETURN 0
;他のＡ系装着具使用中は不可
SIF (TEQUIP:Ａ系装着器具 && TEQUIP:Ａ系装着器具 != 8) || TEQUIP:氷柱
	RETURN 0
;蒸し風呂と岩盤浴以外の○○風呂入浴中はダメ
SIF TEQUIP:特殊風呂入浴中 && TEQUIP:特殊風呂入浴中 != 12 && TEQUIP:特殊風呂入浴中 != 15
	RETURN 0
;距離のチェック
SIF EQUIP:戦闘中 && COM_ABLE_BATTLE(TARGET, PLAYER, 175) == 0
	RETURN 0
RETURN 1

;--------------------------------------------------
;コマンド本体
;--------------------------------------------------
@COM175
PRINTL 空気浣腸

;戦闘判定
CALL COM_TRY_BATTLE, 175
;実行できない
SIF RESULT == 1
	RETURN 1

TSTR:0 = 空気浣腸
CALL DISPLAY_KOJO_TRAIN_MESSAGE_F_COM

DOWNBASE:0 += 200
DOWNBASE:1 += 300

;ABL:Ａ感覚をみる
IF ABL:Ａ感覚 == 0
	SOURCE:快Ａ = 0
	SOURCE:屈従 = 300
ELSEIF ABL:Ａ感覚 == 1
	SOURCE:快Ａ = 10
	SOURCE:屈従 = 800
ELSEIF ABL:Ａ感覚 == 2
	SOURCE:快Ａ = 450
	SOURCE:屈従 = 1400
ELSEIF ABL:Ａ感覚 == 3
	SOURCE:快Ａ = 80
	SOURCE:屈従 = 1800
ELSEIF ABL:Ａ感覚 == 4
	SOURCE:快Ａ = 120
	SOURCE:屈従 = 2100
ELSE
	SOURCE:快Ａ = 160
	SOURCE:屈従 = 2400
ENDIF

;ABL:マゾっ気をみる
IF ABL:マゾっ気 == 0
	SOURCE:痛み = 2000
	SOURCE:屈従 += 200
	SOURCE:トラウマ = 8000
	SOURCE:不潔 = 8000
	SOURCE:逸脱 = 1000
	SOURCE:反感追加 = 2000
ELSEIF ABL:マゾっ気 == 1
	SOURCE:痛み = 2000
	SOURCE:屈従 += 500
	SOURCE:トラウマ = 5000
	SOURCE:不潔 = 6000
	SOURCE:逸脱 = 1000
	SOURCE:反感追加 = 1000
ELSEIF ABL:マゾっ気 == 2
	SOURCE:痛み = 2000
	SOURCE:屈従 += 800
	SOURCE:トラウマ = 3000
	SOURCE:不潔 = 5000
	SOURCE:逸脱 = 1000
	SOURCE:反感追加 = 500
ELSEIF ABL:マゾっ気 == 3
	SOURCE:痛み = 1800
	SOURCE:屈従 += 1200
	SOURCE:トラウマ = 1200
	SOURCE:不潔 = 4000
	SOURCE:逸脱 = 1000
	SOURCE:反感追加 = 100
ELSEIF ABL:マゾっ気 == 4
	SOURCE:痛み = 1800
	SOURCE:屈従 += 1500
	SOURCE:トラウマ = 500
	SOURCE:不潔 = 3000
	SOURCE:逸脱 = 1000
	SOURCE:反感追加 = 0
ELSE
	SOURCE:痛み = 1800
	SOURCE:屈従 += 2000
	SOURCE:トラウマ = 200
	SOURCE:不潔 = 2000
	SOURCE:逸脱 = 1000
	SOURCE:反感追加 = 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES SOURCE:快Ａ , 0.40
	SOURCE:痛み += 800
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES SOURCE:快Ａ , 0.80
	SOURCE:痛み += 500
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES SOURCE:快Ａ , 1.00
	SOURCE:痛み += 300
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES SOURCE:快Ａ , 1.40
	SOURCE:痛み += 120
ELSE
	TIMES SOURCE:快Ａ , 1.80
	SOURCE:痛み += 100
ENDIF

;ABL:従順をみる
IF ABL:従順 == 0
	TIMES SOURCE:快Ａ , 0.80
ELSEIF ABL:従順 == 1
	TIMES SOURCE:快Ａ , 0.90
ELSEIF ABL:従順 == 2
	TIMES SOURCE:快Ａ , 1.00
ELSEIF ABL:従順 == 3
	TIMES SOURCE:快Ａ , 1.10
ELSEIF ABL:従順 == 4
	TIMES SOURCE:快Ａ , 1.20
ELSE
	TIMES SOURCE:快Ａ , 1.30
ENDIF

;Ａ敏感、鈍感をみる
;快Ａ自体のチェックは後でまとめてやる
IF TALENT:Ａ鈍感
	TIMES SOURCE:痛み , 1.50
	TIMES SOURCE:屈従 , 1.50
	TIMES SOURCE:逸脱 , 1.50
ELSEIF TALENT:Ａ敏感
	TIMES SOURCE:痛み , 0.60
	TIMES SOURCE:屈従 , 0.60
	TIMES SOURCE:逸脱 , 0.60
ENDIF

;浣腸プラグの着脱
IF TEQUIP:Ａ系装着器具
	SOURCE:屈従 += 1000
	SOURCE:羞恥扇情 += 2000
	TEQUIP:Ａ系装着器具 = 0
ELSE
	SOURCE:痛み += 1500
	SOURCE:屈従 += 2500
	TEQUIP:Ａ系装着器具 = 8
ENDIF

TCVAR:Ａ経験 += 1

;コマンド属性：道具(調教者)
TFLAG:98 = 1
RETURN 1

;--------------------------------------------------
;空気浣腸＋アナルプラグ
;--------------------------------------------------
@EQUIP_COM175
PRINTL ＜空気浣腸＋プラグ挿入中＞

DOWNBASE:0 += 180
DOWNBASE:1 += 300

SOURCE:痛み += 2000
SOURCE:屈従 += 3000

;ABL:Ａ感覚をみる
IF ABL:Ａ感覚 == 0
	LOCAL = 0
	LOCAL:1 = 300
ELSEIF ABL:Ａ感覚 == 1
	LOCAL = 10
	LOCAL:1 = 800
ELSEIF ABL:Ａ感覚 == 2
	LOCAL = 30
	LOCAL:1 = 1400
ELSEIF ABL:Ａ感覚 == 3
	LOCAL = 80
	LOCAL:1 = 1800
ELSEIF ABL:Ａ感覚 == 4
	LOCAL = 120
	LOCAL:1 = 2100
ELSE
	LOCAL = 160
	LOCAL:1 = 2400
ENDIF

;EXP:Ａ経験をみる
IF EXP:Ａ経験 < EXPLV:1
	TIMES LOCAL , 0.50
	LOCAL:2 = 2000
ELSEIF EXP:Ａ経験 < EXPLV:2
	TIMES LOCAL , 1.00
	LOCAL:2 = 300
ELSEIF EXP:Ａ経験 < EXPLV:3
	TIMES LOCAL , 1.10
	LOCAL:2 = 50
ELSEIF EXP:Ａ経験 < EXPLV:4
	TIMES LOCAL , 1.20
	LOCAL:2 = 10
ELSEIF EXP:Ａ経験 < EXPLV:5
	TIMES LOCAL , 1.40
	LOCAL:2 = 0
ELSE
	TIMES LOCAL , 1.60
	LOCAL:2 = 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES LOCAL , 0.40
	LOCAL:2 += 800
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES LOCAL , 0.80
	LOCAL:2 += 500
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES LOCAL , 1.00
	LOCAL:2 += 300
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES LOCAL , 1.40
	LOCAL:2 += 120
ELSE
	TIMES LOCAL , 1.80
	LOCAL:2 += 100
ENDIF

;ABL:従順をみる
IF ABL:従順 == 0
	TIMES LOCAL , 0.80
ELSEIF ABL:従順 == 1
	TIMES LOCAL , 0.90
ELSEIF ABL:従順 == 2
	TIMES LOCAL , 1.00
ELSEIF ABL:従順 == 3
	TIMES LOCAL , 1.10
ELSEIF ABL:従順 == 4
	TIMES LOCAL , 1.20
ELSE
	TIMES LOCAL , 1.30
ENDIF

;Ａ敏感、鈍感をみる
;快Ａ自体のチェックは後でまとめてやる
IF TALENT:Ａ鈍感
	TIMES LOCAL , 1.50
	TIMES LOCAL:1 , 1.50
	TIMES LOCAL:2 , 1.50
ELSEIF TALENT:Ａ敏感
	TIMES LOCAL , 0.60
	TIMES LOCAL:1 , 0.60
	TIMES LOCAL:2 , 0.60
ENDIF

;処女で貞操観念
SIF TALENT:処女 == 1 && TALENT:貞操観念
	LOCAL:1 /= 3

SOURCE:快Ａ += LOCAL
SOURCE:屈従 += LOCAL:1
SOURCE:痛み += LOCAL:2
SOURCE:逸脱 += LOCAL:1

TCVAR:Ａ経験 += 1

IF TALENT:オトコ == 0 && TALENT:PLAYER:オトコ == 0
	TCVAR:レズ経験 += 1
ELSEIF TALENT:オトコ && TALENT:PLAYER:オトコ
	TCVAR:ＢＬ経験 += 1
ENDIF
RETURN 1

;--------------------------------------------------
;調教時メッセージ
;--------------------------------------------------
@SYSTXT_TRAIN_MESSAGE_F_COM_175
IF TEQUIP:Ａ系装着器具
	IF MARK:反発刻印 > 1 && EQUIP:睡眠薬 == 0 && TFLAG:899 == 0
		PRINTFORML %CALLNAME:PLAYER%が嫌がる%CALLNAME:TARGET%を押さえつけて、
		PRINTFORML お尻に刺さっている栓を抜くと、%CALLNAME:TARGET%の菊穴からけたたましい放屁の音が響き渡った
		PRINTFORML その恥ずかしい音を聞くと、%CALLNAME:TARGET%は悔しさ\@(TALENT:恥薄い == 0 && TALENT:保守的) ? と恥ずかしさ # \@に涙を浮かべながら%CALLNAME:PLAYER%を睨みつけた…
	ELSEIF TALENT:恥じらい && TALENT:保守的 && EQUIP:睡眠薬 == 0 && TFLAG:899 == 0
		PRINTFORML %CALLNAME:PLAYER%が栓を抜くと、%CALLNAME:TARGET%の菊穴からけたたましい放屁の音が響いた
		PRINTFORML その恥ずかしい音に、%CALLNAME:TARGET%は顔を真っ赤にして俯いてしまった…
	ELSE
		PRINTFORML %CALLNAME:PLAYER%が栓を抜くと、%CALLNAME:TARGET%の菊穴からけたたましい放屁の音が響いた…
	ENDIF
ELSE
	IF MARK:反発刻印 > 1 && EQUIP:睡眠薬 == 0 && TFLAG:899 == 0
		PRINTFORML %CALLNAME:PLAYER%は睨みつけてくる%CALLNAME:TARGET%の反応を無視して、
		PRINTFORM %CALLNAME:TARGET%の
	ELSE
		PRINTFORM %CALLNAME:PLAYER%は、%CALLNAME:TARGET%の
	ENDIF
	SIF PALAM:潤滑 >= PALAMLV:4
		PRINT 粘液にまみれた
	PRINTL 菊穴に腹が膨れるくらいポンプで空気を送り込み、栓をした…
ENDIF
