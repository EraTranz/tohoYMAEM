;=============================================================================
;触手秘貝開き
;=============================================================================
;--------------------------------------------------
;実行判定
;--------------------------------------------------
@COM_ABLE512
;個別設定コマンドフィルタ
SIF DITEMTYPE:999:512 > 0
	RETURN 0
;紳士的な触手系コマンドを有効にしてないとダメ
SIF (FLAG:15 & 4096) == 0
	RETURN 0
;アイテム進化の秘法を持っていないとダメ
SIF ITEM:進化の秘法 == 0
	RETURN 0
;触手調教中じゃないとダメ
SIF TEQUIP:触手調教 == 0 && TEQUIP:触手服 == 0
	RETURN 0
;対象の性別が♀限定
SIF COM_ABLE_CHECK_FEMALE(TARGET) == 0
	RETURN 0
;Ｖ系触手責め中は不可
SIF TEQUIP:Ｖ系装着器具 || TEQUIP:クスコ
	RETURN 0
;距離のチェック
SIF EQUIP:戦闘中 && COM_ABLE_BATTLE(TARGET, PLAYER, 512) == 0
	RETURN 0
RETURN 1

;--------------------------------------------------
;コマンド本体
;--------------------------------------------------
@COM512
PRINTL 触手秘貝開き

;戦闘判定
CALL COM_TRY_BATTLE, 512
;実行できない
SIF RESULT == 1
	RETURN 1

TSTR:0 = 触手秘貝開き
CALL DISPLAY_KOJO_TRAIN_MESSAGE_F_COM

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOCAL:1 = 60
LOCAL:2 = 100

;ABL:触手中毒をみる
IF ABL:触手中毒 == 0
	TIMES LOCAL:1 , 1.20
	TIMES LOCAL:2 , 1.20
	SOURCE:欲情追加 = 10
ELSEIF ABL:触手中毒 == 1
	TIMES LOCAL:1 , 1.00
	TIMES LOCAL:2 , 1.00
	SOURCE:欲情追加 = 50
ELSEIF ABL:触手中毒 == 2
	TIMES LOCAL:1 , 0.90
	TIMES LOCAL:2 , 0.90
	SOURCE:欲情追加 = 250
ELSEIF ABL:触手中毒 == 3
	TIMES LOCAL:1 , 0.80
	TIMES LOCAL:2 , 0.80
	SOURCE:欲情追加 = 800
ELSEIF ABL:触手中毒 == 4
	TIMES LOCAL:1 , 0.70
	TIMES LOCAL:2 , 0.70
	SOURCE:欲情追加 = 1500
ELSE
	TIMES LOCAL:1 , 0.60
	TIMES LOCAL:2 , 0.60
	SOURCE:欲情追加 = 2200
ENDIF

DOWNBASE:0 += LOCAL:1
DOWNBASE:1 += LOCAL:2

SOURCE:液体追加 = 10
SOURCE:露出 = 250
SOURCE:屈従 = 100
SOURCE:逸脱 = 2500

;ABL:Ｃ感覚をみる
IF ABL:Ｃ感覚 == 0
	SOURCE:快Ｃ = 100
	SOURCE:屈従 += 100
ELSEIF ABL:Ｃ感覚 == 1
	SOURCE:快Ｃ = 200
	SOURCE:屈従 += 150
ELSEIF ABL:Ｃ感覚 == 2
	SOURCE:快Ｃ = 350
	SOURCE:屈従 += 300
ELSEIF ABL:Ｃ感覚 == 3
	SOURCE:快Ｃ = 700
	SOURCE:屈従 += 600
ELSEIF ABL:Ｃ感覚 == 4
	SOURCE:快Ｃ = 1400
	SOURCE:屈従 += 1000
ELSE
	SOURCE:快Ｃ = 2100
	SOURCE:屈従 += 1500
ENDIF

;ABL:露出癖をみる
IF ABL:露出癖 == 0
	SOURCE:不潔 = 4000
	SOURCE:露出 += 1500
ELSEIF ABL:露出癖 == 1
	SOURCE:不潔 = 2500
	SOURCE:露出 += 1250
	SOURCE:液体追加 *= 5
ELSEIF ABL:露出癖 == 2
	SOURCE:不潔 = 1000
	SOURCE:露出 += 750
	SOURCE:液体追加 *= 10
ELSEIF ABL:露出癖 == 3
	SOURCE:不潔 = 500
	SOURCE:露出 += 500
	SOURCE:液体追加 *= 50
ELSEIF ABL:露出癖 == 4
	SOURCE:不潔 = 250
	SOURCE:露出 += 250
	SOURCE:液体追加 *= 100
ELSE
	SOURCE:不潔 = 100
	SOURCE:液体追加 *= 200
ENDIF

;PALAM:欲情をみる
IF PALAM:欲情 < PALAMLV:1
	TIMES SOURCE:不潔 , 1.80
	TIMES SOURCE:液体追加 , 0.50
	TIMES SOURCE:欲情追加 , 0.50
ELSEIF PALAM:欲情 < PALAMLV:2
	TIMES SOURCE:不潔 , 1.50
	TIMES SOURCE:液体追加 , 0.80
	TIMES SOURCE:欲情追加 , 0.80
ELSEIF PALAM:欲情 < PALAMLV:3
	TIMES SOURCE:不潔 , 1.20
	TIMES SOURCE:液体追加 , 1.20
	TIMES SOURCE:欲情追加 , 1.00
ELSEIF PALAM:欲情 < PALAMLV:4
	TIMES SOURCE:不潔 , 0.80
	TIMES SOURCE:液体追加 , 1.50
	TIMES SOURCE:欲情追加 , 1.20
ELSE
	TIMES SOURCE:不潔 , 0.50
	TIMES SOURCE:液体追加 , 1.80
	TIMES SOURCE:欲情追加 , 1.50
ENDIF

;恥じらい・恥薄い
IF TALENT:恥じらい
	TIMES SOURCE:不潔 , 1.20
ELSEIF TALENT:恥薄い
	TIMES SOURCE:不潔 , 0.50
ENDIF
;Ｃ敏感、鈍感をみる
;快Ｃ自体のチェックは後でまとめてやる
IF TALENT:Ｃ敏感
	TIMES SOURCE:快Ｃ , 1.50
	TIMES SOURCE:屈従 , 1.50
	TIMES SOURCE:逸脱 , 1.50
ELSEIF TALENT:Ｃ鈍感
	TIMES SOURCE:快Ｃ , 0.60
	TIMES SOURCE:屈従 , 0.60
	TIMES SOURCE:逸脱 , 1.50
ENDIF

;処女、貞操観念
IF TALENT:処女
	TIMES SOURCE:不潔 , 1.20
	IF TALENT:処女 == 1
		TIMES SOURCE:不潔 , 2.00
		TIMES SOURCE:屈従 , 2.00
		SOURCE:露出 += 2000
		SIF TALENT:貞操観念
			TIMES SOURCE:露出 , 2.00
	ENDIF
ENDIF

;露出快楽経験入手判定
TFLAG:100 |= 2

;コマンド属性：触手
TFLAG:98 = 8
;触手経験フラグ
TFLAG:90 = 1
RETURN 1

