;=============================================================================
;触手アナル産卵
;=============================================================================
;--------------------------------------------------
;実行判定
;--------------------------------------------------
@COM_ABLE534
;個別設定コマンドフィルタ
SIF DITEMTYPE:999:534 > 0
	RETURN 0
;紳士的な触手系コマンドを有効にしてないとダメ
SIF (FLAG:15 & 4096) == 0
	RETURN 0
;アイテム進化の秘法を持っていないとダメ
SIF ITEM:進化の秘法 == 0
	RETURN 0
;触手調教中じゃないとはダメ
SIF TEQUIP:触手調教 == 0
	RETURN 0
;Ａ調教フィルタがオフになってないとダメ
SIF FLAG:2 & 2
	RETURN 0
;調教者は触手使役LV4以上必要
SIF ABL:PLAYER:触手使役 < 4
	RETURN 0
;機械だとダメ
SIF TALENT:機械
	RETURN 0
;アナル挿入以外のＡ系触手責め中は不可
SIF TEQUIP:Ａ系装着器具 && TEQUIP:Ａ系装着器具 != 1
	RETURN 0
;Ａ拡張経験が少ないと卵は追加不可能
IF EXP:Ａ拡張経験 < 1
	LOCAL = 0
ELSEIF EXP:Ａ拡張経験 < 4
	LOCAL = 1
ELSEIF EXP:Ａ拡張経験 < 20
	LOCAL = 2
ELSEIF EXP:Ａ拡張経験 < 50
	LOCAL = 3
ELSEIF EXP:Ａ拡張経験 < 200
	LOCAL = 4
ELSEIF EXP:Ａ拡張経験 < 500
	LOCAL = 5
ELSE
	LOCAL = 6
ENDIF
SIF TEQUIP:触手腸内産卵 >= LOCAL
	RETURN 0
;距離のチェック
SIF EQUIP:戦闘中 && COM_ABLE_BATTLE(TARGET, PLAYER, 534) == 0
	RETURN 0
RETURN 1

;--------------------------------------------------
;コマンド本体
;--------------------------------------------------
@COM534
PRINTL 触手アナル産卵

;戦闘判定
CALL COM_TRY_BATTLE, 534
;実行できない
SIF RESULT == 1
	RETURN 1

TSTR:0 = 触手アナル産卵
CALL DISPLAY_KOJO_TRAIN_MESSAGE_F_COM

LOCAL:1 = 65
LOCAL:2 = 70

;ABL:触手中毒をみる
IF ABL:触手中毒 == 0
	TIMES LOCAL:1 , 1.00
	TIMES LOCAL:2 , 1.40
ELSEIF ABL:触手中毒 == 1
	TIMES LOCAL:1 , 0.90
	TIMES LOCAL:2 , 1.20
ELSEIF ABL:触手中毒 == 2
	TIMES LOCAL:1 , 0.80
	TIMES LOCAL:2 , 1.00
ELSEIF ABL:触手中毒 == 3
	TIMES LOCAL:1 , 0.70
	TIMES LOCAL:2 , 0.80
ELSEIF ABL:触手中毒 == 4
	TIMES LOCAL:1 , 0.60
	TIMES LOCAL:2 , 0.60
ELSE
	TIMES LOCAL:1 , 0.50
	TIMES LOCAL:2 , 0.40
ENDIF

DOWNBASE:0 += LOCAL:1
DOWNBASE:1 += LOCAL:2

SOURCE:不潔 = 4000

;ABL:Ａ感覚をみる
IF ABL:Ａ感覚 == 0
	SOURCE:快Ａ = 80
	SOURCE:屈従 = 300
ELSEIF ABL:Ａ感覚 == 1
	SOURCE:快Ａ = 250
	SOURCE:屈従 = 800
ELSEIF ABL:Ａ感覚 == 2
	SOURCE:快Ａ = 600
	SOURCE:屈従 = 1400
ELSEIF ABL:Ａ感覚 == 3
	SOURCE:快Ａ = 1000
	SOURCE:屈従 = 1800
ELSEIF ABL:Ａ感覚 == 4
	SOURCE:快Ａ = 1300
	SOURCE:屈従 = 2100
ELSE
	SOURCE:快Ａ = 1700
	SOURCE:屈従 = 2400
ENDIF

;ABL:マゾっ気をみる
IF ABL:マゾっ気 == 0
	SOURCE:痛み = 2000
	SOURCE:屈従 += 200
	SOURCE:逸脱 = 1000
	SOURCE:反感追加 = 2000
ELSEIF ABL:マゾっ気 == 1
	SOURCE:痛み = 1600
	SOURCE:屈従 += 500
	SOURCE:逸脱 = 1000
	SOURCE:反感追加 = 1000
ELSEIF ABL:マゾっ気 == 2
	SOURCE:痛み = 1200
	SOURCE:屈従 += 800
	SOURCE:逸脱 = 1000
	SOURCE:反感追加 = 500
ELSEIF ABL:マゾっ気 == 3
	SOURCE:痛み = 800
	SOURCE:屈従 += 1200
	SOURCE:逸脱 = 1000
	SOURCE:反感追加 = 100
ELSEIF ABL:マゾっ気 == 4
	SOURCE:痛み = 600
	SOURCE:屈従 += 1500
	SOURCE:逸脱 = 1000
	SOURCE:反感追加 = 0
ELSE
	SOURCE:痛み = 400
	SOURCE:屈従 += 2000
	SOURCE:逸脱 = 1000
	SOURCE:反感追加 = 0
ENDIF

;ABL:触手中毒をみる
IF ABL:触手中毒 == 0
	TIMES SOURCE:不潔 , 1.50
ELSEIF ABL:触手中毒 == 1
	TIMES SOURCE:不潔 , 1.00
ELSEIF ABL:触手中毒 == 2
	TIMES SOURCE:不潔 , 0.75
ELSEIF ABL:触手中毒 == 3
	TIMES SOURCE:不潔 , 0.50
ELSEIF ABL:触手中毒 == 4
	TIMES SOURCE:不潔 , 0.25
ELSE
	TIMES SOURCE:不潔 , 0.10
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES SOURCE:快Ａ , 0.40
	SOURCE:痛み += 800
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES SOURCE:快Ａ , 0.80
	SOURCE:痛み += 500
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES SOURCE:快Ａ , 1.00
	SOURCE:痛み += 300
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES SOURCE:快Ａ , 1.40
	SOURCE:痛み += 120
ELSE
	TIMES SOURCE:快Ａ , 1.80
	SOURCE:痛み += 100
ENDIF

;PALAM:欲情をみる
IF PALAM:欲情 < PALAMLV:1
	TIMES SOURCE:快Ａ , 0.80
ELSEIF PALAM:欲情 < PALAMLV:2
	TIMES SOURCE:快Ａ , 0.90
ELSEIF PALAM:欲情 < PALAMLV:3
	TIMES SOURCE:快Ａ , 1.00
ELSEIF PALAM:欲情 < PALAMLV:4
	TIMES SOURCE:快Ａ , 1.10
ELSE
	TIMES SOURCE:快Ａ , 1.20
ENDIF

;ABL:従順をみる
IF ABL:従順 == 0
	TIMES SOURCE:快Ａ , 0.80
ELSEIF ABL:従順 == 1
	TIMES SOURCE:快Ａ , 0.90
ELSEIF ABL:従順 == 2
	TIMES SOURCE:快Ａ , 1.00
ELSEIF ABL:従順 == 3
	TIMES SOURCE:快Ａ , 1.10
ELSEIF ABL:従順 == 4
	TIMES SOURCE:快Ａ , 1.20
ELSE
	TIMES SOURCE:快Ａ , 1.30
ENDIF

;小柄体形
SIF TALENT:小柄体型
	TIMES SOURCE:痛み , 2.00

;Ａ敏感、鈍感をみる
;快Ａ自体のチェックは後でまとめてやる
IF TALENT:Ａ鈍感
	TIMES SOURCE:痛み , 1.50
	TIMES SOURCE:屈従 , 1.50
	TIMES SOURCE:逸脱 , 1.50
ELSEIF TALENT:Ａ敏感
	TIMES SOURCE:痛み , 0.60
	TIMES SOURCE:屈従 , 0.60
	TIMES SOURCE:逸脱 , 0.60
ENDIF

SOURCE:支配 = SOURCE:屈従
;処女で貞操観念
SIF TALENT:処女 == 1 && TALENT:貞操観念
	SOURCE:屈従 /= 3

;卵プラグの着脱
SIF TEQUIP:Ａ系装着器具 == 0
	TEQUIP:Ａ系装着器具 = 1
TEQUIP:触手腸内産卵 += 1
STAIN:アナル |= 2
STAIN:アナル |= 4
STAIN:アナル |= 64

TCVAR:Ａ経験 += 15

;コマンド属性：触手
TFLAG:98 = 8
;触手経験フラグ
TFLAG:90 = 1
RETURN 1

;--------------------------------------------------
;触手腸内産卵中
;--------------------------------------------------
@EQUIP_COM534
PRINTL ＜触手腸内産卵中＞
CALL DISPLAY_EQUIP_MESSAGE_COM, 534

LOCAL:3 = 120
LOCAL:4 = 100

;ABL:触手中毒をみる
IF ABL:触手中毒 == 0
	TIMES LOCAL:3 , 1.00
	TIMES LOCAL:4 , 1.40
ELSEIF ABL:触手中毒 == 1
	TIMES LOCAL:3 , 0.90
	TIMES LOCAL:4 , 1.20
ELSEIF ABL:触手中毒 == 2
	TIMES LOCAL:3 , 0.80
	TIMES LOCAL:4 , 1.00
ELSEIF ABL:触手中毒 == 3
	TIMES LOCAL:3 , 0.70
	TIMES LOCAL:4 , 0.80
ELSEIF ABL:触手中毒 == 4
	TIMES LOCAL:3 , 0.60
	TIMES LOCAL:4 , 0.60
ELSE
	TIMES LOCAL:3 , 0.50
	TIMES LOCAL:4 , 0.40
ENDIF

DOWNBASE:0 += LOCAL:3
DOWNBASE:1 += LOCAL:4

;ABL:Ａ感覚をみる
IF ABL:Ａ感覚 == 0
	LOCAL = 80
	LOCAL:1 = 300
ELSEIF ABL:Ａ感覚 == 1
	LOCAL = 250
	LOCAL:1 = 800
ELSEIF ABL:Ａ感覚 == 2
	LOCAL = 600
	LOCAL:1 = 1400
ELSEIF ABL:Ａ感覚 == 3
	LOCAL = 1000
	LOCAL:1 = 1800
ELSEIF ABL:Ａ感覚 == 4
	LOCAL = 1300
	LOCAL:1 = 2100
ELSE
	LOCAL = 1700
	LOCAL:1 = 2400
ENDIF

;EXP:Ａ経験をみる
IF EXP:Ａ経験 < EXPLV:1
	TIMES LOCAL , 0.50
	LOCAL:2 = 2000
ELSEIF EXP:Ａ経験 < EXPLV:2
	TIMES LOCAL , 1.00
	LOCAL:2 = 300
ELSEIF EXP:Ａ経験 < EXPLV:3
	TIMES LOCAL , 1.10
	LOCAL:2 = 50
ELSEIF EXP:Ａ経験 < EXPLV:4
	TIMES LOCAL , 1.20
	LOCAL:2 = 10
ELSEIF EXP:Ａ経験 < EXPLV:5
	TIMES LOCAL , 1.40
	LOCAL:2 = 0
ELSE
	TIMES LOCAL , 1.60
	LOCAL:2 = 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES LOCAL , 0.40
	LOCAL:2 += 800
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES LOCAL , 0.80
	LOCAL:2 += 500
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES LOCAL , 1.00
	LOCAL:2 += 300
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES LOCAL , 1.40
	LOCAL:2 += 120
ELSE
	TIMES LOCAL , 1.80
	LOCAL:2 += 100
ENDIF

;PALAM:欲情をみる
IF PALAM:欲情 < PALAMLV:1
	TIMES LOCAL , 0.80
ELSEIF PALAM:欲情 < PALAMLV:2
	TIMES LOCAL , 0.90
ELSEIF PALAM:欲情 < PALAMLV:3
	TIMES LOCAL , 1.00
ELSEIF PALAM:欲情 < PALAMLV:4
	TIMES LOCAL , 1.10
ELSE
	TIMES LOCAL , 1.20
ENDIF

;ABL:従順をみる
IF ABL:従順 == 0
	TIMES LOCAL , 0.80
ELSEIF ABL:従順 == 1
	TIMES LOCAL , 0.90
ELSEIF ABL:従順 == 2
	TIMES LOCAL , 1.00
ELSEIF ABL:従順 == 3
	TIMES LOCAL , 1.10
ELSEIF ABL:従順 == 4
	TIMES LOCAL , 1.20
ELSE
	TIMES LOCAL , 1.30
ENDIF

;小柄体形
SIF TALENT:小柄体型
	TIMES LOCAL:2 , 2.00

;Ａ敏感、鈍感をみる
;快Ａ自体のチェックは後でまとめてやる
IF TALENT:Ａ鈍感
	TIMES SOURCE:痛み , 1.50
	TIMES SOURCE:屈従 , 1.50
	TIMES SOURCE:逸脱 , 1.50
ELSEIF TALENT:Ａ敏感
	TIMES SOURCE:痛み , 0.60
	TIMES SOURCE:屈従 , 0.60
	TIMES SOURCE:逸脱 , 0.60
ENDIF

;処女で貞操観念
SIF TALENT:処女 == 1 && TALENT:貞操観念
	LOCAL:1 /= 3

SOURCE:快Ａ += LOCAL
SOURCE:屈従 += LOCAL:1
SOURCE:痛み += LOCAL:2
SOURCE:逸脱 += LOCAL:1 *2
SOURCE:支配 += LOCAL:1

TCVAR:Ａ経験 += 3
;触手経験フラグ
SIF TEQUIP:触手調教
	TFLAG:90 += 1
RETURN 1

;--------------------------------------------------
;調教時メッセージ
;--------------------------------------------------
@SYSTXT_TRAIN_MESSAGE_F_COM_534
IF TEQUIP:触手腸内産卵
	PRINTFORML 触手は%CALLNAME:TARGET%の腸内に新たな卵を産みつけた…
ELSEIF TEQUIP:Ａ系装着器具
	PRINTFORML %CALLNAME:TARGET%の直腸を責めていた触手は軽く身震いすると、腸内の奥に向けて粘液と共に卵を吐きだした…
ELSE
	PRINTFORML 極太の触手が%CALLNAME:TARGET%の菊穴に潜り込み、腸内で卵を産みつけた…
ENDIF

;--------------------------------------------------
;装着時メッセージ
;--------------------------------------------------
@SYSTXT_EQUIP_MESSAGE_COM_534
SIF EQUIP:睡眠薬 || TFLAG:899 > 0
	RETURN 0
RESETCOLOR
PRINTFORML %CALLNAME:TARGET%は菊門から触手の卵を押し込まれ、栓をされた状態で責め続けられている
IF ABL:マゾっ気 == 0
	PRINTFORML %CALLNAME:TARGET%は腸内の異物感に顔を真っ青にしている…
ELSEIF ABL:マゾっ気 == 1
	PRINTFORM %CALLNAME:TARGET%は脂汗を浮かべながらも異物感を誤魔化す為か、しきりに
	IF TEQUIP:緊縛関連
		PRINTL 腰を左右に振っている…
	ELSE
		PRINTL 尻部を撫でている…
	ENDIF
ELSEIF ABL:マゾっ気 == 2
	PRINTFORML %CALLNAME:TARGET%は苦しそうな表情をしながらも、顔を紅潮させもじもじとお尻を振っている…
ELSEIF ABL:マゾっ気 == 3
	PRINTFORML %CALLNAME:TARGET%は排泄感に苛まれながらも、時折恍惚とした表情を浮かべている
ELSEIF ABL:マゾっ気 == 4
	PRINTFORML %CALLNAME:TARGET%はうっとりした表情を浮かべ、時々卵が腸内の壁に当たっている刺激を愉しんでいる…
ELSE
	PRINTFORML %CALLNAME:TARGET%は排泄感を心ゆくまで味わっているようだ
	PRINTFORML %CALLNAME:TARGET%は背徳感を湛えた笑みを浮かべ、何かを期待するような眼で%CALLNAME:MASTER%を見上げている…
ENDIF
SETCOLOR COLOR("P-RED")
