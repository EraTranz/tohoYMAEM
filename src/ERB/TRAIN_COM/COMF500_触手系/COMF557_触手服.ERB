@COM_ABLE557
;実装したらこの下のリターンは消す
;RETURN 0
;個別設定コマンドフィルタ
SIF DITEMTYPE:999:557 > 0
	RETURN 0
;対象が触手娘かつ触手使役術を触手娘から取得していないかつ触手娘の触手使役Lvを適用していない場合はダメ
SIF NO:TARGET == 5001 && (FLAG:570 & 48) == 0
	RETURN 0
;調教者が禁断の知識をもっていないとダメ
SIF TALENT:PLAYER:禁断の知識 == 0
	RETURN 0
;調教者が触手使役術を持っていないとダメ
SIF TALENT:PLAYER:触手使役術 == 0
	RETURN 0
;解除はいつでも可能
SIF TEQUIP:触手服
	RETURN 1
;触手経験が0の場合、異常系調教フィルタがオフになってないとダメ
SIF FLAG:2 & 4 && EXP:触手経験 == 0
	RETURN 0
;紳士的な触手系コマンドを有効にしてないとダメ
SIF (FLAG:15 & 4096) == 0
	RETURN 0
;ＶＡ経験は伴わなくてもいいかも？非処女時は触手を挿入し経験があるときアナルに触手を挿入
;Ｖ調教フィルタがオフになってないとダメ(対象が[オトコ]の場合は除く)
SIF FLAG:2 & 1 && TALENT:オトコ == 0 && TALENT:処女 == 0
	RETURN 0
;Ａ調教フィルタがオフになってないとダメ
SIF FLAG:2 & 2 && EXP:Ａ経験
	RETURN 0
;調教者は触手使役LV2必要＜結構強いコマンドになったので適宜変える
SIF ABL:PLAYER:触手使役 < 3
	RETURN 0
;距離のチェック
SIF EQUIP:戦闘中 && COM_ABLE_BATTLE(TARGET, PLAYER, 557) == 0
	RETURN 0
RETURN 1

;--------------------------------------------------
;コマンド本体
;--------------------------------------------------
@COM557
PRINTL 触手服

;戦闘判定
CALL COM_TRY_BATTLE, 557
;実行できない
SIF RESULT == 1
	RETURN 1

TSTR:0 = 触手服
CALL DISPLAY_KOJO_TRAIN_MESSAGE_F_COM

;Ｖ経験は伴わなくてもいいかも？とりあえず処女じゃないとき
;Ｖ経験を伴うコマンドのフラグ(処女膜再生対応)
SIF TALENT:処女 == 0
	TFLAG:1 = 1

LOCAL:1 = 60
;触手経験をみる
SELECTCASE EXP:触手経験
	CASE IS < EXPLV:1
		TIMES LOCAL:1 , 3.00
	CASE IS < EXPLV:2
		TIMES LOCAL:1 , 2.50
	CASE IS < EXPLV:3
		TIMES LOCAL:1 , 2.00
	CASE IS < EXPLV:4
		TIMES LOCAL:1 , 1.00
	CASE IS < EXPLV:5
		TIMES LOCAL:1 , 0.80
	CASEELSE
		TIMES LOCAL:1 , 0.60
ENDSELECT

;触手中毒をみる
SELECTCASE ABL:触手中毒
	CASE 0
		TIMES LOCAL:1 , 1.00
	CASE 1
		TIMES LOCAL:1 , 0.90
	CASE 2
		TIMES LOCAL:1 , 0.80
	CASE 3
		TIMES LOCAL:1 , 0.70
	CASE 4
		TIMES LOCAL:1 , 0.60
	CASEELSE
		TIMES LOCAL:1 , 0.50
ENDSELECT
;感情乏しい
SIF TALENT:感情乏しい
	TIMES LOCAL:1 , 0.60
;寄生
SIF TALENT:寄生
	TIMES LOCAL:1 , 0.70
;懐卵
SIF TALENT:懐卵
	TIMES LOCAL:1 , 0.80
;触手インプラント
SIF CFLAG:41
	TIMES LOCAL:1 , 0.75

DOWNBASE:0 += LOCAL:1
DOWNBASE:1 += LOCAL:1 * 2
SOURCE:トラウマ += LOCAL:1 * 20

;臆病
SIF TALENT:臆病
	TIMES LOCAL:1 , 2.00

SOURCE:逸脱 += LOCAL:1 * 10

;触手服の着脱
IF TEQUIP:触手服
	SIF TEQUIP:触手服 & 2
		TEQUIP:Ｃ系装着器具 = 0
	SIF TEQUIP:触手服 & 4
		TEQUIP:Ｂ系装着器具 = 0
	SIF TEQUIP:触手服 & 8
		TEQUIP:Ｖ系装着器具 = 0
	SIF TEQUIP:触手服 & 16
		TEQUIP:Ａ系装着器具 = 0
	SIF TEQUIP:触手服 & 32
		TEQUIP:Ｕ系装着器具 = 0
	TEQUIP:触手服 = 0
ELSE
	TEQUIP:触手服 = 1
	SIF EXIST_BAR(TARGET)
		STAIN:ペニス |= 64
	STAIN:胸 |= 64
ENDIF

;コマンド属性：触手
TFLAG:98 = 8

;触手経験フラグ
TFLAG:90 = 2
RETURN 1

;-------------------------------------------------
;触手服着衣中
;-------------------------------------------------
@EQUIP_COM557
#DIM TENTACLE_ABLE,					1	;関数TENTACLE_COM_ABLEXXXを呼び出してビット格納
#DIM TENTACLE_EXECUTE,				1	;TENTACLE_EXECUTE内部の値で実行するコマンド決め
#DIM TENTACLE_EXECUTABLE,			4	;0～3に下一桁から順に数値代入
#DIMS TENTACLE_ABLE_STR,			1	;文字列としてRESULTから帰ってきた値を保存
#DIMS TENTACLE_SELECT_STR,			7 = "503,505,515","501,520,521,535","502,522,523,524","504,525,526","527,528","511,512,550,552,553,554,556,560","541,542,543"
										;見に行きたいTENTACLE_COM_ABLEの番号を入れておく
#DIMS RAND_STR,						5 = "0/10/10/10/10/10","0/10/10/10/10/10/10/5/5","0/3/3/3/3/3","0/10/8/15/20/20/15/15/10","0/5/5/5"
										;文字列として確率を保存
										;最初が一括変更用（入れればすべてその値に）
										;その次から(C,B,V,A,U)の確率(一括/C/B/V/A/U)
										;2つ目は最後に回復、催眠、チキチキ用
										;4つ目は特殊行動用(一括/くすぐり/秘貝/催眠/針/放電/吸血/チキチキ/回復)
										;5つ目は洗浄用(一括/V/A/U)
#DIMS RAND_NAME_STR,				5 = "START_RAND","END_RAND","START_WRIGGLE_RAND","START_SPECIAL_WRIGGLE_RAND","START_WASH_RAND"
										;RANDにつけた名前を保存する＜これを使用する方式は無理そう
#DIM START_RAND,					5	;責め始める確率(RAND_STR:0に対応)
#DIM END_RAND,						8	;責め終わる確率(RAND_STR:1に対応)
#DIM START_WRIGGLE_RAND,			5	;何もつけていないときに蠢く確率(RAND_STR:2に対応)
#DIM START_SPECIAL_WRIGGLE_RAND,	8	;特殊な責めを始める確率(RAND_STR:3に対応)
#DIM START_WASH_RAND,				3	;洗浄する確率(RAND_STR:4に対応)
#DIM TCOUNT,						2	;FOR文のカウント用
#DIM SAVE_SLOT,						2	;0:催眠/チキチキ/回復が今回始まったことを示すBIT格納用
										;1:SELECTCOM	転写用
PRINTL ＜触手服着衣中＞

;RAND_STR:0 = 1
;RAND_STR:1 = 3
;RAND_STR:2 = 3
;RAND_STR:1 = 0/3/4/10/5/6

;RAND_NAME_STRの要素数分回す
FOR TCOUNT:0, 0, VARSIZE("RAND_NAME_STR")
	;RAND_STRの指定番目に入れた文字列を分割
	SPLIT  RAND_STR:(TCOUNT:0), "/", LOCALS
	;最初に0以外が入っていれば一括変更
	IF LOCALS:0 != "0"
		SELECTCASE TCOUNT:0
		CASE 0
			VARSET START_RAND, TOINT(LOCALS:0)
			;RANDに0以下を渡さないため
			SIF TOINT(LOCALS:0) < 0
				VARSET START_RAND, 1
		CASE 1
			VARSET END_RAND, TOINT(LOCALS:0)
			SIF TOINT(LOCALS:0) < 0
				VARSET END_RAND, 1
		CASE 2
			VARSET START_WRIGGLE_RAND, TOINT(LOCALS:0)
			SIF TOINT(LOCALS:0) < 0
				VARSET START_WRIGGLE_RAND, 1
		CASE 3
			VARSET START_SPECIAL_WRIGGLE_RAND, TOINT(LOCALS:0)
			SIF TOINT(LOCALS:0) < 0
				VARSET START_SPECIAL_WRIGGLE_RAND, 1
		CASE 4
			VARSET START_WASH_RAND, TOINT(LOCALS:0)
			SIF TOINT(LOCALS:0) < 0
				VARSET START_WASH_RAND, 1	
		ENDSELECT
	;最初が0だったのに要素数が足りない場合
	ELSEIF RESULT != 9 && GROUPMATCH(TCOUNT:0, 1, 3)
		SELECTCASE TCOUNT:0
		CASE 3
			SPLIT  "0/10/8/15/20/20/15/15/10", "/", LOCALS
			FOR TCOUNT:1, 0, RESULT - 1
				START_SPECIAL_WRIGGLE_RAND:(TCOUNT:1) = TOINT(LOCALS:(TCOUNT:1 + 1))
				SIF TOINT(LOCALS:(TCOUNT:1 + 1)) == 0
					START_SPECIAL_WRIGGLE_RAND:(TCOUNT:1) = 1
			NEXT
		CASE 1
			SPLIT  "0/10/10/10/10/10/10/5/5", "/", LOCALS
			FOR TCOUNT:1, 0, RESULT - 1
				END_RAND:(TCOUNT:1) = TOINT(LOCALS:(TCOUNT:1 + 1))
				SIF TOINT(LOCALS:(TCOUNT:1 + 1)) == 0
					END_RAND:(TCOUNT:1) = 1
			NEXT
		ENDSELECT
	ELSEIF RESULT != 6 && GROUPMATCH(TCOUNT:0, 0, 2, 4)
		SELECTCASE TCOUNT:0
		CASE 0
			VARSET START_RAND, 10
		CASE 2
			VARSET START_WRIGGLE_RAND, 3
		CASE 4
			VARSET START_WASH_RAND, 5
		ENDSELECT
	ELSE
		FOR TCOUNT:1, 0, RESULT - 1
			SELECTCASE TCOUNT:0
			CASE 0
				START_RAND:(TCOUNT:1) = TOINT(LOCALS:(TCOUNT:1 + 1))
				SIF TOINT(LOCALS:(TCOUNT:1 + 1)) == 0
					START_RAND:(TCOUNT:1) = 1
			CASE 1
				END_RAND:(TCOUNT:1) = TOINT(LOCALS:(TCOUNT:1 + 1))
				SIF TOINT(LOCALS:(TCOUNT:1 + 1)) == 0
					END_RAND:(TCOUNT:1) = 1
			CASE 2
				START_WRIGGLE_RAND:(TCOUNT:1) = TOINT(LOCALS:(TCOUNT:1 + 1))
				SIF TOINT(LOCALS:(TCOUNT:1 + 1)) == 0
					START_WRIGGLE_RAND:(TCOUNT:1) = 1
			CASE 3
				START_SPECIAL_WRIGGLE_RAND:(TCOUNT:1) = TOINT(LOCALS:(TCOUNT:1 + 1))
				SIF TOINT(LOCALS:(TCOUNT:1 + 1)) == 0
					START_SPECIAL_WRIGGLE_RAND:(TCOUNT:1) = 1
			CASE 4
				START_WASH_RAND:(TCOUNT:1) = TOINT(LOCALS:(TCOUNT:1 + 1))
				SIF TOINT(LOCALS:(TCOUNT:1 + 1)) == 0
					START_WASH_RAND:(TCOUNT:1) = 1
			ENDSELECT
		NEXT
	ENDIF	
NEXT

; ～～(RAND_NAME_STR:0):1 が START_RAND:1 と等しくなるような命令？関数？ってあるのだろうか

;PRINTW ST1

SPLIT (TENTACLE_SELECT_STR:6), ",", LOCALS
FOR COUNT, 0, RESULT
	CALLFORM TENTACLE_COM_ABLE{TOINT(LOCALS:COUNT)}(START_WASH_RAND:COUNT)
	IF RESULT
		CALLFORM SYSTXT_TRAIN_MESSAGE_F_COM_{TOINT(LOCALS:COUNT)}
		CALLFORM COM{TOINT(LOCALS:COUNT)}
	ENDIF
NEXT

SAVE_SLOT:0 = 0
SAVE_SLOT:1 = SELECTCOM
SELECTCOM:2 = SELECTCOM
VARSET TENTACLE_ABLE_STR
VARSET TENTACLE_EXECUTABLE

SPLIT (TENTACLE_SELECT_STR:5), ",", LOCALS
FOR COUNT, 0, RESULT
	CALLFORM TENTACLE_COM_ABLE{TOINT(LOCALS:COUNT)}
	TENTACLE_EXECUTABLE |= RESULT
NEXT
IF TENTACLE_EXECUTABLE
		;触手くすぐり
		IF TENTACLE_EXECUTABLE & 1p0
			SELECTCASE RAND:(START_SPECIAL_WRIGGLE_RAND:0)
			CASE 0
				;口上用にTFLAG:201を使用。(0=ヘソくすぐり, 1=ペニス/クリトリスくすぐり, 2=バストくすぐり, 3=アナルくすぐり)
				TFLAG:201 = 0
				;SAVE_SLOT:1 = 0
				;IF FLAG:10
				;	SAVE_SLOT:1 |= FLAG:10
				;	FLAG:10 = 0
				;ENDIF
				SELECTCOM = 511	
				CALL COM511
				PREVCOM = SAVE_SLOT:1
				;IF SAVE_SLOT:1
				;	FLAG:10 |= SAVE_SLOT:1
				;ENDIF
			ENDSELECT
		ENDIF
		;触手秘貝開き
		IF TENTACLE_EXECUTABLE & 1p1
			SELECTCASE RAND:(START_SPECIAL_WRIGGLE_RAND:1)
			CASE 0
				;SAVE_SLOT:1 = 0
				;IF FLAG:10
				;	SAVE_SLOT:1 |= FLAG:10
				;	FLAG:10 = 0
				;ENDIF
				SELECTCOM = 512
				CALL COM512
				PREVCOM = SAVE_SLOT:1
				;IF SAVE_SLOT:1
				;	FLAG:10 |= SAVE_SLOT:1
				;ENDIF
			ENDSELECT
		ENDIF
		;触手催眠
		;IF TENTACLE_EXECUTABLE & 1p2
		;	SELECTCASE RAND:(START_SPECIAL_WRIGGLE_RAND:2)
		;	CASE 0
		;		CALL COM553
		;		TEQUIP:触手服 |= 128
		;		SAVE_SLOT:0 |= 128
		;	ENDSELECT
		;ENDIF
		;触手針責め
		IF TENTACLE_EXECUTABLE & 1p3
			SELECTCASE RAND:(START_SPECIAL_WRIGGLE_RAND:3)
			CASE 0
				;SAVE_SLOT:1 = 0
				;IF FLAG:10
				;	SAVE_SLOT:1 |= FLAG:10
				;	FLAG:10 = 0
				;ENDIF
				SELECTCOM = 552
				CALL COM552
				PREVCOM = SAVE_SLOT:1
				;IF SAVE_SLOT:1
				;	FLAG:10 |= SAVE_SLOT:1
				;ENDIF
			ENDSELECT
		ENDIF
		;触手放電
		IF TENTACLE_EXECUTABLE & 1p4
			SELECTCASE RAND:(START_SPECIAL_WRIGGLE_RAND:4)
			CASE 0
				SELECTCOM = 553
				CALL COM553
				PREVCOM = SAVE_SLOT:1
			ENDSELECT
		ENDIF
		;触手吸血
		IF TENTACLE_EXECUTABLE & 1p5
			SELECTCASE RAND:(START_SPECIAL_WRIGGLE_RAND:5)
			CASE 0
				SELECTCOM = 554
				CALL COM554
				PREVCOM = SAVE_SLOT:1
			ENDSELECT
		ENDIF
		;触手チキチキ
		IF TENTACLE_EXECUTABLE & 1p6
			SELECTCASE RAND:(START_SPECIAL_WRIGGLE_RAND:6)
			CASE 0
				CALL SYSTXT_TRAIN_MESSAGE_F_COM_556
				TEQUIP:触手チキチキ = 1
				CALL EQUIP_COM556
				TEQUIP:触手服 |= 256
				SAVE_SLOT:0 |= 256
			ENDSELECT
		ENDIF
		;触手強制回復
		IF TENTACLE_EXECUTABLE & 1p7 && (BASE:体力 <= 750 || BASE:体力 <= MAXBASE:0 / 2 )
			SELECTCASE RAND:(START_SPECIAL_WRIGGLE_RAND:7)
			CASE 0
				CALL SYSTXT_TRAIN_MESSAGE_F_COM_560
				;口上用
				TFLAG:201 = 0
				IF (TEQUIP:Ａ系装着器具 == 3 || TEQUIP:Ａ系装着器具 == 23) && TEQUIP:口装着器具 == 2
					;触手貫通＋触手口辱
					TFLAG:201 = 3
				ELSEIF (TEQUIP:Ａ系装着器具 == 3 || TEQUIP:Ａ系装着器具 == 23)
					;触手貫通のみ
					TFLAG:201 = 1
				ELSEIF TEQUIP:口装着器具 == 2
					;触手口辱のみ
					TFLAG:201 = 2
				ENDIF
				CALL DISPLAY_KOJO_TRAIN_MESSAGE_F_COM
				;触手強制回復
				IF (TEQUIP:Ａ系装着器具 == 3 || TEQUIP:Ａ系装着器具 == 23) || TEQUIP:口装着器具 == 1
					TEQUIP:触手強制回復 = 2
				ELSEIF TEQUIP:口装着器具 == 2
					TEQUIP:口装着器具 = 0
					TEQUIP:触手強制回復 = 1
				ELSE
					TEQUIP:触手強制回復 = 1
				ENDIF
				CALL EQUIP_COM560
				TEQUIP:触手服 |= 64
				SAVE_SLOT:0 |= 64
			ENDSELECT
		ENDIF
ENDIF

CALL TENTACLE_COM_ABLE550
;触手催眠解除
IF RESULT && TEQUIP:触手催眠 && TEQUIP:触手服 & 128 && !(SAVE_SLOT:0 & 128)
	SELECTCASE RAND:(END_RAND:6)
	CASE 0
		CALL SYSTXT_TRAIN_MESSAGE_F_COM_550
		TEQUIP:触手催眠 = 0
		TEQUIP:触手服 &= 0b101111111
	CASEELSE
	ENDSELECT
ENDIF
CALL TENTACLE_COM_ABLE556
;触手チキチキ解除
IF RESULT && TEQUIP:触手チキチキ && TEQUIP:触手服 & 256 && !(SAVE_SLOT:0 & 256)
	SELECTCASE RAND:(END_RAND:7)
	CASE 0
		CALL SYSTXT_TRAIN_MESSAGE_F_COM_556
		TEQUIP:触手チキチキ = 0
		TEQUIP:触手服 &= 0b011111111
	CASEELSE
	ENDSELECT
ENDIF
CALL TENTACLE_COM_ABLE560
;触手強制回復解除
IF RESULT && TEQUIP:触手強制回復 && TEQUIP:触手服 & 64 && !(SAVE_SLOT:0 & 64)
	SELECTCASE RAND:(END_RAND:5)
	CASE 0
		CALL SYSTXT_TRAIN_MESSAGE_F_COM_560
		TEQUIP:触手強制回復 = 0
		TEQUIP:触手服 &= 0b110111111
	CASEELSE
	ENDSELECT
ENDIF

VARSET TENTACLE_ABLE_STR
VARSET TENTACLE_EXECUTABLE

SPLIT (TENTACLE_SELECT_STR:0), ",", LOCALS
FOR COUNT, 0, RESULT
	CALLFORM TENTACLE_COM_ABLE{TOINT(LOCALS:COUNT)}
	IF RESULT
		SIF !(TENTACLE_ABLE_STR == "")
			TENTACLE_ABLE_STR += "/"
		TENTACLE_ABLE_STR += TOSTR(RESULT)
	ENDIF
NEXT

;PRINTW C1

;Ｃ責め分 C系統に装備がある場合は飛ばす
IF TEQUIP:Ｃ系装着器具 && TEQUIP:触手服 & 2
	;{END_RAND}分の一で責め解除
	SELECTCASE RAND:(END_RAND:0)
	CASE 0
		IF TEQUIP:Ｃ系装着器具 == 26
			CALL SYSTXT_TRAIN_MESSAGE_F_COM_515
		ELSEIF TEQUIP:Ｃ系装着器具 == 22
			CALL SYSTXT_TRAIN_MESSAGE_F_COM_505
		ELSEIF TEQUIP:Ｃ系装着器具 == 21
			CALL SYSTXT_TRAIN_MESSAGE_F_COM_503
		ENDIF
		TEQUIP:Ｃ系装着器具 = 0
		TEQUIP:触手服 &= 0b111111101
	CASEELSE
	ENDSELECT
ELSEIF TENTACLE_ABLE_STR != ""
	;{START_RAND}分の一で責め始める
	SELECTCASE RAND:(START_RAND:0)
	CASE 0
		SPLIT TENTACLE_ABLE_STR, "/", LOCALS
		FOR COUNT, 0, RESULT
			TENTACLE_EXECUTABLE:COUNT = TOINT(LOCALS:COUNT)
		NEXT
		;PRINTW C2
		SELECTCASE TENTACLE_EXECUTABLE:(RAND:RESULT)
		CASE 1p2
			PRINTFORML %CALLNAME:TARGET%に纏わりつく触手服は放電しつつ肉棒を扱き始めたようだ
			TEQUIP:Ｃ系装着器具 = 26
			TEQUIP:触手服 |= 2
			CALL EQUIP_COM17
		CASE 1p1
			PRINTFORML %CALLNAME:TARGET%に纏わりつく触手服は肉棒を扱き始めたようだ
			TEQUIP:Ｃ系装着器具 = 22
			TEQUIP:触手服 |= 2
			CALL EQUIP_COM16
		CASE 1p0
			PRINTFORML %CALLNAME:TARGET%に纏わりつく触手服は陰核を責め始めたようだ
			TEQUIP:Ｃ系装着器具 = 21
			TEQUIP:触手服 |= 2
			CALL EQUIP_COM14
		ENDSELECT
	ENDSELECT
ENDIF

VARSET TENTACLE_ABLE_STR
VARSET TENTACLE_EXECUTABLE

SPLIT (TENTACLE_SELECT_STR:1), ",", LOCALS
FOR COUNT, 0, RESULT
	CALLFORM TENTACLE_COM_ABLE{TOINT(LOCALS:COUNT)}
	IF RESULT
		SIF !(TENTACLE_ABLE_STR == "")
			TENTACLE_ABLE_STR += "/"
		TENTACLE_ABLE_STR += TOSTR(RESULT)
	ENDIF
NEXT

;PRINTW B1

;Ｂ責め分 B系統に装備がある場合は飛ばす
IF TEQUIP:Ｂ系装着器具 && TEQUIP:触手服 & 4
	SELECTCASE RAND:(END_RAND:1)
	CASE 0
		IF TEQUIP:Ｂ系装着器具 == 23
			CALL SYSTXT_TRAIN_MESSAGE_F_COM_526
		ELSEIF TEQUIP:Ｂ系装着器具 == 22
			CALL SYSTXT_TRAIN_MESSAGE_F_COM_525
		ELSEIF TEQUIP:Ｂ系装着器具 == 21
			CALL SYSTXT_TRAIN_MESSAGE_F_COM_504
		ENDIF
		TEQUIP:Ｂ系装着器具 = 0
		TEQUIP:触手服 &= 0b111111011
	CASEELSE
	ENDSELECT
ELSEIF TENTACLE_ABLE_STR != ""
	SELECTCASE RAND:(START_RAND:1)
	CASE 0
