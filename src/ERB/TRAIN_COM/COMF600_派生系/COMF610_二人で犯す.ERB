	IF EXIST_PENIS(MASTER)
		STAIN:アナル |= STAIN:MASTER:ペニス
		STAIN:MASTER:ペニス |= STAIN:アナル
	ENDIF
ELSEIF TFLAG:130 == 3
	;奴隷の口⇔主人のＰの汚れが移動
	IF EXIST_PENIS(MASTER)
		STAIN:口 |= STAIN:MASTER:ペニス
		STAIN:MASTER:ペニス |= STAIN:口
	ENDIF
ENDIF

IF TALENT:オトコ == 0 && TALENT:PLAYER:オトコ == 0
	TCVAR:レズ経験 += 10
ELSEIF TALENT:オトコ && TALENT:PLAYER:オトコ
	TCVAR:ＢＬ経験 += 10
ENDIF
;奉仕快楽経験入手判定
TFLAG:100 |= 1

;処女喪失の後でペニスを抜いているか
TFLAG:101 = (TFLAG:101 == 2) ? 1 # 0
RETURN 1

