TFLAG:91 = SAMEN_CHECK(PLAYER)

;射精時の処理
IF TFLAG:91 >= 1
	;コマンドごとに固有の処理はこの下に書く
	TIMES SOURCE:性行動 , 3.00
	TIMES SOURCE:達成感 , 5.00
	TIMES SOURCE:屈従 , 1.50

	;ABL:精液中毒をみる
	IF ABL:精液中毒 == 0
		SOURCE:中毒充足 += 0
	ELSEIF ABL:精液中毒 == 1
		SOURCE:中毒充足 += 300
	ELSEIF ABL:精液中毒 == 2
		SOURCE:中毒充足 += 1000
	ELSEIF ABL:精液中毒 == 3
		SOURCE:中毒充足 += 1500
	ELSEIF ABL:精液中毒 == 4
		SOURCE:中毒充足 += 2200
	ELSE
		SOURCE:中毒充足 += 3000
	ENDIF

	;射精した人物を設定
	TFLAG:92 = PLAYER
	;射精された人物を設定
	TFLAG:94 = TARGET
	;このコマンドのデフォルト射精箇所タイプ、射精箇所選択の可・不可を設定
	CALL COM_SHOOT_613
	;汎用的な処理はこの関数で行う
	;（射精ゲージの再計算や射精経験の上昇と共通部分の表示）
	CALL SAMEN_SHOOT
ENDIF

;調教者がふたなり
SIF TALENT:PLAYER:ふたなり
	SOURCE:屈従 /= 2

;射精に関係なく行われる処理はここから
STAIN:ヴァギナ |= STAIN:PLAYER:ペニス
STAIN:PLAYER:ペニス |= STAIN:ヴァギナ

;奴隷の指⇔奴隷のＢの汚れが移動
STAIN:手 |= STAIN:胸
STAIN:胸 |= STAIN:手

;奴隷が巨乳または爆乳の時のみ、奴隷の口⇔奴隷のＢの汚れが移動
IF TALENT:巨乳 || TALENT:爆乳
	STAIN:口 |= STAIN:胸
	STAIN:胸 |= STAIN:口
ENDIF
;主人経験フラグ
SIF ASSIPLAY == 0
	TFLAG:50 += (ABL:Ｖ感覚 >= 3) ? 3 # 2

;撮影時の調教内容チェック
SIF TEQUIP:Ｂ系装着器具 == 2
	TFLAG:122 = 90

IF TALENT:オトコ == 0 && TALENT:PLAYER:オトコ == 0
	TCVAR:レズ経験 += 10
ELSEIF TALENT:オトコ && TALENT:PLAYER:オトコ
	TCVAR:ＢＬ経験 += 10
ENDIF

;奉仕快楽経験入手判定
TFLAG:100 |= 1
;露出快楽経験入手判定
TFLAG:100 |= 2

SIF TFLAG:101
	TFLAG:101 = 2
;処女喪失の後でペニスを抜いているか
TFLAG:101 = (TFLAG:101 == 2) ? 1 # 0
RETURN 1

