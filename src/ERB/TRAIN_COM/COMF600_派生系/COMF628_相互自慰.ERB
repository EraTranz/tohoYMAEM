		LOCAL:4 += 700
	ELSEIF ABL:Ａ感覚 == 3
		LOCAL:2 = 500
		LOCAL:4 += 900
	ELSEIF ABL:Ａ感覚 == 4
		LOCAL:2 = 650
		LOCAL:4 += 1000
	ELSE
