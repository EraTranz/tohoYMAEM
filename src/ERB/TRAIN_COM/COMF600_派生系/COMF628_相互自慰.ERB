;=============================================================================
;相互自慰
;=============================================================================
;バイブ、アナルバイブ、ビデオ撮影の時に内容変化
;ビデオ撮影中なら公開オナニー
;シャワー使用中ならシャワーオナニー
;読心中なら読心オナニー
;バイブ、アナルバイブ、或いは両方装備でバイブオナニー
;--------------------------------------------------
;実行判定
;--------------------------------------------------
@EXCOM_ABLE628
#FUNCTION
;羞恥プレイ系拡張コマンドを有効にしてないとダメ
SIF (FLAG:15 & 512) == 0
	RETURNF 0
;従順4未満・レズっ気（ＢＬっ気）4未満のASSIの場合、調教対象が同性だと実行できない（小悪魔か妄信か服従ならＯＫ）
IF ASSIPLAY
	SIF (ABL:ASSI:従順 < 4 || ((TALENT:オトコ == 0 && TALENT:ASSI:オトコ == 0 && ABL:ASSI:レズっ気 < 4) || (TALENT:オトコ && TALENT:ASSI:オトコ && ABL:ASSI:ＢＬっ気 < 4))) && TALENT:ASSI:小悪魔 == 0 && TALENT:ASSI:妄信 == 0 && TALENT:ASSI:服従 == 0
		RETURNF 0
ENDIF
;野外もしくは公開自慰を行ったことがない場合の野外およびビデオ撮影中は、異常系調教フィルタがオフになってないとダメ
SIF FLAG:2 & 4 && (CFLAG:21 & 1) == 0 && (TEQUIP:ビデオ撮影 || TEQUIP:野外プレイ)
	RETURNF 0
SIF ASSIPLAY && FLAG:2 & 4 && (CFLAG:ASSI:21 & 1) == 0 && (TEQUIP:ビデオ撮影 || TEQUIP:野外プレイ)
	RETURNF 0
;睡眠薬で寝ているor失神している場合
SIF EQUIP:睡眠薬 || TFLAG:899 > 0
	RETURNF 0
;バイブ、クスコ、アナルバイブ、要石以外のＶ系装着具、Ａ系装着具使用中は不可
SIF TEQUIP:Ｖ系装着器具 > 2 || TEQUIP:Ａ系装着器具 > 2 || TEQUIP:氷柱
	RETURNF 0
;縄使用中は不可
SIF TEQUIP:緊縛関連
	RETURNF 0
;乗馬中はダメ
SIF TEQUIP:三角木馬乗馬中
	RETURNF 0
;お風呂場プレイ中はプレイマットが無いとダメ
SIF TEQUIP:お風呂場プレイ && ITEM:プレイマット == 0
	RETURNF 0
;シャワー中はダメ
SIF TEQUIP:シャワープレイ
	RETURNF 0
;○○風呂入浴中はダメ
SIF TEQUIP:特殊風呂入浴中
	RETURNF 0
;助手が褌着用中の場合、助手の技巧4以上必須
SIF ASSI >= 0 && ASSIPLAY && TEQUIP:ASSI:コスチュームプレイ == 17 && ABL:ASSI:技巧 < 4
	RETURNF 0
;褌着用中の場合技巧4以上必須
SIF TEQUIP:コスチュームプレイ == 17 && ABL:技巧 < 4
	RETURNF 0
;時止め中は無理
SIF TEQUIP:時間止め
	RETURNF 0
;触手調教中はダメ
SIF TEQUIP:触手調教
	RETURNF 0
;距離のチェック
SIF EQUIP:戦闘中 && COM_ABLE_BATTLE(TARGET, PLAYER, 628) == 0
	RETURNF 0
RETURNF 1

;--------------------------------------------------
;コマンド本体
;--------------------------------------------------
@COM628
LOCALS = 
SIF TEQUIP:ビデオ撮影
	LOCALS = %LOCALS%公開
SIF TEQUIP:野外プレイ
	LOCALS = %LOCALS%野外
SIF TEQUIP:読心
	LOCALS = %LOCALS%読心
SIF TEQUIP:シャワープレイ
	LOCALS = %LOCALS%シャワー
IF TEQUIP:Ｖ系装着器具 == 2 && TEQUIP:Ａ系装着器具 == 2
	LOCALS = %LOCALS%二穴要石
ELSEIF TEQUIP:Ｖ系装着器具 == 1 && TEQUIP:Ａ系装着器具 == 1
	LOCALS = %LOCALS%二穴バイブ
ELSEIF TEQUIP:Ｖ系装着器具 == 1
	LOCALS = %LOCALS%バイブ
ELSEIF TEQUIP:Ｖ系装着器具 == 2
	LOCALS = %LOCALS%要石
ELSEIF TEQUIP:Ａ系装着器具 == 1
	LOCALS = %LOCALS%アナルバイブ
ELSEIF TEQUIP:Ａ系装着器具 == 2
	LOCALS = %LOCALS%要石アナル
ENDIF
IF TEQUIP:Ｖ系装着器具 || TEQUIP:Ａ系装着器具 || TEQUIP:ビデオ撮影 || TEQUIP:シャワープレイ || TEQUIP:読心
	LOCALS = %LOCALS%相互オナニー
ELSE
	LOCALS = %LOCALS%相互自慰
ENDIF
PRINTSL LOCALS

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
;戦闘判定
CALL COM_TRY_BATTLE, 628
;実行できない
SIF RESULT == 1
	RETURN 1

IF TFLAG:699 == 0
	CALL COM_ORDER,628
	;実行できない
	SIF RESULT == 0
		RETURN 0
ENDIF

;-------------------------------------------------
;実行決定
;-------------------------------------------------
TSTR:0 = %LOCALS%
SELECTCOM = 628
CALL DISPLAY_KOJO_TRAIN_MESSAGE_F_COM

;-------------------------------------------------
;射精ゲージチェック
;-------------------------------------------------
LOCAL = 0

;調教者の技巧をみる
IF ABL:PLAYER:技巧 == 0
	LOCAL = 800
ELSEIF ABL:PLAYER:技巧 == 1
	LOCAL = 1600
ELSEIF ABL:PLAYER:技巧 == 2
	LOCAL = 2400
ELSEIF ABL:PLAYER:技巧 == 3
	LOCAL = 3200
ELSEIF ABL:PLAYER:技巧 == 4
	LOCAL = 4000
ELSE
	LOCAL = 4800
ENDIF

;調教者のＣ感覚をみる
LOCAL = EJACULATIONCHECK_ABL_0_2(PLAYER, LOCAL)

;Ｃ感覚の上げられない主人かつオトコの場合2倍(ということにしておく&それでも10000には届かないんだぜウフフ)
SIF ASSIPLAY == 0 && TALENT:PLAYER:オトコ
