;=============================================================================
;サンドイッチ
;=============================================================================
;特定のコンビ専用ダブル素股派生コマンド
;上下に奴隷＆助手の真ん中を肉棒でこする
;--------------------------------------------------
;実行判定
;--------------------------------------------------
@EXCOM_ABLE629
#FUNCTION
;助手がいないとダメ
SIF ASSI < 0
	RETURNF 0
;キャラ専用コマンドを有効にしてないとダメ
SIF (FLAG:15 & 16384) == 0
	RETURNF 0
;主人にペニスがないとダメ
SIF EXIST_BAR(MASTER) == 0
	RETURNF 0
;主人が【魅惑】か【謎の魅力】じゃないとダメ
SIF TALENT:MASTER:魅惑 == 0 && TALENT:MASTER:謎の魅力 == 0
	RETURNF 0
;助手が奉仕精神3以上、技巧4以上じゃないとダメ
SIF ABL:ASSI:技巧 < 4 || ABL:ASSI:奉仕精神 < 3
	RETURNF 0
;全員体のサイズが同程度か、主人が禁断の知識持ちじゃないとダメ
IF TALENT:MASTER:禁断の知識 == 0
	IF TALENT:MASTER:小人体型
		SIF TALENT:小人体型 == 0 || TALENT:ASSI:小人体型 == 0
			RETURNF 0
	ELSE
		SIF TALENT:小人体型 || TALENT:ASSI:小人体型
			RETURNF 0
	ENDIF
ENDIF
;調教対象か助手の両方、あるいはどちらかが肥大陰核かオトコかふたなりだとダメ
SIF EXIST_BAR(TARGET) || EXIST_BAR(ASSI)
	RETURNF 0
;潤滑が高くないとダメ
SIF PALAM:潤滑 < 2000
	RETURNF 0
;睡眠中は不可
SIF EQUIP:睡眠薬
	RETURNF 0
;失神中は不可
SIF TFLAG:899 > 0
	RETURNF 0
;パニック中は不可
SIF TFLAG:108 > 0
	RETURNF 0
;Ｖ系装着具使用中は不可
SIF TEQUIP:Ｖ系装着器具 || TEQUIP:クスコ
	RETURNF 0
;縄使用中は不可
SIF TEQUIP:緊縛関連
	RETURNF 0
;乗馬中はダメ
SIF TEQUIP:三角木馬乗馬中
	RETURNF 0
;お風呂場プレイ中はプレイマットが無いとダメ
SIF TEQUIP:お風呂場プレイ && ITEM:プレイマット == 0
	RETURNF 0
;オムツプレイ中はダメ
SIF TEQUIP:オムツプレイ
	RETURNF 0
;助手が褌着用中の場合はダメ
SIF ASSI >= 0 && TEQUIP:ASSI:コスチュームプレイ == 17
	RETURNF 0
;褌着用中の場合はダメ
SIF TEQUIP:コスチュームプレイ == 17
	RETURNF 0
;時止め中は無理
SIF TEQUIP:時間止め
	RETURNF 0
;触手調教中はダメ
SIF TEQUIP:触手調教
	RETURNF 0
;人魚はダメ
SIF TALENT:人魚 == 1
	RETURNF 0
;距離のチェック
SIF EQUIP:戦闘中 && COM_ABLE_BATTLE(TARGET, PLAYER, 629) == 0
	RETURNF 0
RETURNF 1

;--------------------------------------------------
;コマンド本体
;--------------------------------------------------
@COM629
IF TFLAG:175 == 1
	LOCALS = サンドイッチ
ELSEIF TFLAG:175 == 2
	LOCALS = 巫女サンド
ELSEIF TFLAG:175 == 3
	LOCALS = 秋の味覚サンド
ELSEIF TFLAG:175 == 4
	LOCALS = ムーンサルトサンド
ELSEIF TFLAG:175 == 5
	LOCALS = 秘封サンド
ELSEIF TFLAG:175 == 6
	LOCALS = 春のダブルリリーサンド
ELSEIF TFLAG:175 == 7
	LOCALS = 古明地ロリロリサンド
ELSEIF TFLAG:175 == 8
	LOCALS = 夢幻世界姉妹サンド
ELSEIF TFLAG:175 == 9
	LOCALS = 百合結界お姉さまサンド
ELSEIF TFLAG:175 == 10
	LOCALS = 躁鬱ＭＩＸサンド
ELSEIF TFLAG:175 == 11
	LOCALS = 幻想ムーディーサンド
ELSEIF TFLAG:175 == 12
	LOCALS = 幻想ハッピーサンド
ENDIF
PRINTFORML %LOCALS%

;-------------------------------------------------
;実行できるかの判定(ダブル素股の処理と同じ)
;-------------------------------------------------
;戦闘判定
CALL COM_TRY_BATTLE, 629
;実行できない
SIF RESULT == 1
	RETURN 1

IF TFLAG:699 == 0
	CALL COM_ORDER, 306
	;実行できない
	SIF RESULT == 0
		RETURN 0
ENDIF

;-------------------------------------------------
;実行決定
;-------------------------------------------------
SELECTCOM = 629
TSTR:0 = %LOCALS%
CALL DISPLAY_KOJO_TRAIN_MESSAGE_F_COM

;-------------------------------------------------
;射精ゲージチェック
;-------------------------------------------------
LOCAL = 0

;ABL:技巧をみる
IF ABL:技巧 == 0
	LOCAL = 500
ELSEIF ABL:技巧 == 1
	LOCAL = 1100
ELSEIF ABL:技巧 == 2
	LOCAL = 2000
ELSEIF ABL:技巧 == 3
	LOCAL = 3000
ELSEIF ABL:技巧 == 4
	LOCAL = 3900
ELSE
	LOCAL = 4600
ENDIF

;ABL:ASSI:技巧をみる
IF ABL:ASSI:技巧 == 0
	LOCAL += 500
ELSEIF ABL:ASSI:技巧 == 1
	LOCAL += 1100
ELSEIF ABL:ASSI:技巧 == 2
	LOCAL += 2000
ELSEIF ABL:ASSI:技巧 == 3
	LOCAL += 3000
ELSEIF ABL:ASSI:技巧 == 4
	LOCAL += 3900
ELSE
	LOCAL += 4600
ENDIF

;潤滑をみる
LOCAL = EJACULATIONCHECK_PALAM_10_5(TARGET, LOCAL)
;従順をみる
LOCAL = EJACULATIONCHECK_ABL_10_1(TARGET, LOCAL)
;奉仕精神をみる
LOCAL = EJACULATIONCHECK_ABL_13_1(TARGET, LOCAL)

;調教者のＣ感覚をみる
LOCAL = EJACULATIONCHECK_ABL_0_1(PLAYER, LOCAL)

SIF EXIST_PENIS(PLAYER)
	BASE:PLAYER:射精 += LOCAL

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
DOWNBASE:0 += 45
DOWNBASE:1 += 190

SOURCE:接触 = 650
SOURCE:露出 = 350
SOURCE:屈従 = 1700
SOURCE:逸脱 = 700

;上のほうで計算した汚れデータ
SOURCE:不潔 = TFLAG:242*10 + 60

;ABL:奉仕精神をみる
IF ABL:奉仕精神 == 0
	SOURCE:性行動 = 220
	SOURCE:達成感 = 120
	TIMES SOURCE:不潔 , 4.00
ELSEIF ABL:奉仕精神 == 1
	SOURCE:性行動 = 280
	SOURCE:達成感 = 210
	TIMES SOURCE:不潔 , 2.50
ELSEIF ABL:奉仕精神 == 2
	SOURCE:性行動 = 330
	SOURCE:達成感 = 280
	TIMES SOURCE:不潔 , 1.50
ELSEIF ABL:奉仕精神 == 3
	SOURCE:性行動 = 380
	SOURCE:達成感 = 380
	TIMES SOURCE:不潔 , 1.00
ELSEIF ABL:奉仕精神 == 4
	SOURCE:性行動 = 440
	SOURCE:達成感 = 540
	TIMES SOURCE:不潔 , 0.50
ELSE
	SOURCE:性行動 = 500
	SOURCE:達成感 = 850
	TIMES SOURCE:不潔 , 0.10
ENDIF

;ABL:技巧をみる
IF ABL:技巧 == 0
	TIMES SOURCE:性行動 , 0.70
	TIMES SOURCE:達成感 , 0.70
ELSEIF ABL:技巧 == 1
	TIMES SOURCE:性行動 , 0.90
	TIMES SOURCE:達成感 , 0.90
ELSEIF ABL:技巧 == 2
	TIMES SOURCE:性行動 , 1.00
	TIMES SOURCE:達成感 , 1.00
ELSEIF ABL:技巧 == 3
	TIMES SOURCE:性行動 , 1.20
	TIMES SOURCE:達成感 , 1.20
ELSEIF ABL:技巧 == 4
	TIMES SOURCE:性行動 , 1.40
	TIMES SOURCE:達成感 , 1.40
ELSE
	TIMES SOURCE:性行動 , 1.60
	TIMES SOURCE:達成感 , 1.60
ENDIF

;ABL:Ｃ感覚をみる
IF ABL:Ｃ感覚 == 0
	SOURCE:快Ｃ = 20
	TIMES SOURCE:露出 , 0.80
ELSEIF ABL:Ｃ感覚 == 1
	SOURCE:快Ｃ = 80
	TIMES SOURCE:露出 , 0.90
ELSEIF ABL:Ｃ感覚 == 2
	SOURCE:快Ｃ = 350
	TIMES SOURCE:露出 , 1.00
ELSEIF ABL:Ｃ感覚 == 3
	SOURCE:快Ｃ = 750
	TIMES SOURCE:露出 , 1.10
ELSEIF ABL:Ｃ感覚 == 4
	SOURCE:快Ｃ = 1200
	TIMES SOURCE:露出 , 1.20
ELSE
	SOURCE:快Ｃ = 1750
	TIMES SOURCE:露出 , 1.30
ENDIF

;PALAM:潤滑
IF PALAM:潤滑 < PALAMLV:1
	TIMES SOURCE:快Ｃ , 0.30
	TIMES SOURCE:性行動 , 0.60
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES SOURCE:快Ｃ , 0.60
	TIMES SOURCE:性行動 , 0.80
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES SOURCE:快Ｃ , 1.00
	TIMES SOURCE:性行動 , 1.00
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES SOURCE:快Ｃ , 1.50
	TIMES SOURCE:性行動 , 1.20
ELSEIF PALAM:潤滑 < PALAMLV:5
	TIMES SOURCE:快Ｃ , 2.00
	TIMES SOURCE:性行動 , 1.40
ELSE
	TIMES SOURCE:快Ｃ , 2.50
	TIMES SOURCE:性行動 , 1.60
ENDIF

;助手のABL:Ｃ感覚をみる
IF ABL:ASSI:Ｃ感覚 == 0
	TIMES SOURCE:性行動 , 0.80
	TIMES SOURCE:達成感 , 0.50
ELSEIF ABL:ASSI:Ｃ感覚 == 1
	TIMES SOURCE:性行動 , 0.90
	TIMES SOURCE:達成感 , 0.70
ELSEIF ABL:ASSI:Ｃ感覚 == 2
	TIMES SOURCE:性行動 , 1.00
	TIMES SOURCE:達成感 , 1.00
ELSEIF ABL:ASSI:Ｃ感覚 == 3
	TIMES SOURCE:性行動 , 1.10
	TIMES SOURCE:達成感 , 1.20
ELSEIF ABL:ASSI:Ｃ感覚 == 4
	TIMES SOURCE:性行動 , 1.20
	TIMES SOURCE:達成感 , 1.40
ELSE
	TIMES SOURCE:性行動 , 1.30
	TIMES SOURCE:達成感 , 1.70
ENDIF

;-------------------------------------------------
;射精チェック
;-------------------------------------------------
TFLAG:91 = SAMEN_CHECK(MASTER)

;射精時の処理
IF TFLAG:91 >= 1
	;コマンドごとに固有の処理はこの下に書く
	TIMES SOURCE:性行動 , 2.00

	;ABL:精液中毒をみる
	IF ABL:精液中毒 == 0
		SOURCE:中毒充足 = 0
		TIMES SOURCE:達成感 , 1.50
		TIMES SOURCE:屈従, 1.40
	ELSEIF ABL:精液中毒 == 1
		SOURCE:中毒充足 = 200
		TIMES SOURCE:達成感 , 2.00
		TIMES SOURCE:屈従, 1.00
	ELSEIF ABL:精液中毒 == 2
		SOURCE:中毒充足 = 400
		TIMES SOURCE:達成感 , 2.50
		TIMES SOURCE:屈従, 0.80
	ELSEIF ABL:精液中毒 == 3
		SOURCE:中毒充足 = 700
		TIMES SOURCE:達成感 , 3.00
		TIMES SOURCE:屈従, 0.50
	ELSEIF ABL:精液中毒 == 4
		SOURCE:中毒充足 = 1000
		TIMES SOURCE:達成感 , 4.00
		TIMES SOURCE:屈従, 0.20
	ELSE
		SOURCE:中毒充足 = 1500
		TIMES SOURCE:達成感 , 5.00
		TIMES SOURCE:屈従, 0.00
	ENDIF

	;調教者を大量射精させた場合は達成感と中毒充足のソースが増加
	IF TFLAG:91 == 2
		TIMES SOURCE:達成感 , 1.20
		TIMES SOURCE:中毒充足 , 1.50
	ENDIF

	;射精した人物を設定
	TFLAG:92 = MASTER
	;射精された人物を設定
	TFLAG:94 = TARGET
	;このコマンドのデフォルト射精箇所タイプ、射精箇所選択の可・不可を設定
	CALL COM_SHOOT_629
	;汎用的な処理はこの関数で行う
	;（射精ゲージの再計算や射精経験の上昇と共通部分の表示）
	CALL SAMEN_SHOOT
ENDIF

;射精に関係なく行われる処理はここから
;奴隷のＶ⇔調教者のＰの汚れが移動
STAIN:ヴァギナ |= STAIN:PLAYER:ペニス
STAIN:PLAYER:ペニス |= STAIN:ヴァギナ

;助手のＶ⇔調教者のＰの汚れが移動
STAIN:ASSI:ヴァギナ |= STAIN:PLAYER:ペニス
STAIN:PLAYER:ペニス |= STAIN:ASSI:ヴァギナ

;奴隷のＶ⇔助手のＶの汚れが移動
STAIN:ヴァギナ |= STAIN:ASSI:ヴァギナ
STAIN:ASSI:ヴァギナ |= STAIN:ヴァギナ

IF TALENT:オトコ == 0 && TALENT:PLAYER:オトコ == 0
	TCVAR:レズ経験 += 7
ELSEIF TALENT:オトコ && TALENT:PLAYER:オトコ
	TCVAR:ＢＬ経験 += 7
ENDIF
IF TALENT:MASTER:オトコ
	TCVAR:Ｃ調律経験 += 1
	TCVAR:ASSI:Ｃ調律経験 += 1
ENDIF
;奉仕快楽経験入手判定
TFLAG:100 |= 1

;調教者がふたなり
SIF TALENT:PLAYER:ふたなり
	SOURCE:屈従 /= 2

TFLAG:175 = 0
RETURN 1

;--------------------------------------------------
;命令拒否のメッセージ
;--------------------------------------------------
@SYSTXT_DENIAL_MESSAGE_COM_629

;--------------------------------------------------
;調教時メッセージ
;--------------------------------------------------
