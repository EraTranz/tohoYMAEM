;実行判定
;--------------------------------------------------
@EXCOM_ABLE652
#FUNCTION
;キャラ専用コマンドを有効にしてないとダメ
SIF (FLAG:15 & 16384) == 0
	RETURNF 0
;Ａ調教フィルタがオフになってないとダメ
SIF FLAG:2 & 2
	RETURNF 0
;調教者がフランちゃんじゃないとダメだよウフフ
SIF NO:PLAYER != 1011
	RETURNF 0
;調教者にペニスがないとダメ
SIF EXIST_BAR(PLAYER) == 0
	RETURNF 0
;対象の性別が♀限定
SIF COM_ABLE_CHECK_FEMALE(TARGET) == 0
	RETURNF 0
;主人が[禁断の知識]持ちじゃないとき、体格差が2段階以内か調教者が[禁断の知識]持ちじゃないとダメ
SIF TALENT:MASTER:禁断の知識 == 0 && COMMON_BODYSIZE_CHECK(PLAYER, TARGET) == 0
	RETURNF 0
;対象の《潤滑》は大丈夫かチェック
SIF COM_ABLE_SLICK(TARGET, PLAYER) == 0
	RETURNF 0
;調教者が[猫舌]だと猫フェラパッチ有効でないとダメ
SIF TALENT:PLAYER:猫舌 && (FLAG:14 & 16) == 0
	RETURNF 0
;調教者が[猫舌]だと技巧5が必要
SIF TALENT:PLAYER:猫舌 && ABL:PLAYER:技巧 < 5
	RETURNF 0
;気力が500または体力が1500未満では使用不可
SIF BASE:体力 < 1500 || BASE:気力 < 500
	RETURNF 0
;一定のＡ経験が必要
SIF EXP:Ａ経験 < EXPLV:3
	RETURNF 0
;その調教中に浣腸プレイ＋排泄があった場合、調教者が汚れ無視もしくは汚臭鈍感を持たないなら実行できない
SIF TFLAG:140 & 256 && TALENT:PLAYER:汚臭鈍感 == 0 && TALENT:PLAYER:汚れ無視 == 0
